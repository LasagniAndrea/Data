  <!-- FI 20210108 [XXXXX] Add SpheresTrace and SpheresTraceTime -->
  <system.diagnostics>
    <sources>
      <source name="SpheresTrace" switchName="SpheresTraceSwitch" switchType="System.Diagnostics.SourceSwitch" sqlDurationLimit="30">
        <listeners>
          <add name="SpheresTraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="SpheresTraceTime" switchName="SpheresTraceTimeSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="SpheresTraceTimeListener" />
          <remove name="Default" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="SpheresTraceSwitch" value="Information" />
      <add name="SpheresTraceTimeSwitch" value="Off" />
    </switches>
    <sharedListeners>
      <add name="SpheresTraceListener" type="System.Diagnostics.DelimitedListTraceListener"  initializeData="~\Temporary\Trc_.txt" traceOutputOptions="DateTime" />
      <add name="SpheresTraceTimeListener" type="System.Diagnostics.DelimitedListTraceListener" initializeData="~\Temporary\TrcTime_.txt" traceOutputOptions="DateTime" />
    </sharedListeners>
    <trace autoflush="true" />
  </system.diagnostics>


  <!--Ajax FI-->
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <!-- 20210216 [25664] Security Hide the programming language used in Response Header -->
        <remove name="X-Powered-By" />
        <!-- EG 20210204 [25590] New -->
        <!--Note: X-Content-Type-Options only apply request-blocking due to nosniff for request destinations of "script" and "style". 
        However, it also enables Cross-Origin Read Blocking (CORB) protection for HTML, TXT, JSON and XML files (excluding SVG image/svg+xml).-->
        <remove name="X-Content-Type-Options" />
        <add name="X-Content-Type-Options" value="nosniff" />
        <!-- 20210216 [25664] Security the browser will only load the resource in an iframe if the request comes from the same site -->
        <remove name="X-Frame-Options" />
        <add name="X-Frame-Options" value="sameorigin" />
        <!-- 20210216 [25664] Security Referrer policy -->
        <add name="Referrer-Policy" value="strict-origin-when-cross-origin" />
      </customHeaders>
    </httpProtocol>
    <!-- FI 20193112 [XXXXX] for prevent "Failed to load resource: the server responded with a status of 404 (Not Found)" Error message on .woff or .woff2 file -->
    <staticContent>
      <remove fileExtension=".woff" />
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff" mimeType="application/x-font-woff" />
      <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
    </staticContent>
