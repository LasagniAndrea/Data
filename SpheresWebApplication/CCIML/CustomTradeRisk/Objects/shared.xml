<?xml version="1.0" encoding="utf-8" ?>
<Objects>

  <!--NettingDesignation_L-->
  <Object name="NettingDesignation_L">
    <Label helpschema="efsml" helpelement="NettingDesignation"/>
    <Table>
      <TextBox datatype="string" style="width:180px;"  />
      <Space />
      <Button type="Submit" referential="NETDESIGNATION" cssclass="fa-icon"   tooltip="search" />
    </Table>
  </Object>

  <!-- Complex_NettingInformation -->
  <Object name="NettingInformation">
    <NettingMethodEnum_L clientid = "nettingMethod" required="false"  postback="true"/>
    <NettingDesignation_L clientid = "nettingDesignation" required="false" postback="true"/>
  </Object>

  <!-- Complex_Money -->
  <Object name="Money_L" helpschema="fpml" helpelement="Money">
    <Label clientid="amount" helpschema="fpml" helpelement="Money" />
    <Money />
  </Object>
  <Object name="Money" helpschema="fpml" helpelement="Money">
    <Table>
      <Amount clientid="amount" required="true" />
      <Space />
      <Currency clientid="currency" required="true" />
    </Table>
  </Object>

  <!--Complex_PartyReference-->
  <Object name="PartyReference_L" xhelpschema="fpml" xhelpelement="PartyReference">
    <Label helpschema="fpml" xhelpelement="PartyReference" />
    <DropDown datatype="string" width="190px"  />
  </Object>

  <Object name="Trader">
    <DropDown_L_w180 clientid="identifier" required="false" keepinquickinput="false" />
  </Object>
  <Object name="Trader_L">
    <DropDown_L_w180 clientid="identifier" required="false"  />
  </Object>
  
  <Object name="SalesFactor" helpschema="efsml" helpelement="sales">
    <OptGroupDropDown_L_w180 clientid="identifier" required="false" />
    <Factor clientid="factor" required="false" />
    <br/>
  </Object>
  <Object name="PartyBroker_L" helpschema="efsml" helpelement="Party">
    <Label helpschema="fpml-doc" helpelement="Party"/>
    <PartyBroker/>
  </Object>
  <Object name="PartyBroker" helpschema="efsml" helpelement="Party">
    <TextBox datatype="string" style="width:170px;" autocomplete="true"/>
    <Button type="Submit" referential="ACTOR" condition="BROKER" cssclass="fa-icon"  tooltip="search"/>
    <Space />
    <Table>
      <Display helpelement="none" />
      <Button clientid ="img" source="" style="display:none" />
    </Table>
  </Object>

  <Object name="PartyBrokerDisplayAfterBr" helpschema="efsml" helpelement="Party">
    <Table verticalAlign ="Top">
      <table>
        <TextBox datatype="string" style="width:150px;" autocomplete="true"/>
        <Button type="Submit" referential="ACTOR" condition="BROKER" cssclass="fa-icon"  tooltip="search"/>
      </table>
      <br />
      <table controlalignleft="1">
        <Display colspan="2" helpelement="none" style="white-space:nowrap"/>
        <Button clientid ="img" source="" style="display:none" />
      </table>
    </Table>
  </Object>

  <!-- Complex_Payment -->
  <Object name="Payment_ValueDate" helpschema="efsml" helpelement="Payment">
    <Table colspan="-1" controlalignleft="true" >
      <!-- PL 20180605 [24002] -->
      <!--<PaymentPartyBlock/>-->
      <Table>
        <Date_L clientid="paymentDate_unadjustedDate" caption="ValueDate" required="false" helpschema="fpml" helpelement="AdjustableDate" />
        <BDA_BC clientid="paymentDate_dateAdjustments" required="false" postback="true" />
        <!--<br />-->
        <PaymentType_L clientid="paymentType" required="false" helpschema="fpml" helpelement="PaymentType" />
        <Money_L clientid="paymentAmount" required="true" postback="true" />
        <Space />
        <SettlementInformation_L clientid="settlementInformation" postback="true" />
        <Fill/>
      </Table>
      <Fill/>
    </Table>
  </Object>

  <Object name="PaymentPartyBlock">
    <Table>
      <PartyReference_L clientid="payer" postback="true" required="true" />
      <br/>
      <PartyReference_L clientid="receiver" postback="1" required="true" />
    </Table>
  </Object>

  <!-- Complex_SimplePayment -->
  <Object name="SimplePaymentMoneyFormWithOrigin" helpschema="efsml" helpelement="SimplePayment">
    <Table colspan="-1" controlalignleft="true" >
      <PaymentPartyBlock/>
      <table controlalignleft="true">
        <table controlalignleft="true">
          <Money_L clientid="paymentAmount" required="true" postback="true" />
        </table>
        <table controlalignleft="true">
          <Money_L clientid="paymentAmountOrigin" required="false" postback="false" />
        </table>
      </table>
    </Table>
  </Object>

  <!--Complex_SettlementInformation-->
  <Object name="SettlementInformation_L" helpschema="fpml" helpelement="SettlementInformation">
    <Label helpschema="fpml" helpelement="SettlementInformation" />
    <Table>
      <DropDown datatype="string" listretrieval="predef:OTCmlSettlementType" />
      <Space />
      <Button type="Submit" xcaption="S.I." helpschema="fpml" imageurl="fas fa-external-link-square-alt" helpelement="SettlementInformation" tooltip="settlement instructions" />
    </Table>
  </Object>

  <!-- Common Abstract objects -->
  <Object name="DropDown_L_w180">
    <Label />
    <DropDown datatype="string" width="180px" />
  </Object>


  <Object name="OptGroupDropDown_L_w180">
    <Label />
    <OptGroupDropDown datatype="string" width="180px" />
  </Object>

  <!--Complex_BusinessDayAdjustments -->
  <Object name="BDA_BC">
    <documentation>BusinessDayAdjustments</documentation>
    <BDC_BC_L clientid="bDC" helpschema="fpml" helpelement="BusinessCenter" />
  </Object>

  

</Objects>
