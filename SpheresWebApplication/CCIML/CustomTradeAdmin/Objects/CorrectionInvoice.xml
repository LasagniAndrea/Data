<?xml version="1.0" encoding="utf-8" ?>
<Objects>
	<!-- START : MASTER BLOCKS -->

	<!-- GridInvoiceDatesAndCorrectionAmountBlock -->
	<Object name="GridInvoiceDatesAndCorrectionAmountBlock">
		<Table verticalAlign="Top">
			<Table style="istableH:true;tblH-titleR:InvoiceDates;">
				<InvoiceDatesBlock/>
			</Table>
			<br/>
			<CorrectionMasterAmountBlock/>
		</Table>
	</Object>

  <!-- GridInvoiceDatesAndCorrectionAmountLightBlock -->
  <Object name="GridInvoiceDatesAndCorrectionAmountLightBlock">
    <Table verticalAlign="Top">
      <Table style="istableH:true;tblH-titleR:InvoiceDates;">
        <InvoiceDatesBlock/>
      </Table>
      <br/>
      <CorrectionMasterAmountLightBlock/>
    </Table>
  </Object>

  
  <!-- CorrectionAmountAndRebateConditionsBlock -->
	<Object name="CorrectionAmountAndRebateConditionsBlock">
		<Table verticalAlign="Top" xstyle="width:10px;">
			<CorrectionMasterAmountBlock/>
			<br/>
			<InvoiceRebateConditionsBlock clientid="rebateConditions" helpschema="efsml" helpelement="RebateConditions"/>
		</Table>
	</Object>

	<!-- CorrectionMasterAmountBlock -->
	<Object name="CorrectionMasterAmountBlock">
		<Table style="istableH:true;tblH-titleR:InvoiceAmounts;">
			<CorrectionAmountsBlock/>
		</Table>
		<br/>
	</Object>

  <!-- CorrectionMasterAmountBlock -->
  <Object name="CorrectionMasterAmountLightBlock">
    <Table style="istableH:true;tblH-titleR:InvoiceAmounts;">
      <CorrectionAmountsLightBlock/>
    </Table>
    <br/>
  </Object>

  <!-- CorrectionAmountsBlock -->
	<Object name="CorrectionAmountsBlock">
		<table>
      <ReadOnlyInExcessInvoiceAmount clientid="rebateAmount" required="false"/>
		</table>
		<br/>
		<table>
			<br/>
			<hr style="margin-top:5px;margin-bottom:5px;" />
			<ReadOnlyInvoiceAmount clientid="netTurnOverAmount" />
			<ReadOnlyInvoiceAmount clientid="taxAmount" required="false"/>
			<br/>
			<InvoiceAmount clientid="netTurnOverIssueAmount" />
			<InvoiceAmount clientid="taxIssueAmount" required="false"/>
			<br/>
			<InvoiceAmount clientid="netTurnOverAccountingAmount" required="false"/>
			<InvoiceAmount clientid="taxAccountingAmount"         required="false"/>
			<Fill/>
			<br/>
		</table>
		<br/>
    <!--<ExpandDetailCorrectionAmount clientid="correction"/>-->
    <DetailCorrectionAmount clientid="correction"/>
		<br/>
		<hr style="margin-top:5px;margin-bottom:5px;" />
		<table>
			<br/>
      <NetTurnOverIssueRateBlock clientid="netTurnOverIssueRate" required="false"/>
			<br/>
			<InvoiceRateSourceBlock clientid="invoiceRateSource" required="false"/>
			<br/>
      <NetTurnOverAccountingRateBlock clientid="netTurnOverAccountingRate" required="false" />
		</table>
	</Object>

  <Object name="CorrectionAmountsLightBlock">
    <table style="width:140px;">
      <ReadOnlyInExcessInvoiceAmount clientid="rebateAmount" required="false"/>
    </table>
    <br/>
    <table style="width:140px;">
      <br/>
      <hr style="margin-top:5px;margin-bottom:5px;" />
      <ReadOnlyInvoiceAmount clientid="netTurnOverAmount" />
      <ReadOnlyInvoiceAmount clientid="taxAmount" required="false"/>
      <TextBox clientid="feesDetail" datatype="string" style="display:none;width:100px" required="false" misc="IsReadOnly:true"/>
      <Button type="Submit" clientid="feesDetail" invoicing="INVOICEFEESDETAIL" cssclass="fa-icon"   imageurl="fas fa-external-link-square-alt" tooltip="ViewEvent" displaymode="Consult" />
      <Fill/>
      <br/>
      <InvoiceAmount clientid="netTurnOverIssueAmount" />
      <InvoiceAmount clientid="taxIssueAmount" required="false"/>
      <br/>
      <InvoiceAmount clientid="netTurnOverAccountingAmount" required="false"/>
      <InvoiceAmount clientid="taxAccountingAmount"         required="false"/>
      <Fill/>
      <br/>
    </table>
    <br/>
    <!--<ExpandDetailCorrectionAmount clientid="correction"/>-->
    <DetailCorrectionAmount clientid="correction"/>
    <br/>
    <hr style="margin-top:5px;margin-bottom:5px;" />
    <table>
      <br/>
      <NetTurnOverIssueRateBlock clientid="netTurnOverIssueRate" required="false"/>
      <br/>
      <InvoiceRateSourceBlock clientid="invoiceRateSource" required="false"/>
      <br/>
      <NetTurnOverAccountingRateBlock clientid="netTurnOverAccountingRate" required="false" />
    </table>
  </Object>

	<!-- END : MASTER BLOCKS -->

	<!-- ReadOnlyInExcessInvoiceAmount -->
	<Object name="ReadOnlyInExcessInvoiceAmount">
		<Table style="width:10px;">
			<Label clientid="amount" helpschema="fpml" helpelement="Money" />
			<TextBox clientid="isInExcess" datatype="string" misc="IsReadOnly:true"/>
		</Table>
		<Table style="width:10px;">
			<TextBox clientid="amount" datatype="decimal" regex="RegexAmountExtend" style="width:150px" misc="IsReadOnly:true"/>
			<Space />
			<DropDown clientid="currency" datatype="string" width="60px" listretrieval="predef:CURRENCY" misc="IsReadOnly:true"/>
		</Table>
    <Fill/>
	</Object>

	<!-- ReadOnlyDetailInvoiceAmount -->
	<Object name="ReadOnlyDetailInvoiceAmount">
		<Label clientid="amount" helpschema="fpml" helpelement="Money" />
		<Table style="width:10px;">
			<TextBox clientid="initialAmount" datatype="decimal" regex="RegexAmountExtend" style="width:100px" misc="IsReadOnly:true"/>
			<Space />
			<TextBox clientid="finalAmount" datatype="decimal" regex="RegexAmountExtend" style="width:100px" misc="IsReadOnly:true"/>
		</Table>
	</Object>

	<!-- TitleDetailInvoiceAmount -->
	<Object name="TitleDetailInvoiceAmount">
		<Label clientid="titleAmounts" cssclass="lblRightRowHeader" style="text-align:right;" helpschema="fpml" helpelement="Money" />
		<Table style="width:10px;">
			<Label clientid="initialTitleAmounts" style="color:seaGreen;width:100px"/>
			<Space />
			<Label clientid="finalTitleAmounts" cssclass="lblRowHeader" style="width:100px"/>
		</Table>
	</Object>


	<!-- DetailCorrectionAmount -->
  <Object name="DetailCorrectionAmount">
    <table id="tblDetailCorrectionAmount_H" style="istableH:true;tblH-titleR:invoice_titleAmounts;tblH-css:size5;tblH-reverse:1;tblH-startdisplay:collapse;">
      <table id="tblDetailCorrectionAmount" colspan="2" style="display:block;">
        <table colspan="-1" style="width:530px;">
          <Label clientid="invoiceReference" />
          <TextBox clientid="trade_identifier"  style="width:150px" datatype="string" misc="IsReadOnly:true" />
          <InvoiceTradeAdminInfoId clientid="trade_otcmlId"/>
          <space/>
        </table>
        <br/>
        <space/>
        <table style="width:530px;display:block;">
          <br/>
          <table overflow="140" style="class:tableH;">
            <DetailCorrectionAmountHeader clientid="header"/>
            <DetailCorrectionAmountBody   clientid="grossTurnOverAmount" required="false"/>
            <DetailCorrectionAmountBody   clientid="totalRebateAmount" required="false"/>
            <DetailCorrectionAmountBody   clientid="taxAmount" required="false"/>
            <DetailCorrectionAmountBody   clientid="netTurnOverAmount"/>
            <DetailCorrectionAmountBody   clientid="netTurnOverIssueAmount"/>
            <DetailCorrectionAmountBody   clientid="netTurnOverAccountingAmount" required="false"/>
          </table>
        </table>
        <space/>
        <br/>
      </table>
    </table>
  </Object>
  <!-- DetailCorrectionAmountHeader -->
	<Object name="DetailCorrectionAmountHeader">
		<rowheader cssclass="tableH">
			<cellheader clientid="amount" helpschema="efsml" helpelement="InvoiceAmounts"/>
			<cellheader clientid="initial" helpschema="efsml" helpelement="initialInvoiceAmount"/>
			<cellheader clientid="base" helpschema="efsml" helpelement="baseNetInvoiceAmount"/>
			<cellheader clientid="theoric" helpschema="efsml" helpelement="theoricInvoiceAmount"/>
		</rowheader>
	</Object>
	<!-- DetailCorrectionAmountBody -->
	<Object name="DetailCorrectionAmountBody">
		<br/>
		<Label clientid="amount" />
		<CorrectionAmount clientid="initial"/>
		<CorrectionAmount clientid="base"/>
		<CorrectionAmount clientid="theoric"/>
	</Object>
	<!-- DetailCorrectionAmount -->
	<Object name="CorrectionAmount">
		<Table style="class:rowBorderNone;">
			<TextBox clientid="amount" datatype="decimal" regex="RegexAmountExtend" style="width:100px;" misc="IsReadOnly:true"/>
			<Space />
			<DropDown clientid="currency" datatype="string" width="30px" listretrieval="predef:CURRENCY" misc="IsReadOnly:true"/>
		</Table>
	</Object>

	<!-- InvoiceTradeAdminInfoId -->
	<Object name="InvoiceTradeAdminInfoId">
		<Label/>
		<TextBox style="width:100px" datatype="string"  misc="IsReadOnly:true" />
		<Button type="Submit" menu="OTC_INP_ADM" cssclass="fa-icon" imageurl="fas fa-external-link-square-alt" tooltip="ViewTradeAdmin" />
	</Object>

</Objects>
