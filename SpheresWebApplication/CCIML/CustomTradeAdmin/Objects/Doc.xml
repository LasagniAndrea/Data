<?xml version="1.0" encoding="utf-8" ?>
<!-- EG 20141020 [20442] Introduction BeneficiaryBlock -->
<!-- EG 20230526 [WI640] Gestion Side sur Parties Bénéficiaire et Payeur -->
<Objects>
  <!-- TradeAdminHeader -->
  <Object name="TradeAdminHeader" helpschema="fpml" helpelement="TradeHeader">
    <table cellstyle="vertical-align:top;width:50%" controlalignleft="true">
      <Date_L clientid="tradeDate" required="true" />
    </table>
    <table cellstyle="vertical-align:top;width:50%">
      <InvoicingScope_L clientid="scope" required="true" keepinquickinput="false" />
    </table>
  </Object>
	<!-- ClientBlock -->
	<Object name="ClientBlock">
		<PartyActor clientid="actor" postback="true" required="true" />
		<br />
	</Object>
  <!-- ClientDossierBlock -->
  <Object name="ClientDossierBlock">
    <PartyActor clientid="actor" postback="true" required="true" />
    <br />
    <Folder_L clientid="folder" required="false"/>
    <br />
  </Object>

  <!--Folder_L-->
  <Object name="Folder_L">
    <Label helpschema="efsml" helpelement="LinkId" />
    <TextBox datatype="string" style="width:120px;" />
  </Object>

  <!-- BeneficiaryBlock -->
  <Object name="BeneficiaryBlock" >
    <PartyActor clientid="actor" postback="true" required="true" />
		<br />
		<Book_L clientid="book" postback="true" required="false" keepinquickinput="false"/>
		<br/>
	</Object>
	<!--Book_L-->
	<Object name="Book_L">
		<Label helpschema="efsml" helpelement="BookId" />
		<TextBox datatype="string" style="width:170px" autocomplete="true" />
		<Button type="Submit" referential="BOOK" cssclass="fa-icon"  tooltip="search" />
		<Space />
		<Display width="100%" />
	</Object>
	<!--InvoicingScope_L-->
  <Object name="InvoicingScope_L">
    <Label helpschema="efsml" helpelement="scope" />
    <TextBox datatype="string" style="width:400px" />
    <Button type="Submit" referential="INVOICINGRULES" cssclass="fa-icon"  tooltip="search" />
  </Object>

  <!--Side-->
	<Object name="Side">
		<Table>
			<HtmlSelect datatype="string" listretrieval="predef:buysell" />
			<Space />
		</Table>
	</Object>

	<!--TradeID_L-->
	<Object name="TradeID_L">
		<Label helpelement="none" />
		<TextBox datatype="string" style="width:170px" />
	</Object>

</Objects>
