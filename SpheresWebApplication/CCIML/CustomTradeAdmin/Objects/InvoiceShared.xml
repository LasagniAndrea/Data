<?xml version="1.0" encoding="utf-8" ?>
<!-- EG 20141020 [20442] Introduction BeneficiaryBlock -->
<!-- EG 20230526 [WI640] Gestion Side sur Parties Bénéficiaire et Payeur -->
<Objects>

	<!-- InvoiceHeaderBlock -->
  <Object name="InvoiceHeaderBlock">
    <table>
      <table cellstyle="vertical-align:top;width:50%">
        <AdminPartyActor clientid="tradeAdminHeader_party" occurs="1" firstindex="1" required="true" />
      </table>
      <table cellstyle="vertical-align:top;width:50%">
          <AdminPartyActor clientid="tradeAdminHeader_party" occurs="1" firstindex="2" required="true" />
      </table>
      <br/>
    </table>
    <table colspan="2" style="class:PnlRoundedColor1;">
      <TradeAdminHeader clientid="tradeAdminHeader" helpschema="fpml" helpelement="TradeHeader" />
    </table>
  </Object>

  <Object name="AdminPartyActor" >
    <table id="party" verticalAlign="Top" style="istableH:true;tblH-mainclass:blue;tblH-headerlinkid:ptyheader;">
      <table id="ptyheader">
        <SidePayRec clientid="side" postback="true" required="true" />
      </table>
      <table controlalignleft="1" style="margin-top:2px">
        <PartyActor clientid="actor" postback="true" required="true" />
        <br/>
        <Book_L clientid="book" postback="true" required="false" keepinquickinput="false"/>
      </table>
    </table>
  </Object>
  <!--SidePayRec-->
  <Object name="SidePayRec">
    <HtmlSelect datatype="string" listretrieval="predef:invpayrec" />
  </Object>
<!-- InvoiceDatesBlock -->
	<Object name="InvoiceDatesBlock">
    <table  verticalAlign="Top" xcolspan="-1" controlalignleft="true">
      <Date_L clientid="invoiceDate" required="true" helpschema="efsml" helpelement="invoiceDate" />
      <Date_L clientid="paymentDate_adjustedDate" required="true" helpschema="fpml" helpelement="IdentifiedDate" />
      <br/>
			<InvoicePaymentDateBlock clientid="paymentDate" required="true" />
		</table>
  </Object>
	<!-- InvoicePaymentDateBlock -->
  <Object name="InvoicePaymentDateBlock">
    <table verticalAlign="Top" colspan="-1">
      <table id="tblInvoicePaymentDateH" style="istableH:true;tblH-titleR:InvoicePaymentDateRelativeTo;tblH-css:size5;tblH-reverse:1;tblH-startdisplay:collapse;">
        <table id="tblInvoicePaymentDate" verticalAlign="Top" controlalignleft="true">
            <RelativeDateOffset_L clientid="relativeDate" required="true" postback="false" helpschema="fpml" helpelement="RelativeDateOffset" />
        </table>
      </table>
    </table>
  </Object>
  <!-- InvoiceDetailsBlock -->
	<Object name="InvoiceDetailsBlock" >
		<table verticalAlign="Top">
			<Table style="istableH:true;tblH-titleR:InvoiceDates;">
				<InvoiceDatesBlock />
      </Table>
			<br/>
			<Table style="istableH:true;tblH-titleR:InvoiceDetails;">
				<Banner caption="InvoiceDetails" level="2" helpschema="efsml" helpelement="InvoiceDetails"/>
				<Table overflow="430">
					<InvoiceTrade clientid="invoiceTrade" occurs="1" helpschema="efsml" helpelement="InvoiceTrade"/>
					<br/>
				</Table>
			</Table>
		</table>
	</Object>



</Objects>