<?xml version="1.0" encoding="utf-8" ?>
<Objects>
	<!-- START : BLOCKS INVOICE FEES -->

	<!-- InvoiceFees -->
  <Object name="InvoiceFees">
    <table rowspan="4" colspan="4">
      <table id="tblInvoiceFeeH" style="istableH:true;tblH-title:InvoiceFees;tblH-css:size5;tblH-reverse:1;tblH-startdisplay:collapse;">
        <table id="tblInvoiceFee" verticalAlign="Top">
          <!--<OpenBanner caption="InvoiceFees" level="4" helpschema="efsml" tablelinkid="tblInvoiceFee" startdisplay="collapse" helpelement="InvoiceFees"/>-->
          <space/>
          <table overflow="80" style="class:tableH;">
            <InvoiceFeeHeader clientid="feeHeader" />
            <InvoiceFeeBody   clientid="invoiceFee" occurs="1" helpschema="efsml" helpelement="InvoiceFee"/>
            <br/>
          </table>
        </table>
      </table>
    </table>
  </Object>
  <!-- InvoiceFeeHeader -->
	<Object name="InvoiceFeeHeader">
		<rowheader cssclass="tableH">
			<cellheader clientid="feeDate" />
			<cellheader clientid="feeType" />
			<cellheader clientid="xsfeeAmount" />
			<cellheader clientid="xsfeeBaseAmount" />
			<cellheader clientid="xsfeeInitialAmount" />
			<cellheader clientid="otcmlId" />
		</rowheader>
		<br/>
	</Object>
	<!-- InvoiceFeeBody -->
	<Object name="InvoiceFeeBody">
		<br/>
		<Date_RO clientid="feeDate" misc="IsReadOnly:true"/>
		<TextBox clientid="feeType" datatype="string" style="width:35px" misc="IsReadOnly:true"/>
		<!--<InvoiceGlobalFeeAmount />-->
		<InvoiceFeeAmount clientid="feeAmount"/>
		<InvoiceFeeAmount clientid="feeBaseAmount"/>
		<InvoiceFeeAmount clientid="feeInitialAmount"/>
		<InvoiceFeeInfoId clientid="otcmlId"/>
		<br/>
	</Object>

	<!-- InvoiceGlobalFeeAmount -->
	<Object name="InvoiceGlobalFeeAmount">
		<Table style="class:rowBorderNone;">
			<InvoiceFeeAmount clientid="feeAmount"/>
			<InvoiceFeeAmount clientid="feeBaseAmount"/>
			<InvoiceFeeAmount clientid="feeInitialAmount"/>
		</Table>
	</Object>

	<!-- InvoiceFeeAmount -->
	<Object name="InvoiceFeeAmount">
		<Table style="class:rowBorderNone;">
			<TextBox clientid="amount" datatype="decimal" regex="RegexAmountExtend" style="width:65px" misc="IsReadOnly:true"/>
			<DropDown clientid="currency" datatype="string" width="30px" listretrieval="predef:CURRENCY" misc="IsReadOnly:true"/>
		</Table>
	</Object>
	<!-- InvoiceFeeInfoId -->
	<Object name="InvoiceFeeInfoId">
		<table>
			<TextBox datatype="string"  style="width:80px" misc="IsReadOnly:true" />
			<Button type="Submit" menu="OTC_INP_OTHERS_EVT" cssclass="fa-icon"  imageurl="fas fa-external-link-square-alt" tooltip="ViewEvent" />
		</table>
	</Object>
	<!-- END : BLOCKS INVOICE FEES -->
</Objects>
