<?xml version="1.0" encoding="utf-8" ?>
<Objects>
	<!-- START : BLOCKS REBATE CONDITIONS -->

	<!--ApplicationPeriod-->
	<Object name="ApplicationPeriod_L" >
		<Label  helpschema="fpml" helpelement="CalculationPeriodFrequency" />
		<Interval_RO required="false"/>
	</Object>

	<!-- InvoiceRebateConditionsBlock -->
	<Object name="InvoiceRebateConditionsBlock">
		<table verticalAlign="Top" controlalignleft="true">
			<table colspan="-1" style="istableH:true;tblH-titleR:RebateConditions;">
				<br/>
				<BracketConditionsBlock clientid="bracket" required="false"/>
				<br/>
				<CapConditionsBlock clientid="cap" required="false"/>
				<br/>
			</table>
		</table>
	</Object>

	<!-- CapConditionsBlock -->
	<Object name="CapConditionsBlock">
    <table id="tblCapConditionsH" style="istableH:true;tblH-titleR:CapConditions;tblH-css:size5;tblH-reverse:1;tblH-startdisplay:collapse;">
      <table id="tblCapConditions" verticalAlign="Top">
        <table>
          <ApplicationPeriod_L clientid="applicationPeriod" required="false" postback="true" />
          <br/>
          <ReadOnlyInvoiceAmount clientid="maximumNetTurnOverAmount" required="false"/>
          <br/>
          <ReadOnlyInvoiceAmount clientid="sumOfNetTurnOverPreviousPeriodAmount" required="false"/>
          <br/>
          <ReadOnlyInvoiceAmount clientid="netTurnOverInExcessAmount" required="false"/>
        </table>
      </table>
    </table>
	</Object>
	<!-- BracketConditionsBlock -->
	<Object name="BracketConditionsBlock">
    <table id="tblBracketConditions_H" style="istableH:true;tblH-titleR:BracketConditions;tblH-css:size5;tblH-reverse:1;tblH-startdisplay:collapse;">
				<Table id="tblBracketConditions" colspan="-1" controlalignleft="true" verticalAlign="Top">				  
					<Table>
						<ApplicationPeriod_L clientid="applicationPeriod" required="false" postback="true" />
						<br/>
						<ManagementTypeRO_L clientid="managementType" required="false" postback="true" helpschema="efsml" helpelement="BracketApplicationEnum" />
						<br/>
						<ReadOnlyInvoiceAmount clientid="sumOfGrossTurnOverPreviousPeriodAmount" required="false"/>
						<br/>
						<ReadOnlyInvoiceAmount clientid="sumOfNetTurnOverPreviousPeriodAmount" required="false"/>
						<br/>
						<ReadOnlyInvoiceAmount clientid="totalRebateBracketAmount" required="false"/>
					</Table>
          <!--<BracketDetailsConditionsBlock />-->
        </Table>
	      <BracketDetailsConditionsBlock />
    </table>
  </Object>

	<!-- BracketDetailsConditionsBlock -->
  <Object name="BracketDetailsConditionsBlock">
    <table id="tblBracketDetails_H" verticalAlign="top" style="istableH:true;tblH-titleR:BracketDetails;tblH-css:size5;tblH-reverse:1;tblH-startdisplay:collapse;">
      <table id="tblBracketDetails">
        <table overflow="75" style="class:tableH;">
          <BracketCalculationHeader clientid="calculationheader" helpschema="efsml" helpelement="RebateBracketParameter"/>
          <BracketCalculationBody   clientid="calculation" occurs="1" required="false" helpschema="efsml" helpelement="RebateBracketCalculation"/>
        </table>
      </table>
    </table>
  </Object>

  <!-- BrackerCalculationHeader -->
	<Object name="BracketCalculationHeader">
		<rowheader cssclass="tableH">
			<cellheader clientid="lowValue" helpschema="efsml" helpelement="lowValue"/>
			<cellheader clientid="highValue" helpschema="efsml" helpelement="highValue"/>
			<cellheader clientid="rate" helpschema="efsml" helpelement="rebateBracketRate"/>
			<cellheader colspan="2" clientid="amount" helpschema="efsml" helpelement="rebateBracketAmount"/>
		</rowheader>
	</Object>
	<!-- BrackerCalculation -->
	<Object name="BracketCalculationBody">
		<br/>
		<TextBox  clientid="lowValue"  datatype="decimal" regex="RegexAmountExtend"       style="width:90px"  misc="IsReadOnly:true" />
		<TextBox  clientid="highValue" datatype="decimal" regex="RegexAmountExtend"       style="width:90px"  misc="IsReadOnly:true" />
		<TextBox  clientid="rate"      datatype="decimal" regex="RegexFixedRatePercent"   style="width:70px"  misc="IsReadOnly:true" />
		<TextBox  clientid="amount"    datatype="decimal" regex="RegexAmountExtend"       style="width:100px"  misc="IsReadOnly:true" />
		<DropDown clientid="currency"  datatype="string"  listretrieval="predef:CURRENCY" width="30px"  misc="IsReadOnly:true"/>
	</Object>

	<!-- END : BLOCKS REBATE CONDITIONS -->
</Objects>
