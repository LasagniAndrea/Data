<?xml version="1.0" encoding="utf-8" ?>
<Objects>

  <Object name="UnderlyingDeliveryBlock" context="UnderlyingDelivery">
    <UnderlyingDelivery clientid="underlyingDelivery" />
  </Object>

  <!-- UnderlyingDelivery -->
  <Object name="UnderlyingDelivery">
    <table style="istableH:true;tblH-titleR:UnderlyingDelivery;tblH-bkgH-color:#545454;tblH-css:size4;">
      <table style="margin-top:2px;" controlalignleft="true" verticalAlign ="Top">
        <UnderlyingDeliveryInformation/>
      </table>
      <br/>
      <table verticalAlign ="Top" style="istableH:true;tblH-titleR:DeliveryStep;tblH-css:size5;tblH-reverse:1;">
        <table style="margin-top:2px;" colspan="-1" controlalignleft="true" verticalAlign ="Top">
          <UnderlyingDeliveryStep/>
        </table>
      </table>
    </table>
  </Object>

  <!-- Caractéristiques générales -->
  <Object name="UnderlyingDeliveryInformation">
    <table>
      <IntegerQte_w100_L_RO clientid="quantity" required="false"/>
      <String150_RO_L clientid="settlMethod" required="false"/>
      <br/>
      <IntegerQte_w100_L_RO clientid="physicalFactor" required="false"/>
      <String150_RO_L clientid="unlAssetCategory" required="false"/>
    </table>
  </Object>
  
  <!-- UnderlyingDeliveryStep -->
  <Object name="UnderlyingDeliveryStep">
    <table>
      <table controlalignleft="true" colspan="-1">
        <table>
          <String250_RO_L clientid="currentDeliveryStep" required="false"/>
          <br/>
          <DeliveryStep_L clientid="deliveryStep" required="true" helpschema="efsml" helpelement="UnderlyingDeliveryStepEnum" />
        </table>
        <Date_L clientid="date" required="false" postback="false" />
      </table>
    </table>
  </Object>
  
</Objects>