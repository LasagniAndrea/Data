<?xml version="1.0" encoding="utf-8" ?>
<Objects>

  <!-- TradeIdentifier -->
  <Object name="TradeIdentifier">
    <Table colspan="2">
      <Text200_RO_L clientid="identifier" caption="Identifier" required="false"/>
      <Space />
      <Text250_RO clientid="displayName" required="false"/>
      <Space />
      <Text_RO clientid="description" required="false" />
    </Table>
  </Object>

  <!-- TradeActionComments -->
  <Object name="TradeActionComments">
    <table verticalAlign="top" rowspan="3">
      <Banner caption="Comments" level="3"  cssclass="BannerRed" attributes="class:BannerRed"/>
      <Comments clientid="comments" required="false"  />
    </table>
  </Object>


  <!--Text_RO-->
  <Object name="Text_RO">
    <TextBox datatype="string"  style="width:100%;" misc="IsReadOnly:true"/>
  </Object>
  <!--Text200_RO-->
  <Object name="Text200_RO">
    <TextBox datatype="string" style="width:200px;" misc="IsReadOnly:true"/>
  </Object>
  <!--Text200_RO_L-->
  <Object name="Text200_RO_L">
    <Label/>
    <Text200_RO/>
  </Object>
  <!--Text250_RO-->
  <Object name="Text250_RO">
    <TextBox datatype="string" style="width:250px;" misc="IsReadOnly:true"/>
  </Object>
  <!--Comment-->
  <Object name="Comments">
    <TextBox textMode="MultiLine" datatype="string" style="width:450px;" />
  </Object>
  <!--Check-->
  <Object name="Check">
    <Label />
    <HtmlCheckBox style="width:60px;" datatype="bool" />
  </Object>

  <!--Date_RO-->
  <Object name="Date_RO">
    <TextBox datatype="date" width="90px" misc="IsReadOnly:true"/>
  </Object>
  <!--Date_RO_L-->
  <Object name="Date_RO_L">
    <Label width="50px"/>
    <Date_RO />
  </Object>


</Objects>