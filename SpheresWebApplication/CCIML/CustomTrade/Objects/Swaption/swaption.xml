- Suppression de l'instruction : tblH-startdisplay:expand; (inutile valeur par défaut)
- Suppression de l'instruction : tblH-linkid:xxxx; (inutile gestion via ajax quand nécessaire)
-->

<Objects>
	<!--SwaptionAmericanBlock-->
	<Object name="SwaptionAmericanBlock" >
		<Table style="istableH:true;tblH-titleF:DisplayKey_Instrument;tblH-css:size4;helpschema:efsml;helpelement:Swaption;" colspan="-1">
			<Table style="margin-top:2px;istableH:true;tblH-titleR:Exercise;tblH-reverse:1;tblH-css:size5;helpschema:efsml;helpelement:americanExercise;" colspan="-1">
				<AmericanExercise clientid="americanExercise" required="true" helpschema="fpml" helpelement="americanExercise" />
			</Table>
			<SwaptionCommonBlock/>
		</Table>
	</Object>
	<!--SwaptionBermudaBlock-->
	<Object name="SwaptionBermudaBlock" >
		<Table style="istableH:true;tblH-titleF:DisplayKey_Instrument;tblH-css:size4;helpschema:efsml;helpelement:Swaption;" colspan="-1">
			<Table style="margin-top:2px;istableH:true;tblH-titleR:Exercise;tblH-reverse:1;tblH-css:size5;helpschema:efsml;helpelement:bermudaExercise;" colspan="-1">
				<BermudaExercise clientid="bermudaExercise" required="true" helpschema="fpml" helpelement="bermudaExercise" />
			</Table>
			<SwaptionCommonBlock/>
		</Table>
	</Object>
	<!--SwaptionEuropeanBlock-->
	<Object name="SwaptionEuropeanBlock" >
		<Table style="istableH:true;tblH-titleF:DisplayKey_Instrument;tblH-css:size4;helpschema:efsml;helpelement:Swaption;" colspan="-1">
			<Table style="margin-top:2px;istableH:true;tblH-titleR:Exercise;tblH-reverse:1;tblH-css:size5;helpschema:efsml;helpelement:europeanExercise;" colspan="-1">
				<EuropeanExercise clientid="europeanExercise" required="true" helpschema="fpml" helpelement="europeanExercise" />
			</Table>
			<SwaptionCommonBlock/>
		</Table>
	</Object>
	<!--SwaptionCommonBlock-->
	<Object name="SwaptionCommonBlock" >
		<Table colspan="-1" style="margin-top:5px;" controlalignleft="true">
			<Check_L clientid="swaptionStraddle" required="true"/>
			<space />
			<Button clientid = "exerciseProcedure" type="Submit" caption="Procedure" imageurl="fas fa-external-link-square-alt" helpschema="fpml" helpelement="ExerciseProcedure" />
			<space />
			<Button clientid = "calculationAgent"  type="Submit" caption="Calculation Agent" imageurl="fas fa-external-link-square-alt" helpschema="fpml" helpelement="CalculationAgent" />
			<space />
			<Button clientid = "cashSettlement"  type="Submit" caption="Cash Settlement" imageurl="fas fa-external-link-square-alt" helpschema="fpml" helpelement="CashSettlement" />
		</Table>
		<Table style="margin-top:2px;istableH:true;tblH-titleR:swaption_swap;tblH-reverse:1;tblH-css:size4;helpschema:efsml;helpelement:Swap;" xcolspan="-1">
			<Table id="tblVanillaSwap" colspan="-1" controlalignleft="true">
				<Table controlalignleft="true">
				<label clientid="swap"/>
				<DropDown clientid="underlyingSwap" datatype="string"
					  listretrieval="sql:
            select IDI as VALUE, IDENTIFIER as TEXT 
            from dbo.INSTRUMENT where IDP in(
                select IDP from dbo.PRODUCT 
                where IDENTIFIER = 'swap')" width="300px"/>
				</Table>
				<br/>
				<VanillaSwapBlockDDL clientid="swap"/>
			</Table>
		</Table>
		<br/>
		<Table style="istableH:true;tblH-titleR:premium;tblH-reverse:1;tblH-css:size5;" colspan="-1">
			<PaymentWithPaymentQuote clientid="premium" occurs="1" addsuboperator="true" required="false" helpschema="efsml" helpelement="Payment" />
		</Table>
	</Object>
	<Object name="VanillaSwapBlock">
		<Table id="tblVanillaSwap" style="class:PnlRoundedColor1;" colspan="-1">
			<Banner caption ="swaption_swap"  level="2"  helpschema="efsml" helpelement="Swap" />
      <Table style="margin-top:2px" colspan="-1"  controlalignleft="true" >
        <VanillaSwapStreamCommon2 clientid="swapStream" helpschema="efsml" helpelement="InterestRateStream" />
        <Check_L clientid="isSynchronous" required="true" postback="true"/>
      </Table>
			<VanillaSwapStream clientid="swapStream" occurs="2"  helpschema="efsml" helpelement="InterestRateStream" />
			<SwapAdditionalPaymentBlock/>
		</Table>
	</Object>
	<Object name="VanillaSwapBlockDDL">
		<Table id="tblVanillaSwap" colspan="-1" controlalignleft="1">
      <Table style="margin-top:2px" colspan="-1"  controlalignleft="true" >
        <VanillaSwapStreamCommon2 clientid="swapStream" helpschema="efsml" helpelement="InterestRateStream" />
        <Check_L clientid="isSynchronous" required="true" postback="true"/>
      </Table>
      <br/>
			<VanillaSwapStream clientid="swapStream" occurs="2"  helpschema="efsml" helpelement="InterestRateStream" />
			<SwapAdditionalPaymentBlock/>
		</Table>
	</Object>

	<Object name="SwapAdditionalPaymentBlock">
		<!--additionalPayment-->
    <table id="tblAdditionalPaymentH" colspan="-1"
       style="istableH:true;tblH-titleR:AdditionalPayment;tblH-css:size4;tblH-reverse:1;tblH-startdisplay:collapse;">
      <table id ="tblAdditionalPayment">
        <PaymentADP clientid="additionalPayment" occurs="1" addsuboperator="true" required="false" helpschema="efsml" helpelement="Payment" keepinquickinput="false"/>
      </table>
    </table>
	</Object>
</Objects>