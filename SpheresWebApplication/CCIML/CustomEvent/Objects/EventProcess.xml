<?xml version="1.0" encoding="utf-8" ?> 
<Objects>

	<!-- ************************* -->		
	<!-- ***   BLOCS PROCESS   *** -->	
	<!-- ************************* -->		

	<!-- EventsProcessBlock -->
	<Object name="EventsProcessBlock">
    <EventsProcess clientid="event" required="false"/>
	</Object>


	<!--EventsProcess-->
	<Object name="EventsProcess">
    <table>
      <EventParentAttached/>
      <br/>
      <ProcessBlock/>
    </table>
	</Object>

  <Object name="ProcessBlock">
    <table colspan="-1" verticalAlign="Top" style="istableH:true;tblH-titleR:Process;tblH-css:size4;" controlalignleft="true" >
      <Label clientid="eventProcess_process" width="300" />
      <Label clientid="eventProcess_idStProcess" width="90" />
      <Label clientid="eventProcess_dtStProcess" width="150" />
      <br/>
      <hr/>
      <table colspan="-1" verticalAlign="Top" overflow="100" controlalignleft="true">
        <EventProcessItem clientid="eventProcess" required="false" occurs="1" addsuboperator="true" />
      </table>
    </table>
  </Object>


    <!-- ***************************** -->
	<!-- ***   ITEM EVENTPROCESS   *** -->
	<!-- ***************************** -->
	
	<Object name="EventProcessItem">
		<table colspan="-1">
			<ProcessType   clientid="process" required="false" postback="true"/>
			<StatusProcess clientid="idStProcess" required="false"/>
      <!-- FI 20200820 [25468] Une zone text pour afficher la date (UTC) selon le profil -->
      <Text150_RO     clientid="dtStProcess" required="false"/>
			<Fill/>
		</table>
	</Object>

</Objects>

