    <!--CC 20100726 Add SessionRestrict for MARKET-->
    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT srm on srm.ID=tblmain.IDM and srm.CLASS='MARKET' and srm.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLWhere>
        (
        ((tblmain.IDM is null) or (srm.ID is not null))
        )
      </SQLWhere>
    </SQLWhere>

    <SQLRowStyle type="style">
      <![CDATA[case when tblmain.ISENABLED = 0 then 'background-color:#f7b7b8' else 'background-color:#ffffff' end]]>
    </SQLRowStyle>
    <SQLRowState>
      <![CDATA[case when tblmain.ISENABLED = 0 then 'lstaw=name:squareorange;title:imgNotCertified' else 'lstaw=name:ledavailable;title:imgAvailable' end]]>
    </SQLRowState>

