    <LoadOnStart>false</LoadOnStart>
    <Timeout>200</Timeout>

    <SQLSelect>
			<Command rdbms = "all">
        <![CDATA[
        select avi.IDA_INVOICED, avi.IDB_INVOICED, avi.IDA_BENEFICIARY, avi.IDB_BENEFICIARY,avi.INVOICEDATE,
        /* Net initial */
        SUM(avi.NTI_AMOUNT) as INIT_NTI_AMOUNT, avi.NTI_CUR as INIT_NTI_CUR,
        SUM(avi.NTA_AMOUNT) as INIT_NTA_AMOUNT, avi.NTA_CUR as INIT_NTA_CUR,
        /* Avoirs eventuels */
        SUM(isnull(avc_nti.TURNOVER_AMOUNT,0)) as CREDIT_NTI_AMOUNT,avi.NTI_CUR as CREDIT_NTI_CUR,
        SUM(isnull(avc_nta.TURNOVER_AMOUNT,0)) as CREDIT_NTA_AMOUNT,avi.NTA_CUR as CREDIT_NTA_CUR,
        /* Allouees */
        SUM(isnull(alloc_nti.TURNOVER_AMOUNT,0)) as ALLOC_NTI_AMOUNT,avi.NTI_CUR as ALLOC_NTI_CUR,
        SUM(isnull(alloc_nta.TURNOVER_AMOUNT,0)) as ALLOC_NTA_AMOUNT,avi.NTA_CUR as ALLOC_NTA_CUR,
        /* Restant du */
        SUM(avi.NTI_AMOUNT - isnull(avc_nti.TURNOVER_AMOUNT, 0) - isnull(alloc_nti.TURNOVER_AMOUNT, 0)) as REMAIN_NTI_AMOUNT,avi.NTI_CUR as REMAIN_NTI_CUR,
        SUM(avi.NTA_AMOUNT - isnull(avc_nta.TURNOVER_AMOUNT, 0) - isnull(alloc_nta.TURNOVER_AMOUNT, 0)) as REMAIN_NTA_AMOUNT,avi.NTA_CUR as REMAIN_NTA_CUR
        --avi.ROWATTRIBUT,avi.ROWVERSION
        from dbo.VW_AVAILABLEINVOICESTL avi
        %%SR:TRADEADMIN_JOIN%%(avi.IDT)
        left outer join dbo.VW_AVAILABLECREDITSTL avc_nti on (avc_nti.IDT=avi.IDT) and (avc_nti.EVENTTYPE='NTI')
        left outer join dbo.VW_AVAILABLECREDITSTL avc_nta on (avc_nta.IDT=avi.IDT) and (avc_nta.EVENTTYPE='NTA')
        left outer join
        (
          select tr.IDT,SUM(ev.VALORISATION) as TURNOVER_AMOUNT, ev.UNIT as TURNOVER_CUR
          from dbo.TRADE tr
          inner join dbo.INSTRUMENT ns on (ns.IDI=tr.IDI)
          inner join dbo.PRODUCT pr on (pr.IDP=ns.IDP) and (pr.IDENTIFIER in ('invoice','additionalInvoice'))
          inner join dbo.EVENT ev on (ev.IDT=tr.IDT) and (ev.EVENTCODE='LPP') and (ev.EVENTTYPE = 'NTI')  and (ev.IDSTACTIVATION = 'REGULAR')
          inner join dbo.EVENTCLASS ec on (ec.IDE=ev.IDE) and (ec.EVENTCLASS='STL') and (ec.DTEVENT <= @DATE1)
          group by tr.IDT,ev.UNIT
        ) alloc_nti on (alloc_nti.IDT=avi.IDT)
        left outer join
        (
          select tr.IDT,SUM(ev.VALORISATION) as TURNOVER_AMOUNT, ev.UNIT as TURNOVER_CUR
          from dbo.TRADE tr
          inner join dbo.INSTRUMENT ns on (ns.IDI=tr.IDI)
          inner join dbo.PRODUCT pr on (pr.IDP=ns.IDP) and (pr.IDENTIFIER in ('invoice','additionalInvoice'))
          inner join dbo.EVENT ev on (ev.IDT=tr.IDT) and (ev.EVENTCODE='LPP') and (ev.EVENTTYPE = 'NTA')  and (ev.IDSTACTIVATION = 'REGULAR')
          inner join dbo.EVENTCLASS ec on (ec.IDE=ev.IDE) and (ec.EVENTCLASS='STL') and (ec.DTEVENT <= @DATE1)
				  group by tr.IDT,ev.UNIT
				) alloc_nta on (alloc_nta.IDT=avi.IDT)
				where
        (avi.IDA_BENEFICIARY = case when -1=@BENEFICIARY Then  avi.IDA_BENEFICIARY else @BENEFICIARY end ) and (avi.IDA_ENTITY = @ENTITY) 

        and (%%SR:TRADEADMIN_WHERE_PREDICATE%%)
				group by avi.IDA_INVOICED, avi.IDB_INVOICED, avi.IDA_BENEFICIARY, avi.IDB_BENEFICIARY,avi.INVOICEDATE,
				avi.NTI_CUR,avi.NTA_CUR
				having SUM(avi.NTI_AMOUNT - isnull(avc_nti.TURNOVER_AMOUNT, 0) - isnull(alloc_nti.TURNOVER_AMOUNT, 0)) <>0
        ]]>
			</Command>
		</SQLSelect>

