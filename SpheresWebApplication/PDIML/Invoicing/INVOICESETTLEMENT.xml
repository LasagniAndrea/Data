    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>%%SR:TRADEADMIN_JOIN%%(istl.IDT)</SQLJoin>
      <SQLWhere>%%SR:TRADEADMIN_WHERE_PREDICATE%%</SQLWhere>
    </SQLWhere>

    <SQLWhere>
      <SQLWhere>
        <![CDATA[
        (istl.SETTLEMENT_DATE <= %%DA:DATE2.GetDataSQLValue()%%)
        and
        (istl.SETTLEMENT_DATE >= %%DA:DATE1.GetDataSQLValue()%%)
        and
        (istl.IDA_BENEFICIARY = case when -1=%%DA:ENTITY.GetDataSQLValue()%% Then  istl.IDA_BENEFICIARY else %%DA:ENTITY.GetDataSQLValue()%% end )
        ]]>
      </SQLWhere>
    </SQLWhere>

    <SQLRowState>
      <![CDATA[
      'lstaw=name:' ||
      case when UNALLOCATED_AMOUNT is null then  'allocated;title:Allocated'
      when UNALLOCATED_AMOUNT = 0 then  'allocated:title:Allocated'
      when UNALLOCATED_AMOUNT = SETTLEMENT_AMOUNT then 'fullyunallocated;title:Fully unallocated' else 'partiallyunallocated:title:Partially unallocated' end
      ]]>
    </SQLRowState>

    <SQLRowStyle type="class">
      <![CDATA[
      case when UNALLOCATED_AMOUNT is null then  'DataGrid_ItemStyle'
      when UNALLOCATED_AMOUNT = 0 then  'DataGrid_ItemStyle'
      when UNALLOCATED_AMOUNT = SETTLEMENT_AMOUNT then 'DataGrid-ERROR_2' else 'DataGrid-WARNING_2' end
      ]]>
    </SQLRowStyle>

