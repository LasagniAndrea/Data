<?xml version="1.0" encoding="utf-8" ?>
<customObjects xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" style="istableH:true;tblH-titleR:Characteristics;tblH-css:size4;">

  <customObject control="label"    clientid="ENTITY"  caption = "Entity_Title" xsi:type="CustomObjectLabel"/>
  <customObject control="dropdown" clientid="ENTITY" datatype="integer" required="true" listretrieval="predef:ActorEntity"
                postback="true"  postbackargument ="SELFCLEAR_"
                width="250px" misc="isAddAll:true;isUseSessionRestrict:true;isClear:false"
                xsi:type="CustomObjectDropDown"/>

  <customObject control="label"    clientid="MARKET"  caption = "Market_Title" xsi:type="CustomObjectLabel"/>
  <customObject control="dropdown" clientid="MARKET"  datatype="integer" required="true" listretrieval="predef:market_etd"
                postback="true"  postbackargument ="SELFCLEAR_"
                width="280px" misc="isAddAll:true;isUseSessionRestrict:true;isClear:true"
                xsi:type="CustomObjectDropDown"/>

  <customObject control="hr"  xsi:type="CustomObject" colspan="-1"/>

  <customObject control="label"    clientid="OPTIONITMATMOTM"  caption = "OPTIONITMATMOTM" xsi:type="CustomObjectLabel"/>
  <customObject control="dropdown" clientid="OPTIONITMATMOTM"  datatype="string" required="false"  listretrieval="predef:optionITM_ATM_OTM" defaultvalue="ITM"
                postback="true"  postbackargument ="SELFCLEAR_"
                width="250px"
                xsi:type="CustomObjectDropDown"/>

  <customObject control="label"    clientid="OPTIONITMTHRESHOLD"  caption = "OPTIONITMTHRESHOLD" xsi:type="CustomObjectLabel"/>
  <customObject control="textbox"  clientid="OPTIONITMTHRESHOLD"  datatype="decimal" required="true" defaultvalue="0.00"
                postback="true" postbackargument="SELFCLEAR_"
                xsi:type="CustomObjectTextBox" />

  <customObject control="label"    clientid="DENOPTIONACTIONTYPE"  caption = "DENOPTIONACTIONTYPE" xsi:type="CustomObjectLabel"/>
  <customObject control="dropdown" clientid="DENOPTIONACTIONTYPE"  datatype="string" required="true" listretrieval="predef:denoptionactiontype"
                postback="true"  postbackargument ="SELFCLEAR_"
                width="320px"
                xsi:type="CustomObjectDropDown"/>

  <customObject control="space"    width="20px"/>
  <customObject control="checkbox" clientid="ISIGNOREFEECALCULATION" datatype="bool" required="true" defaultvalue="false"
                postback="true"  postbackargument ="SELFNOLOAD_"
                caption="CHKISIGNOREFEECALCULATION" style="width:400px;" />
