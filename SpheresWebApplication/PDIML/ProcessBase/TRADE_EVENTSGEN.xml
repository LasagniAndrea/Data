    <Remove>false</Remove>
    <Image>gif\Process.gif</Image>
    <Notepad>false</Notepad>


    <SQLSelect>
      <Command rdbms="all">
        <![CDATA[
        select
          t.IDT,t.IDENTIFIER,t.DISPLAYNAME,t.DESCRIPTION,
          t.DTTRADE,t.DTSYS,t.DTBUSINESS,
          t.SOURCE,t.EXTLLINK,
          t.IDSTENVIRONMENT,t.IDSTACTIVATION,t.IDSTPRIORITY,t.IDSTBUSINESS,
          i.IDI, p.GPRODUCT
          from dbo.TRADE t
          %%SR:TRADE_JOIN%%(t.IDT, t)
          inner join dbo.INSTRUMENT i on i.IDI=t.IDI
          inner join dbo.PRODUCT p on (p.IDP=i.IDP) and (p.GPRODUCT !='ADM')
          <choose>
              <when test="{ENTITY} != -1">
                left outer join dbo.BOOK bbuyer on bbuyer.IDB = t.IDB_BUYER 
                left outer join dbo.BOOK bseller on bseller.IDB = t.IDB_SELLER
              </when>
          </choose>
          where 
          (t.IDSTENVIRONMENT in ('REGULAR','SIMUL'))
          and (t.IDSTACTIVATION = 'REGULAR')
          and (not exists (select 1 from dbo.EVENT e where e.IDT = t.IDT))
          <choose>
              <when test="{ENTITY} != -1">
              and  (((bbuyer.IDA_ENTITY = @ENTITY) or (bseller.IDA_ENTITY  = @ENTITY)) or (p.GPRODUCT='ASSET'))
              </when>
          </choose>
          and (%%SR:TRADE_WHERE_PREDICATE%%)
        ]]>
       </Command>
    </SQLSelect>
    
