<!-- RD 20100810 [] Restriction pour les messages de type : MONO-TRADE-->
<!-- FI 20120926 [] use value DOCNAME on name attribute of hyperlink -->
<!-- FI 20120926 [] use value DOCNAME on name attribute of hyperlink -->
<!-- FI 20190614 [23912] Link sur TRADE, choose en fonction de ISMODEREGENERATE, Affichage de la colonne Ordre -->
<!-- FI 20190718 [XXXXX] Add culture on RowState -->
<!-- EG 20200226 [25077] RDBMS : New version of Trades tables architecture (TRADEINSTRUMENT (INSTRUMENTNO=1) to TRADE)-->
<!-- FI 20200820 [25468] dates systemes en UTC -->
<!-- EG 20200923 [XXXXX] Alias ORDERID -->
<!-- FI 20240527 [WI937] ISMODEREGENERATE=0 La requête affiche les enregistrements pour lesquels la confirmation n'a pas été générée (tels que  CNFMSGXML is null)
                         ISMODEREGENERATE=1 La requête affiche tous les enregistrements
                         Utilisation des colonnes DTMCOFORCED et DTMCO  
-->
<Referentials>
  <Referential>
    <IsUseSQLParameters>true</IsUseSQLParameters>
    <TableName>VW_MCO</TableName>
    <freezeColumn>9</freezeColumn>
    <Ressource>MCO</Ressource>
    <ColumnByRow>3</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>true</Remove>
    
    <SQLJoin>inner join dbo.CNFMESSAGE cnfmsg on cnfmsg.MSGTYPE = 'MONO-TRADE' and cnfmsg.IDCNFMESSAGE = tblmain.IDCNFMESSAGE</SQLJoin>
    <SQLJoin>inner join dbo.TRADE t on t.IDT = tblmain.IDT</SQLJoin>
    <SQLJoin>inner join dbo.NCS ncs on ncs.IDA = tblmain.IDA_NCS</SQLJoin>
    <SQLJoin>inner join dbo.ACTOR a on a.IDA = tblmain.IDA_SENDTOPARTY</SQLJoin>
    
    <!-- FI 20240527 [WI937] -->
    <SQLWhere>
      <SQLWhere>
        <![CDATA[
        ((DTMCOFORCED=@DATE1) or (DTMCO=@DATE1)) and (tblmain.DTOBSOLETE is null)
        <choose>
          <when test="{ISMODEREGENERATE}=0">
            and (tblmain.CNFMSGXML is null)
          </when>
        </choose>
        ]]>
      </SQLWhere>
    </SQLWhere>

    <SQLRowState>
      <![CDATA[
        case 
          when tblmain.DTOBSOLETE is not null then 'img=src:unlike:title=Obsolete' 
          else ''
        end
        +
        case 
          when isnull(a.CULTURE_CNF,a.CULTURE)='fr-FR' then 'img=src:fr;title:French'
          when isnull(a.CULTURE_CNF,a.CULTURE)='it-IT' then 'img=src:it;title:Italian'
          else 'img=src:gb;title:English' 
        end
      ]]>
    </SQLRowState>

    <SQLRowStyle type="class">
      case when tblmain.DTOBSOLETE is null then  'DataGrid_ItemStyle' else 'DataGrid_ObsoleteStyle' end
    </SQLRowStyle>

