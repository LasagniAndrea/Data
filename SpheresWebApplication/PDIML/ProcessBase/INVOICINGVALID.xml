  <Referential>
    <IsUseSQLParameters>true</IsUseSQLParameters>
    <TableName>VW_EVENTFORINVOICINGVALID</TableName>
    <IsUseSQLParameters>true</IsUseSQLParameters>
    <AliasTableName>tr</AliasTableName>
    <Ressource></Ressource>
    <ColumnByRow>3</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>false</Remove>
    <Notepad>false</Notepad>

    <SQLSelect>
      <Command rdbms="all">
        <![CDATA[
                select distinct tr.IDT, tr.IDENTIFIER,tr.IDI,tr.DTTRADE,tr.DTSYS, tr.IDSTENVIRONMENT,tr.IDSTACTIVATION,tr.IDSTPRIORITY,tr.SOURCE,
                tr.DESCRIPTION,tr.EXTLLINK,
                tr.GTO_AMOUNT, tr.GTO_CUR, tr.GTO_DATE, tr.NTI_AMOUNT, tr.NTI_CUR, tr.NTI_DATE, tr.GRB_AMOUNT, tr.GRB_CUR, tr.GRB_DATE,
                tr.IDA_INVOICED, tr.IDA_ENTITY, tr.SCREENNAME,tr.GPRODUCT
                from dbo.VW_EVENTFORINVOICINGVALID tr
                %%SR:TRADEADMIN_JOIN%%(tr.IDT)
                where tr.GTO_DATE <= @DATE1
                and
                (
                (tr.IDA_ENTITYBUYER = case when -1=@ENTITY
                then tr.IDA_ENTITYBUYER else @ENTITY end)
                or
                (tr.IDA_ENTITYSELLER = case when -1=@ENTITY
                then tr.IDA_ENTITYSELLER else @ENTITY end)
                )
                and (tr.IDSTENVIRONMENT = 'SIMUL')
                and (%%SR:TRADEADMIN_WHERE_PREDICATE%%)
                ]]>
      </Command>
    </SQLSelect>

