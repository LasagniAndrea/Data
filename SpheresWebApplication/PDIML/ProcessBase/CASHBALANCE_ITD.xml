        select distinct em.DTENTITY,
        a_entity.IDA as ENTITY_IDA,a_entity.IDENTIFIER as ENTITY_IDENTIFIER,a_entity.DISPLAYNAME as ENTITY_DISPLAYNAME
        from dbo.ENTITYMARKET em
        %%SR:MARKET_JOIN%%(em.IDM)   
