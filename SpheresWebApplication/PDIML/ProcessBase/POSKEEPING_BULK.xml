        <ColumnGrp alias="RTK_GRP_DEALER">
          pk.ROLEDEALER
        </ColumnGrp>
      </RequestTrackData>
    </RequestTrack>

    <SQLPreSelectResource name="CLEARINGBULK_PRESELECT" />
    <SQLSelect>
      <Command rdbms = "all">
        <![CDATA[
          select 
          result.DATAKEYFIELD, result.IDI, result.DTENTITY, result.IDEM, result.ISCUSTODIAN,
          
          /*--ENTITY--*/          
          result.IDA_ENTITY, a_entity.IDENTIFIER as ENTITY_IDENTIFIER, a_entity.DISPLAYNAME as ENTITY_DISPLAYNAME, 
          
          /* CSSCUTODIAN */
          result.IDA_CSSCUSTODIAN, a_csscust.IDENTIFIER as CSSCUSTODIAN_IDENTIFIER, 
          
          /* MARKET */
          result.IDM, m.SHORT_ACRONYM as MARKET, 
          
          /*--DEALER--*/
          result.IDA_DEALER, result.IDB_DEALER, 
          dealer.IDENTIFIER as DEALER_IDENTIFIER, dealer.DISPLAYNAME as DEALER_DISPLAYNAME, bdealer.IDENTIFIER as DEALER_BOOKIDENTIFIER,
          case when arCLIENT.IDA is not null then 'Client' else 'House' end as ROLEDEALER,
          
          /*--CLEARER--*/
          result.IDA_CLEARER, result.IDB_CLEARER, 
          clearer.IDENTIFIER as CLEARER_IDENTIFIER, clearer.DISPLAYNAME as CLEARER_DISPLAYNAME, bclearer.IDENTIFIER as CLEARER_BOOKIDENTIFIER,
          
          /* ASSET */
          result.ASSETCATEGORY, result.IDASSET, asset.IDENTIFIER as ASSET_IDENTIFIER, asset.CHARACTERISTICS, 
          
          /* QTY */
          result.QTY_BUY, result.QTY_SELL, result.CLEARINGQTY, 
          
          result.GPRODUCT
          
         
          from
          (
            select blk.IDI, blk.DTENTITY, blk.IDEM, blk.ISCUSTODIAN,
            blk.IDA_ENTITY, blk.IDA_CSSCUSTODIAN, blk.IDM, 
            blk.IDA_DEALER, blk.IDB_DEALER,  blk.IDA_CLEARER, blk.IDB_CLEARER,
            blk.ASSETCATEGORY, blk.IDASSET, 			      
            isnull(blk.QTY_BUY,0) as QTY_BUY, isnull(blk.QTY_SELL,0) as QTY_SELL, 0 as CLEARINGQTY, 
            convert(varchar, blk.DTENTITY,112) || '_' || blk.ASSETCATEGORY || '_' || convert(varchar, blk.IDASSET) || '_' ||  convert(varchar, blk.IDB_DEALER) || '_' ||  convert(varchar, blk.IDB_CLEARER) as DATAKEYFIELD,
            blk.GPRODUCT
            from dbo.CLEARINGBULK_%%SHORTSESSIONID%%_W blk
          ) result
          
          inner join dbo.VW_MARKET_IDENTIFIER m on (m.IDM = result.IDM)
          inner join dbo.ACTOR a_entity on (a_entity.IDA = result.IDA_ENTITY)
          inner join dbo.ACTOR a_csscust on (a_csscust.IDA = result.IDA_CSSCUSTODIAN)
          inner join dbo.ACTOR dealer on (dealer.IDA = result.IDA_DEALER)
          inner join dbo.BOOK  bdealer on (bdealer.IDB = result.IDB_DEALER)
          inner join dbo.ACTOR clearer on (clearer.IDA = result.IDA_CLEARER)
          inner join dbo.BOOK  bclearer on (bclearer.IDB = result.IDB_CLEARER)
          inner join dbo.VW_ASSET_CONSULT asset on (asset.IDASSET = result.IDASSET) and (asset.ASSETCATEGORY = result.ASSETCATEGORY) 
          left outer join (select IDA from dbo.ACTOR 
          where exists (select 1 from dbo.ACTORROLE where IDA=ACTOR.IDA and IDROLEACTOR = 'CLIENT'))  arCLIENT on arCLIENT.IDA=dealer.IDA
      ]]>
      </Command>
    </SQLSelect>

