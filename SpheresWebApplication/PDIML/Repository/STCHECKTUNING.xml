    <Create>true</Create>
    <Modify>true</Modify>
    <Remove>true</Remove>
    <Image>gif\Administration.gif</Image>

    <SQLWhere>
      <!-- accès aux paramétrages sur IDA accessibles ou IDA = null -->
      <!-- accès aux paramétrages sur IDGINSTR accessibles (ie composés instrurments accessibles) ou IDGINSTR = null -->
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sr on sr.ID=tblmain.IDA and sr.CLASS='ACTOR' And sr.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>left outer join (select distinct IDGINSTR from dbo.INSTRG nsg
        inner join dbo.SESSIONRESTRICT sr on sr.ID=nsg.IDI and sr.CLASS='INSTRUMENT' And sr.SESSIONID='%%SESSIONID%%') gInstrSr													  on gInstrSr.IDGINSTR=tblmain.IDGINSTR
      </SQLJoin>
      <SQLWhere>(((tblmain.IDA is null) or  (sr.ID is not null)) And ((tblmain.IDGINSTR is null) or (gInstrSr.IDGINSTR is not null)))</SQLWhere>
    </SQLWhere>
