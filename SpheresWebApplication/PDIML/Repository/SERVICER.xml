    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>inner join dbo.SESSIONRESTRICT sr on sr.ID=tblmain.IDA and sr.CLASS='ACTOR' And sr.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>inner join dbo.BOOK b on b.IDB=tblmain.IDB</SQLJoin>
      <SQLJoin>inner join dbo.SESSIONRESTRICT sr2 on sr2.ID=b.IDA and sr2.CLASS='ACTOR' And sr2.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sre on sre.ID=b.IDA_ENTITY and sre.CLASS='ACTOR' And sre.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLWhere>((sre.ID is null and b.IDA_ENTITY is null) or (sre.ID is not null))</SQLWhere>
    </SQLWhere>

