    <SQLWhere>
      <!--FACILTY -->
      <ConditionApplication>FACILITY</ConditionApplication>
      <SQLWhere>(tblmain.FIXML_SecurityExchange=tblmain.ISO10383_ALPHA4)</SQLWhere>
      <SQLWhere>(tblmain.DTENABLED &lt;= getdate() and (tblmain.DTDISABLED is null or tblmain.DTDISABLED>getdate()))</SQLWhere>
    </SQLWhere>
    <SQLWhere>
      <!--FACILTY on ETD -->
      <ConditionApplication>ETD_FACILITY</ConditionApplication>
      <SQLWhere>
        (tblmain.ISTRADEDDERIVATIVE = 1) and (tblmain.FIXML_SecurityExchange=tblmain.ISO10383_ALPHA4) and
        (tblmain.DTENABLED &lt;= getdate() and (tblmain.DTDISABLED is null or tblmain.DTDISABLED>getdate()))
      </SQLWhere>
    </SQLWhere>
    <SQLWhere>
      <!--FACILTY on ESE -->
      <ConditionApplication>ESE_FACILITY</ConditionApplication>
      <SQLWhere>
        (tblmain.ISEQUITYMARKET = 1) and (tblmain.FIXML_SecurityExchange=tblmain.ISO10383_ALPHA4) and
        (tblmain.DTENABLED &lt;= getdate() and (tblmain.DTDISABLED is null or tblmain.DTDISABLED>getdate()))
      </SQLWhere>
    </SQLWhere>
    <SQLWhere>
      <!--FACILTY on COM -->
      <ConditionApplication>COM_FACILITY</ConditionApplication>
      <SQLWhere>
        (tblmain.ISCOMMODITYSPOT = 1) and (tblmain.FIXML_SecurityExchange=tblmain.ISO10383_ALPHA4) and
        (tblmain.DTENABLED &lt;= getdate() and (tblmain.DTDISABLED is null or tblmain.DTDISABLED>getdate()))
      </SQLWhere>
    </SQLWhere>

    <SQLWhere>
      <!--MARKET of FACILTY on ETD -->
      <ConditionApplication>ETD_MARKET_FACILITY</ConditionApplication>
      <SQLJoin>inner join dbo.VW_MARKET_FACILITY mkF on mkF.IDM=tblMain.IDM and mkF.FACILITY = '%%PARAM1%%'</SQLJoin>
      <SQLWhere>
        (tblmain.ISTRADEDDERIVATIVE = 1) and
        (tblmain.DTENABLED &lt;= getdate() and (tblmain.DTDISABLED is null or tblmain.DTDISABLED>getdate()))
      </SQLWhere>
    </SQLWhere>
    <SQLWhere>
      <!--MARKET of FACILTY on ETD -->
      <ConditionApplication>ESE_MARKET_FACILITY</ConditionApplication>
      <SQLJoin>inner join dbo.VW_MARKET_FACILITY mkF on mkF.IDM=tblMain.IDM and mkF.FACILITY = '%%PARAM1%%'</SQLJoin>
      <SQLWhere>
        (tblmain.ISEQUITYMARKET = 1) and
        (tblmain.DTENABLED &lt;= getdate() and (tblmain.DTDISABLED is null or tblmain.DTDISABLED>getdate()))
      </SQLWhere>
    </SQLWhere>
    <SQLWhere>
      <!--MARKET of FACILTY on ETD -->
      <ConditionApplication>COM_MARKET_FACILITY</ConditionApplication>
      <SQLJoin>inner join dbo.VW_MARKET_FACILITY mkF on mkF.IDM=tblMain.IDM and mkF.FACILITY = '%%PARAM1%%'</SQLJoin>
      <SQLWhere>
        (tblmain.ISCOMMODITYSPOT = 1) and
        (tblmain.DTENABLED &lt;= getdate() and (tblmain.DTDISABLED is null or tblmain.DTDISABLED>getdate()))
      </SQLWhere>
    </SQLWhere>

    <!-- CC 20130116 SQLJoin for VirtualColumn -->
    <!--<SQLJoin>left outer join dbo.CSS css on css.IDA = tblmain.IDA-->
    
    <SQLJoin>
      left outer join
      (
      select
      tblmain.IDM, case when im_mk.INITIALMARGINMETH is null then im_css.INITIALMARGINMETH else im_mk.INITIALMARGINMETH end as INITIALMARGINMETH
      from dbo.MARKET tblmain
      left outer join dbo.CSS css on css.IDA = tblmain.IDA
      left outer join dbo.IMMETHOD im_mk on (im_mk.IDIMMETHOD = tblmain.IDIMMETHOD)
      left outer join dbo.IMMETHOD im_css on (im_css.IDIMMETHOD = css.IDIMMETHOD)
      ) immeth on immeth.IDM = tblmain.IDM
    </SQLJoin >

    <SQLWhere>
      <!--
      USER non admin peut accéder aux acteurs :                             
      - non affectés à un profil (ou model) et créés ou modifiés par lui-même                     
      - accessibles par application des profils rattachés à lui ou à sa hiérarchie (ISENABLED=true) 
      - dits publics ACTEUR.ISENABLED=true                            
      -->
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT srm on srm.ID=tblmain.IDM and srm.CLASS='MARKET' and srm.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sra on sra.ID=tblmain.IDA and sra.CLASS='ACTOR' and sra.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLWhere>
        (
        (srm.ID is not null) or
        (not exists(select 1 from dbo.MARKETMODEL where IDM=tblmain.IDM)) and
        ((tblmain.IDAINS=%%ID_USER%% and tblmain.IDAUPD is null) or tblmain.IDAUPD=%%ID_USER%%)
        )
        and
        (
        (tblmain.IDA is null)
        or
        (sra.ID is not null)
        or
        (
        (not exists(select 1 from dbo.ACTORMODEL where IDA=tblmain.IDA))
        and
        ((tblmain.IDAINS=%%ID_USER%% and tblmain.IDAUPD is null) or tblmain.IDAUPD=%%ID_USER%%)
        )
        )
      </SQLWhere>
    </SQLWhere>

