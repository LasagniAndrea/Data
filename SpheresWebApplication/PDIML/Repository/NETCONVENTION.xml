  <Referential>
    <TableName>NETCONVENTION</TableName>
    <Ressource>NETCONVENTION</Ressource>
    <ColumnByRow>3</ColumnByRow>
    <Create>true</Create>
    <Modify>true</Modify>
    <Remove>true</Remove>
    <Image></Image>

    <SQLWhere>
      <!-- accès aux paramétrages sur IDGINSTR accessibles (ie composés instrurments accessibles) ou IDGINSTR = null -->
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>

      <SQLJoin>inner join dbo.MASTERAGREEMENT ma on ma.IDMASTERAGREEMENT=tblmain.IDMASTERAGREEMENT</SQLJoin>
      <SQLJoin>inner join dbo.SESSIONRESTRICT sr1 on sr1.ID=ma.IDA_1 and sr1.CLASS='ACTOR' And sr1.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>inner join dbo.SESSIONRESTRICT sr2 on sr2.ID=ma.IDA_2 and sr2.CLASS='ACTOR' And sr2.SESSIONID='%%SESSIONID%%'</SQLJoin>

      <SQLJoin>left outer join (select distinct IDGINSTR from dbo.INSTRG nsg
        inner join dbo.SESSIONRESTRICT sr on sr.ID=nsg.IDI and sr.CLASS='INSTRUMENT' And sr.SESSIONID='%%SESSIONID%%') gInstrSr
        on gInstrSr.IDGINSTR=tblmain.IDGINSTR
      </SQLJoin>
      <SQLJoin>Left outer join dbo.SESSIONRESTRICT sri on sri.ID=tblmain.IDI and sri.CLASS='INSTRUMENT' And sri.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLWhere>(((tblmain.IDGINSTR is null) or (gInstrSr.IDGINSTR is not null)) And ((tblmain.IDI is null) or (sri.ID is not null)))</SQLWhere>
    </SQLWhere>

