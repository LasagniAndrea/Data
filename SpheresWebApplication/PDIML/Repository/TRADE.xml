    <SQLSelect>
      <Command rdbms="all">
        <![CDATA[
        select
          t.IDT,t.IDENTIFIER,t.DISPLAYNAME,t.DESCRIPTION,
          t.DTTRADE,t.DTSYS,t.DTBUSINESS,
          t.SOURCE,t.EXTLLINK,
          t.IDSTENVIRONMENT,t.IDSTACTIVATION,t.IDSTPRIORITY,t.IDSTBUSINESS,
          i.IDI, p.GPRODUCT
          from dbo.TRADE t
          %%SR:TRADE_JOIN%%(t.IDT, t)
          inner join dbo.INSTRUMENT i on i.IDI=t.IDI
          inner join dbo.PRODUCT p on p.IDP=i.IDP and (p.GPRODUCT not in ('ADM','RISK') or (p.IDENTIFIER in ('cashBalance','cashPayment') ))  
          where (t.IDSTENVIRONMENT in ('REGULAR','SIMUL'))
          and (%%SR:TRADE_WHERE_PREDICATE%%)
        ]]>
      </Command>
    </SQLSelect>
    
    
