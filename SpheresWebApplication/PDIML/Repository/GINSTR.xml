    <xSQLWhere use="no">
      <!--USER non admin accéder aux Groupe d'instruments :                     -->
      <!--qui possède des instruments accessibles                               -->
      <!--sans instruments rattachés créé ou modifié par lui même               -->
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>
        left outer join ( select distinct IDGINSTR from dbo.INSTRG instrg
        inner  join dbo.SESSIONRESTRICT sr on sr.ID=instrg.IDI And sr.CLASS = 'INSTRUMENT' And sr.SESSIONID='%%SESSIONID%%')
        existInstrInGrpoup on existInstrInGrpoup.IDGINSTR = tblmain.IDGINSTR
      </SQLJoin>
      <SQLJoin>
        left outer join ( select distinct IDGINSTR from dbo.INSTRG instrg	)
        gInstrIsUsed on gInstrIsUsed.IDGINSTR = tblmain.IDGINSTR
      </SQLJoin>

      <SQLWhere>
        (existInstrInGrpoup.IDGINSTR is not null)
        or ((gInstrIsUsed.IDGINSTR is null) and ( (tblmain.IDAINS=%%ID_USER%% And tblmain.IDAUPD is null) Or tblmain.IDAUPD=%%ID_USER%%))
      </SQLWhere>
    </xSQLWhere>

