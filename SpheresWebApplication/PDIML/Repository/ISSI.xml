    <Create>true</Create>
    <Modify>true</Modify>
    <Remove>true</Remove>
    <Image></Image>

    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <!-- accès aux paramétrages sur IDA_STLOFFICE accessibles -->
      <SQLJoin>inner join dbo.SESSIONRESTRICT sra on sra.ID=tblmain.IDA_STLOFFICE and sra.CLASS='ACTOR' and sra.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <!-- accès aux paramétrages sur IDA_COUNTERPARTY accessibles ou IDA_COUNTERPARTY = null -->
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sra2 on sra2.ID=tblmain.IDA_COUNTERPARTY and sra2.CLASS='ACTOR' and sra2.SESSIONID='%%SESSIONID%%'</SQLJoin>2
      <!-- accès aux paramétrages sur IDGINSTR accessibles (composés d'au moins 1 instrument accessible) ou IDGINSTR = null -->
      <SQLJoin>
        left outer join (
        select distinct instrg.IDGINSTR
        from dbo.INSTRG instrg
        inner join dbo.SESSIONRESTRICT sr_gi on sr_gi.ID=instrg.IDI and sr_gi.CLASS='INSTRUMENT' and sr_gi.SESSIONID='%%SESSIONID%%'
        ) ginstrsr on ginstrsr.IDGINSTR=tblmain.IDINSTR and tblmain.TYPEINSTR='GrpInstr'
      </SQLJoin>
      <!-- accès aux paramétrages sur IDINSTR accessibles ou IDINSTR = null -->
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sr_i on sr_i.ID=tblmain.IDINSTR and tblmain.TYPEINSTR='Instr' and sr_i.CLASS='INSTRUMENT' and sr_i.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <!-- accès aux paramétrages sur IDB accessibles -->
      <!--USER non admin peut accéder :                                                                               -->
      <!--aux enregistrements SANS BOOK (IDB is null)                                                                 -->
      <!--OU aux enregistrements AVEC BOOK SANS ENTITY pour lequel le user a accès au propriétaire du book            -->                                                                                             -->
      <!--OU aux enregistrements AVEC BOOK AVEC ENTITY  pour lequel le user a accès au propriétaire du book           -->                                                                                               -->
      <!--aux enregistrements AVEC BOOK AVEC ENTITY (b.IDB is not null et sre.ID is not null) ainsi qu'à son entity   -->
      <SQLJoin>left outer join dbo.BOOK b on b.IDB = tblmain.IDB </SQLJoin>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sr3 on sr3.ID=b.IDA and sr3.CLASS='ACTOR' and sr3.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sr4 on sr4.ID=b.IDA_ENTITY and sr4.CLASS='ACTOR' and sr4.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <!--CC 20100726 Add SessionRestrict for INSTRUMENT (xxxINSTR_UNL) and MARKET -->
      <!-- accès aux paramétrages sur IDGINSTR accessibles (composés d'au moins 1 instrument accessible) ou IDGINSTR = null -->
      <SQLJoin>
        left outer join (
        select distinct instrg_unl.IDGINSTR
        from dbo.INSTRG instrg_unl
        inner join dbo.SESSIONRESTRICT sr_giunl on sr_giunl.ID=instrg_unl.IDI and sr_giunl.CLASS='INSTRUMENT' and sr_giunl.SESSIONID='%%SESSIONID%%'
        ) ginstrsr_unl on ginstrsr_unl.IDGINSTR=tblmain.IDINSTR_UNL and tblmain.TYPEINSTR_UNL='GrpInstr'
      </SQLJoin>
      <!-- accès aux paramétrages sur IDINSTR_UNL accessibles ou IDINSTR_UNL = null -->
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sr_iunl on sr_iunl.ID=tblmain.IDINSTR_UNL and tblmain.TYPEINSTR_UNL='Instr' and sr_iunl.CLASS='INSTRUMENT' and sr_iunl.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <!-- accès aux paramétrages sur IDGMARKET accessibles (composés d'au moins 1 marché accessible) ou IDGMARKET = null -->
      <SQLJoin>
        left outer join (
        select distinct mkg.IDGMARKET
        from dbo.MARKETG mkg
        inner join dbo.SESSIONRESTRICT sr_gm on sr_gm.ID=mkg.IDM and sr_gm.CLASS='MARKET' and sr_gm.SESSIONID='%%SESSIONID%%'
        ) gmksr on gmksr.IDGMARKET=tblmain.IDMARKET and tblmain.TYPEMARKET='GrpMarket'
      </SQLJoin>
      <!-- accès aux paramétrages sur IDMARKET accessibles ou IDMARKET = null -->
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sr_m on sr_m.ID=tblmain.IDMARKET and tblmain.TYPEMARKET='Market' and sr_m.CLASS='MARKET' and sr_m.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLWhere>
        (
        ((tblmain.IDA_COUNTERPARTY is null) or (sra2.ID is not null))
        and
        ((tblmain.IDINSTR is null) or (sr_i.ID is not null) or (ginstrsr.IDGINSTR is not null))
        and
        (
        (tblmain.IDB is null)
        or
        (b.IDB is not null and ((sr3.ID is not null and b.IDA_ENTITY is null) or (sr3.ID is not null and sr4.ID is not null)))
        )
        and
        ((tblmain.IDINSTR_UNL is null) or (sr_iunl.ID is not null) or (ginstrsr_unl.IDGINSTR is not null))
        and
        ((tblmain.IDMARKET is null) or (sr_m.ID is not null) or (gmksr.IDGMARKET is not null))
        )
      </SQLWhere>
    </SQLWhere>

    <SQLWhere>
      <!-- join for limited resultset to current elementtype -->
      <SQLJoin>
        left outer join dbo.VW_ISSIINSTR vw_ti1 on (vw_ti1.TYPEINSTR=tblmain.TYPEINSTR and vw_ti1.ID=vw_ti2.ID)
        left outer join dbo.VW_ISSIINSTR vw_tiu1 on (vw_tiu1.TYPEINSTR_UNL=tblmain.TYPEINSTR_UNL and vw_tiu1.ID=vw_tiu2.ID)
        left outer join dbo.VW_ISSIMARKET vw_tm1 on (vw_tm1.TYPEMARKET=tblmain.TYPEMARKET and vw_tm1.ID=vw_tm2.ID)
      </SQLJoin>
      <SQLWhere>
        (vw_ti1.TYPEINSTR is not null or vw_ti2.ID is null)
        and
        (vw_tiu1.TYPEINSTR_UNL is not null or vw_tiu2.ID is null)
        and
        (vw_tm1.TYPEMARKET is not null or vw_tm2.ID is null)
      </SQLWhere>
    </SQLWhere>

