    <Create>true</Create>
    <Modify>true</Modify>
    <Remove>true</Remove>
    <Image></Image>

    <SQLWhere>
      <!-- accès aux paramétrages sur IDGINSTR accessibles (ie composés instrurments accessibles) ou IDGINSTR = null -->
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>
        left outer join (select distinct IDGINSTR from dbo.INSTRG nsg
        inner join dbo.SESSIONRESTRICT sr on sr.ID=nsg.IDI and sr.CLASS='INSTRUMENT' And sr.SESSIONID='%%SESSIONID%%') gInstrSr
        on gInstrSr.IDGINSTR=tblmain.IDGINSTR
      </SQLJoin>
      <SQLJoin>Left outer join dbo.SESSIONRESTRICT sri on sri.ID=tblmain.IDI and sri.CLASS='INSTRUMENT' And sri.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLWhere>(((tblmain.IDGINSTR is null) or (gInstrSr.IDGINSTR is not null)) And ((tblmain.IDI is null) or (sri.ID is not null)))</SQLWhere>
    </SQLWhere>

