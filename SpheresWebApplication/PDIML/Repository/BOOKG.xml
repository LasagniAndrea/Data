    <!--CC 20100803 Add SessionRestrict for ACTOR -->
    <!--USER non admin peut accéder :                                                                         -->
    <!--aux enregistrements SANS BOOK (IDB is null)                                                           -->
    <!--OU                                                                                                    -->
    <!--aux enregistrements AVEC BOOK SANS ENTITY (b.IDB is not null et b.IDA_ENTITY is null)                 -->
    <!--                        pour lequel le user a accès au propriétaire du book (sra.ID is not null)      -->
    <!--OU                                                                                                    -->
    <!--aux enregistrements AVEC BOOK AVEC ENTITY (b.IDB is not null et sre.ID is not null)                   -->
    <!--                        pour lequel le user a accès au propriétaire du book (sra.ID is not null)      -->
    <!--                        ainsi qu'à son entity (sre.ID is not null)                                    -->
    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>left outer join dbo.BOOK b on b.IDB=tblmain.IDB</SQLJoin>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sra on sra.ID=b.IDA and sra.CLASS='ACTOR' and sra.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sre on sre.ID=b.IDA_ENTITY and sre.CLASS='ACTOR' and sre.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLWhere>
        (
        (tblmain.IDB is null)
        or
        (b.IDB is not null and ((sra.ID is not null and b.IDA_ENTITY is null) or (sra.ID is not null and sre.ID is not null)))
        )
      </SQLWhere>
    </SQLWhere>
    

