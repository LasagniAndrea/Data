      <!-- Check Book FK and Book Owner and Book Entity when exist -->
      <SQLJoin>inner join dbo.SESSIONRESTRICT srParent on srParent.ID=tblmain.FK and srParent.CLASS='ACTOR' And srParent.SESSIONID='%%SESSIONID%%'</SQLJoin>
