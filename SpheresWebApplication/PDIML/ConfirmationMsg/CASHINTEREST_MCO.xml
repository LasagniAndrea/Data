    <Image></Image>

    <SQLSelect>
      <Command rdbms="all">
        <![CDATA[
            select mco.IDMCO, 
            mco.IDT,t.IDENTIFIER,t.IDI,
            (select  ec.DTEVENT
				             from dbo.MCODET mcodet
		                 inner join dbo.EVENTCLASS ec on ec.IDE = mcodet.IDE 
			               where mcodet.IDMCO = mco.IDMCO) as DTENDPERIOD,  
            mco.DTMCO,mco.DTMCOFORCED,
            cnfmsg.STEPLIFE,
            mco.IDA_NCS,
            mco.IDNCSLINK,
            mco.CNFMSGXML,mco.CNFMSGXSL, mco.ISMSGTXTGENERATED, mco.DOCTYPEMSGTXT, mco.LOCNFMSGTXT,
            mco.ISMSGBINGENERATED, mco.DOCTYPEMSGBIN, mco.LOCNFMSGBIN,
            mco.IDA_SENDBYPARTY, mco.IDA_SENDBYOFFICE, mco.IDA_SENDTOPARTY, mco.IDA_SENDTOOFFICE,
            mco.IDCNFMESSAGE,
            ncs.IDIOTASK,
            mco.IDAINS, mco.DTINS,
            mco.IDAUPD, mco.DTUPD, mco.EXTLLINK, mco.ROWATTRIBUT, mco.ROWVERSION, mco.DOCNAME
            from dbo.VW_MCO mco
            inner join dbo.TRADE t on t.IDT = mco.IDT
            inner join dbo.CNFMESSAGE cnfmsg on cnfmsg.IDCNFMESSAGE = mco.IDCNFMESSAGE
            inner join dbo.NCS ncs on ncs.IDA = mco.IDA_NCS
            inner join dbo.INSTRUMENT i on (i.IDI = t.IDI)
            inner join dbo.PRODUCT p on (p.IDP = i.IDP) and (p.GPRODUCT = 'RISK') and (p.IDENTIFIER = 'cashBalanceInterest')
            ]]>
      </Command>
    </SQLSelect>

