    <IsUseSQLParameters>true</IsUseSQLParameters>
    <TableName>MONITORING</TableName>
    <Ressource>MONITORING</Ressource>
    <ColumnByRow>4</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>false</Remove>

    <SQLSelect>
      <Command rdbms = "all">
        <![CDATA[
        
        select cb.IDA_ENTITY, a_entity.IDENTIFIER as ENTITY_IDENTIFIER, a_entity.DISPLAYNAME as ENTITY_DISPLAYNAME, 
        cb.DTBUSINESS, cb.STATUS, cb.DTEND as DTSTATUS, cb.CBTIMING
        from dbo.CBREQUEST cb
        inner join dbo.ACTOR a_entity on (a_entity.IDA=cb.IDA_ENTITY)
        where (cb.IDA_ENTITY = case when -1= @ENTITY then cb.IDA_ENTITY else @ENTITY end) and 
              (cb.IDCBREQUEST = (select max(cbmax.IDCBREQUEST) 
                                 from dbo.CBREQUEST cbmax 
                                 where (cbmax.IDA_ENTITY=cb.IDA_ENTITY) and (cbmax.IDA_CBO is null))) 
        
      ]]>
      </Command>
    </SQLSelect>

