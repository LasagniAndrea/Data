    <SQLSelect>
      <Command rdbms = "sqlserver">
        <![CDATA[
select pl.DTSTPROCESS as DTSTPROCESS_L, pl.PROCESS as PROCESS_L, pl.IDSTPROCESS as IDSTPROCESS_L, pl.IDA, pl.HOSTNAME, pl.APPNAME, pl.APPVERSION, 
pdl.IDPROCESSDET_L,pdl.IDPROCESS_L,pdl.PROCESS,pdl.IDSTPROCESS,pdl.DTSTPROCESS,
case when smd.SYSCODE is null and smd_gb.SYSCODE is null
     then pdl.MESSAGE
     else 
     case when sm.SYSCODE = 'LOG' 
          then ''
          else sm.SYSCODE + '-' + right('00000' + convert(varchar,sm.SYSNUMBER),5) + ': '
          end
     +
     replace(replace(replace(replace(replace(isnull(smd.MESSAGE,smd_gb.MESSAGE),
                          '{1}',isnull(pdl.DATA1,'{1}')),
                          '{2}',isnull(pdl.DATA2,'{2}')),
                          '{3}',isnull(pdl.DATA3,'{3}')),
                          '{4}',isnull(pdl.DATA4,'{4}')),
                          '{5}',isnull(pdl.DATA5,'{5}')) 
     end as MESSAGE,
pdl.IDDATA,pdl.DATA1,pdl.DATA2,pdl.DATA3,pdl.DATA4,pdl.DATA5,
pdl.IDDATAIDENT,pdl.DATA1IDENT,pdl.DATA2IDENT,pdl.DATA3IDENT,pdl.DATA4IDENT,pdl.DATA5IDENT,
pdl.EXTLLINK,pdl.ROWATTRIBUT /*,pdl.ROWVERSION*/
from dbo.PROCESSDET_L pdl
inner join dbo.PROCESS_L pl on pl.IDPROCESS_L = pdl.IDPROCESS_L and pl.PROCESS='WEBSESSION' 
left outer join dbo.SYSTEMMSG sm on sm.SYSCODE=pdl.SYSCODE and sm.SYSNUMBER=pdl.SYSNUMBER
left outer join dbo.SYSTEMMSGDET smd on smd.SYSCODE=sm.SYSCODE and smd.SYSNUMBER=sm.SYSNUMBER and smd.CULTURE='%%CULTURE_USER_ISO3166_ALPHA2%%'
left outer join dbo.SYSTEMMSGDET smd_gb on smd_gb.SYSCODE=sm.SYSCODE and smd_gb.SYSNUMBER=sm.SYSNUMBER and smd_gb.CULTURE='en'
        ]]>
      </Command>
      <Command rdbms = "oracle">
        <![CDATA[
select pl.DTSTPROCESS as DTSTPROCESS_L, pl.PROCESS as PROCESS_L, pl.IDSTPROCESS as IDSTPROCESS_L, pl.IDA, pl.HOSTNAME, pl.APPNAME, pl.APPVERSION, 
pdl.IDPROCESSDET_L,pdl.IDPROCESS_L,pdl.PROCESS,pdl.IDSTPROCESS,pdl.DTSTPROCESS,
case when smd.SYSCODE is null and smd_gb.SYSCODE is null
     then pdl.MESSAGE
     else 
     case when sm.SYSCODE = 'LOG' 
          then ''
          else sm.SYSCODE || '-' || substr('00000' || to_char(sm.SYSNUMBER),-5) || ': '
          end
     ||
     replace(replace(replace(replace(replace(nvl(smd.MESSAGE,smd_gb.MESSAGE),
                          '{1}',nvl(pdl.DATA1,'{1}')),
                          '{2}',nvl(pdl.DATA2,'{2}')),
                          '{3}',nvl(pdl.DATA3,'{3}')),
                          '{4}',nvl(pdl.DATA4,'{4}')),
                          '{5}',nvl(pdl.DATA5,'{5}')) 
     end as MESSAGE,
pdl.IDDATA,pdl.DATA1,pdl.DATA2,pdl.DATA3,pdl.DATA4,pdl.DATA5,
pdl.IDDATAIDENT,pdl.DATA1IDENT,pdl.DATA2IDENT,pdl.DATA3IDENT,pdl.DATA4IDENT,pdl.DATA5IDENT,
pdl.EXTLLINK,pdl.ROWATTRIBUT /*,pdl.ROWVERSION*/
from dbo.PROCESSDET_L pdl
inner join dbo.PROCESS_L pl on pl.IDPROCESS_L = pdl.IDPROCESS_L and pl.PROCESS='WEBSESSION' 
left outer join dbo.SYSTEMMSG sm on sm.SYSCODE=pdl.SYSCODE and sm.SYSNUMBER=pdl.SYSNUMBER
left outer join dbo.SYSTEMMSGDET smd on smd.SYSCODE=sm.SYSCODE and smd.SYSNUMBER=sm.SYSNUMBER and smd.CULTURE='%%CULTURE_USER_ISO3166_ALPHA2%%'
left outer join dbo.SYSTEMMSGDET smd_gb on smd_gb.SYSCODE=sm.SYSCODE and smd_gb.SYSNUMBER=sm.SYSNUMBER and smd_gb.CULTURE='en'
        ]]>
      </Command>
    </SQLSelect>

