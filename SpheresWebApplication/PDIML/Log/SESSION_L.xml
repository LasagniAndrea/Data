    <SQLSelect>
      <Command rdbms = "sqlserver">
        <![CDATA[
    select pd.DATA1 as LOGIN,
    p.IDPROCESS_L,
    p.PROCESS,
    isnull(pd.IDSTPROCESS,p.IDSTPROCESS) as IDSTPROCESS,
    p.DTSTPROCESS,
    p.IDDATA,
    case when smd.SYSCODE is null and smd_gb.SYSCODE is null
    then p.MESSAGE
    else
    case when sm.SYSCODE = 'LOG'
    then ''
    else sm.SYSCODE + '-' + right('00000' + convert(varchar,sm.SYSNUMBER),5) + ': '
    end
    +
    replace(replace(replace(replace(replace(
    isnull(smd.MESSAGE,smd_gb.MESSAGE),
    '{1}',isnull(p.DATA1,'{1}')),
    '{2}',isnull(p.DATA2,'{2}')),
    '{3}',isnull(p.DATA3,'{3}')),
    '{4}',isnull(p.DATA4,'{4}')),
    '{5}',isnull(p.DATA5,'{5}'))
    end as MESSAGE,
    p.DATA1,p.DATA2,p.DATA3,p.DATA4,p.DATA5,
    p.IDTRK_L,
    p.IDA,p.HOSTNAME,p.APPNAME,p.APPVERSION,
    p.EXTLLINK,p.ROWATTRIBUT,p.ROWVERSION
    from dbo.PROCESS_L p
    inner join dbo.PROCESSDET_L pd on (pd.IDPROCESS_L=p.IDPROCESS_L) and (pd.PROCESS='WEBSESSION')
      and (pd.MESSAGE in ('Login','Logout','Timeout','Fail') or (pd.SYSCODE='LOG' and pd.SYSNUMBER in (2,3,4,5,6)) or (pd.SYSCODE='SYS' and pd.SYSNUMBER in (1,2)))
      and pd.IDPROCESSDET_L = (
      select MAX(pDetlastLogin.IDPROCESSDET_L)
      from dbo.PROCESSDET_L pDetlastLogin
      where pDetlastLogin.IDPROCESS_L = p.IDPROCESS_L
        and (MESSAGE in ('Login','Logout','Timeout','Fail') or (SYSCODE='LOG' and SYSNUMBER in (2,3,4,5,6)) or (SYSCODE='SYS' and SYSNUMBER in (1,2)))
      )
    left outer join dbo.SYSTEMMSG sm on sm.SYSCODE=pd.SYSCODE and sm.SYSNUMBER=pd.SYSNUMBER
    left outer join dbo.SYSTEMMSGDET smd on smd.SYSCODE=sm.SYSCODE and smd.SYSNUMBER=sm.SYSNUMBER and smd.CULTURE='%%CULTURE_USER_ISO3166_ALPHA2%%'
    left outer join dbo.SYSTEMMSGDET smd_gb on smd_gb.SYSCODE=sm.SYSCODE and smd_gb.SYSNUMBER=sm.SYSNUMBER and smd_gb.CULTURE='en'
    left outer join dbo.SYSTEMMSG sm2 on sm2.SYSCODE=pd.SYSCODE and sm2.SYSNUMBER=pd.SYSNUMBER
    left outer join dbo.SYSTEMMSGDET smd2 on smd2.SYSCODE=sm2.SYSCODE and smd2.SYSNUMBER=sm2.SYSNUMBER and smd2.CULTURE='%%CULTURE_USER_ISO3166_ALPHA2%%'
    left outer join dbo.SYSTEMMSGDET smd2_gb on smd2_gb.SYSCODE=sm2.SYSCODE and smd2_gb.SYSNUMBER=sm2.SYSNUMBER and smd2_gb.CULTURE='en'
    where p.PROCESS='WEBSESSION' and (p.MESSAGE='Session_Start' or (p.SYSCODE='LOG' and p.SYSNUMBER=1))
union all
    select 'N/A' as LOGIN,
    p.IDPROCESS_L,
    p.PROCESS,
    isnull(pd.IDSTPROCESS,p.IDSTPROCESS) as IDSTPROCESS,
    p.DTSTPROCESS,
    p.IDDATA,
    case when smd.SYSCODE is null and smd_gb.SYSCODE is null
    then p.MESSAGE
    else
    case when sm.SYSCODE = 'LOG'
    then ''
    else sm.SYSCODE + '-' + right('00000' + convert(varchar,sm.SYSNUMBER),5) + ': '
    end
    +
    replace(replace(replace(replace(replace(
    isnull(smd.MESSAGE,smd_gb.MESSAGE),
    '{1}',isnull(p.DATA1,'{1}')),
    '{2}',isnull(p.DATA2,'{2}')),
    '{3}',isnull(p.DATA3,'{3}')),
    '{4}',isnull(p.DATA4,'{4}')),
    '{5}',isnull(p.DATA5,'{5}'))
    end as MESSAGE,
    p.DATA1,p.DATA2,p.DATA3,p.DATA4,p.DATA5,
    p.IDTRK_L,
    p.IDA,p.HOSTNAME,p.APPNAME,p.APPVERSION,
    p.EXTLLINK,p.ROWATTRIBUT,p.ROWVERSION
    from dbo.PROCESS_L p
    left outer join dbo.PROCESSDET_L pd on (pd.IDPROCESS_L=p.IDPROCESS_L) and (pd.PROCESS='WEBSESSION')
     and (pd.MESSAGE in ('Login','Logout','Timeout','Fail') or (pd.SYSCODE='LOG' and pd.SYSNUMBER in (2,3,4,5,6)) or (pd.SYSCODE='SYS' and pd.SYSNUMBER in (1,2)))
    left outer join dbo.SYSTEMMSG sm on sm.SYSCODE=p.SYSCODE and sm.SYSNUMBER=p.SYSNUMBER
    left outer join dbo.SYSTEMMSGDET smd on smd.SYSCODE=sm.SYSCODE and smd.SYSNUMBER=sm.SYSNUMBER and smd.CULTURE='%%CULTURE_USER_ISO3166_ALPHA2%%'
    left outer join dbo.SYSTEMMSGDET smd_gb on smd_gb.SYSCODE=sm.SYSCODE and smd_gb.SYSNUMBER=sm.SYSNUMBER and smd_gb.CULTURE='en'
    left outer join dbo.SYSTEMMSG sm2 on sm2.SYSCODE=pd.SYSCODE and sm2.SYSNUMBER=pd.SYSNUMBER
    left outer join dbo.SYSTEMMSGDET smd2 on smd2.SYSCODE=sm2.SYSCODE and smd2.SYSNUMBER=sm2.SYSNUMBER and smd2.CULTURE='%%CULTURE_USER_ISO3166_ALPHA2%%'
    left outer join dbo.SYSTEMMSGDET smd2_gb on smd2_gb.SYSCODE=sm2.SYSCODE and smd2_gb.SYSNUMBER=sm2.SYSNUMBER and smd2_gb.CULTURE='en'
    where (p.PROCESS='WEBSESSION') and (p.MESSAGE='Session_Start' or (p.SYSCODE='LOG' and p.SYSNUMBER=1)) 
      and (pd.PROCESS is null)
        ]]>
      </Command>
