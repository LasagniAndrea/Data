      <SQLWhere>%%SR:TRADE_WHERE_PREDICATE%%</SQLWhere>
    </SQLWhere>

    
    <SQLSelect>
      <Command rdbms = "sqlserver">
        <![CDATA[
        /* SQL: WITH */
        with EVENT_CTE (IDT, IDE, IDE_EVENT, IDE_SOURCE, EVENTCODE, EVENTCLASS, DTEVENT, IDT_SOURCE, NIVEAU, COLSORT, IDEC) as
        (
	        select ev.IDT, ev.IDE, ev.IDE_EVENT, ev.IDE_SOURCE, ev.EVENTCODE, ec.EVENTCLASS, ec.DTEVENT,
          ev.IDT as IDT_SOURCE, 0 as NIVEAU, ev.IDE as COLSORT, ec.IDEC
	        from dbo.EVENT ev
          inner join dbo.EVENTCLASS ec on (ec.IDE = ev.IDE)
	        inner join dbo.EVENTENUM co on (co.CODE = 'EventCode') and (co.VALUE = ev.EVENTCODE) and (co.ISTRADEACTIONTRACKING = 1)
          inner join dbo.EVENTENUM cl on (cl.CODE = 'EventClass') and (cl.VALUE = ec.EVENTCLASS) and (cl.ISTRADEACTIONTRACKING = 1)
	        where 
	        (ev.IDT = @IDT)
        	
	        union all

	        select ev.IDT, ev.IDE, ev.IDE_EVENT, ev.IDE_SOURCE, ev.EVENTCODE, ec.EVENTCLASS, ec.DTEVENT, 
          ev.IDT , NIVEAU + 1 , evcte.COLSORT, ec.IDEC
	        from dbo.EVENT ev
          inner join dbo.EVENTCLASS ec on (ec.IDE = ev.IDE)
	        inner join EVENT_CTE evcte on (evcte.IDE = ev.IDE_EVENT)
          where NIVEAU < case when 2 = @NIVEAU then 10 else @NIVEAU end
          and (ev.IDSTACTIVATION = 'REGULAR')
          
	        union all

	        select evcte.IDT, ev.IDE, ev.IDE_EVENT, ev.IDE_SOURCE, ev.EVENTCODE, ec.EVENTCLASS, ec.DTEVENT, 
          ev.IDT, NIVEAU + 1, evcte.COLSORT, ec.IDEC
	        from dbo.EVENT ev
          inner join dbo.EVENTCLASS ec on (ec.IDE = ev.IDE)
	        inner join EVENT_CTE evcte on (evcte.IDE_SOURCE = ev.IDE)
          where NIVEAU < case when 2 = @NIVEAU then 1 else @NIVEAU end
        )
        /* SQL: ENDWITH */
        
        
        select evcte.IDT, evcte.IDE, evcte.IDE_EVENT, evcte.IDE_SOURCE, evcte.IDT_SOURCE, 
        evcte.EVENTCODE, ev.EVENTTYPE, evcte.EVENTCLASS, evcte.DTEVENT, evcte.NIVEAU,
        co.EXTVALUE as CODEVALUE, ty.EXTVALUE as TYPEVALUE, cl.EXTVALUE as CLASSVALUE,
        ev.STREAMNO, ev.VALORISATION as VALORISATION_FMT, 
        isnull(ev.UNIT,ev.UNITTYPE) as VALORISATIONTYPE, 
        case ev.IDSTTRIGGER when 'NA' then null else ev.IDSTTRIGGER end as IDSTTRIGGER, 
        ev.IDSTACTIVATION, ed.RATE, ed.SETTLEMENTRATE, ed.NOTIONALREFERENCE, ed.NOTE, evcte.COLSORT, pr.GPRODUCT, evcte.IDEC
        from EVENT_CTE evcte
        inner join dbo.EVENT ev on (ev.IDE = evcte.IDE)
        inner join dbo.TRADE tr on (tr.IDT = ev.IDT)
        inner join dbo.INSTRUMENT ti on (ti.IDI = tr.IDI)
        inner join dbo.PRODUCT pr on (pr.IDP = ti.IDP)
        inner join dbo.EVENTENUM co on (co.CODE = 'EventCode' and co.VALUE = evcte.EVENTCODE)
        inner join dbo.EVENTENUM ty on (ty.CODE = 'EventType' and ty.VALUE = ev.EVENTTYPE)
        inner join dbo.EVENTENUM cl on (cl.CODE = 'EventClass' and cl.VALUE = evcte.EVENTCLASS)
        left outer join dbo.EVENTDET ed on (ed.IDE = ev.IDE)
      ]]>
      </Command>
      <Command rdbms = "oracle">
        <![CDATA[
        select ev.IDT, ev.IDE, ev.IDE_EVENT, ev.IDE_SOURCE, evcte.IDT_SOURCE, 
        ev.EVENTCODE, ev.EVENTTYPE, evcte.EVENTCLASS, evcte.DTEVENT, evcte.NIVEAU,
        co.EXTVALUE as CODEVALUE, ty.EXTVALUE as TYPEVALUE, cl.EXTVALUE as CLASSVALUE,
        ev.STREAMNO, ev.VALORISATION as VALORISATION_FMT, 
        nvl(ev.UNIT,ev.UNITTYPE) as VALORISATIONTYPE, 
        case ev.IDSTTRIGGER when 'NA' then null else ev.IDSTTRIGGER end as IDSTTRIGGER, 
        ev.IDSTACTIVATION, ed.RATE, ed.SETTLEMENTRATE, ed.NOTIONALREFERENCE, ed.NOTE, evcte.COLSORT, pr.GPRODUCT, evcte.IDEC
        from dbo.EVENT ev
        inner join 
        (
          select ev.IDT, ev.IDE, ev.IDE_EVENT, ev.IDE_SOURCE, ev.EVENTCODE, ec.EVENTCLASS, ec.DTEVENT, ev.IDT as IDT_SOURCE, LEVEL-1 as NIVEAU, ev.IDE as COLSORT
          from dbo.EVENT ev
          inner join dbo.EVENTCLASS ec on (ec.IDE = ev.IDE)
          inner join dbo.EVENTENUM co on (co.CODE = 'EventCode') and (co.VALUE = ev.EVENTCODE) 
          inner join dbo.EVENTENUM cl on (cl.CODE = 'EventClass') and (cl.VALUE = ec.EVENTCLASS)
          where (ev.IDT = @IDT) and (ev.IDSTACTIVATION = 'REGULAR')
          start with (co.ISTRADEACTIONTRACKING = 1) and (cl.ISTRADEACTIONTRACKING = 1) 
          connect by nocycle prior ev.IDE = ev.IDE_EVENT  and
          LEVEL <= case when 2 = @NIVEAU then 6 else @NIVEAU+1 end
        ) evcte on (evcte.IDE = ev.IDE)
        inner join dbo.TRADE tr on (tr.IDT = ev.IDT)
        inner join dbo.INSTRUMENT ti on (ti.IDI = tr.IDI)
        inner join dbo.PRODUCT pr on (pr.IDP = ti.IDP)
        inner join dbo.EVENTENUM co on (co.CODE = 'EventCode' and co.VALUE = evcte.EVENTCODE)
        inner join dbo.EVENTENUM ty on (ty.CODE = 'EventType' and ty.VALUE = ev.EVENTTYPE)
        inner join dbo.EVENTENUM cl on (cl.CODE = 'EventClass' and cl.VALUE = evcte.EVENTCLASS)
        left outer join dbo.EVENTDET ed on (ed.IDE = ev.IDE)
      ]]>
      </Command>
    </SQLSelect>

