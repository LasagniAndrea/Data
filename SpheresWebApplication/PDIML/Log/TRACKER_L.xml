    <SQLSelect>
      <Command rdbms="sqlserver">
        <![CDATA[select  
        tk.GROUPTRACKER, tk.GROUPTRACKER as GROUPTRACKERDISPLAYNAME, 
        tk.IDTRK_L, tk.DTINS, tk.IDAINS, tk.HOSTNAMEINS , tk.IDSESSIONINS, 
        tk.READYSTATE, tk.STATUSTRACKER, 
        tk.ISMARKED, tk.NOTEMARKED, tk.IDAMARKED, tk.DTMARKED, 
        tk.POSTEDMSG, tk.POSTEDSUBMSG, tk.SUCCESSMSG, tk.WARNINGMSG, tk.ERRORMSG, tk.NONEMSG,
        pl.IDPROCESS_L, pl.PROCESS, pl.DTSTPROCESS, pl.IDA, pl.HOSTNAME, pl.APPNAME, pl.APPVERSION,
        tk.DTUPD, tk.IDAUPD, tk.HOSTNAMEUPD , tk.IDSESSIONUPD,
        tk.IDDATAIDENT, 
        smd.SHORTMESSAGE, 
        %%DH:SQLIF%%[%%PARAM1%%;COMMON;case when mat.MATURITYMONTHYEAR is not null then mat.MATURITYMONTHYEAR 
 			  	   when asset.IDENTIFIER is not null then asset.IDENTIFIER 
				     when io.IDENTIFIER is not null then io.IDENTIFIER 
				     when ac.IDENTIFIER is not null then ac.IDENTIFIER 
				     when tr.IDENTIFIER is not null then tr.IDENTIFIER 
             when tk.IDDATA > 0 then convert(varchar(32), tk.IDDATA)
				     else '-'
				     end as IDDATA,]
        %%DH:SQLIF%%[%%PARAM1%%;ONLYSIO;case when io.IDENTIFIER is not null then io.IDENTIFIER 
				     when ac.IDENTIFIER is not null then ac.IDENTIFIER 
				     when tk.IDDATA > 0 then convert(varchar(32), tk.IDDATA)
				     else '-'
				     end as IDDATA,]
            case when smd.SYSCODE is null and smd_gb.SYSCODE is null
            then pl.MESSAGE
            else
            case sm.SYSCODE when 'LOG' then '' when 'TRK' then ''
                 else sm.SYSCODE || '-' || right('00000' || convert(varchar,sm.SYSNUMBER),5) || ': '
                 end
            ||
            replace(replace(replace(replace(replace(
            isnull(smd.MESSAGE,smd_gb.MESSAGE),
            '{1}',isnull(tk.DATA1,'{1}')),
            '{2}',isnull(tk.DATA2,'{2}')),
            '{3}',isnull(tk.DATA3,'{3}')),
            '{4}',isnull(tk.DATA4,'{4}')),
            '{5}',isnull(tk.DATA5,'{5}'))
            end 
            || 
            case when pl.QUEUEMSG is null then '' else '<br/> - Message: <b>' || pl.QUEUEMSG || '</b>' end
            as MESSAGE,
             
        tk.DATA1IDENT, tk.DATA1, tk.DATA2IDENT, tk.DATA2, tk.DATA3IDENT, tk.DATA3, tk.DATA4IDENT, tk.DATA4, tk.DATA5IDENT, tk.DATA5,
        tk.EXTLID, tk.ACKXML, tk.ACKSTATUS, tk.SYSNUMBER,
        tk.EXTLLINK, tk.ROWATTRIBUT, tk.ROWVERSION
        from dbo.TRACKER_L tk 
        left outer join (select IDTRK_L, IDPROCESS_L, PROCESS, DTSTPROCESS, IDA, HOSTNAME, APPNAME, APPVERSION, MESSAGE, QUEUEMSG from dbo.PROCESS_L where %%WHEREDATAKEY%% order by IDPROCESS_L offset 0 row fetch next 1 rows only) pl on (pl.IDTRK_L = tk.IDTRK_L)
        left outer join dbo.SYSTEMMSG sm on (sm.SYSCODE=tk.SYSCODE) and (sm.SYSNUMBER=tk.SYSNUMBER)
        left outer join dbo.SYSTEMMSGDET smd on (smd.SYSCODE=sm.SYSCODE) and (smd.SYSNUMBER=sm.SYSNUMBER) and (smd.CULTURE='%%CULTURE_USER_ISO3166_ALPHA2%%')
        left outer join dbo.SYSTEMMSGDET smd_gb on (smd_gb.SYSCODE=sm.SYSCODE) and (smd_gb.SYSNUMBER=sm.SYSNUMBER) and (smd_gb.CULTURE='en')
        left outer join dbo.ACTOR ac on (ac.IDA = tk.IDDATA) and (tk.IDDATAIDENT = 'ACTOR')
        %%DH:SQLIF%%[%%PARAM1%%;COMMON;left outer join dbo.TRADE tr on (tr.IDT = tk.IDDATA) and (tk.IDDATAIDENT like 'TRADE%') 
        left outer join dbo.IOTASK io on (io.IDIOTASK = tk.IDDATA) and (tk.IDDATAIDENT = 'IOTASK') 
        left outer join dbo.QUOTE_FXRATE_H quote on (quote.IDQUOTE_H = tk.IDDATA) and (tk.IDDATAIDENT = 'QUOTE_FXRATE_H')
        left outer join dbo.ASSET_FXRATE asset on (asset.IDASSET = quote.IDASSET)
        left outer join dbo.MATURITY mat on (mat.IDMATURITY = tk.IDDATA) and (tk.IDDATAIDENT = 'MATURITY')]
        %%DH:SQLIF%%[%%PARAM1%%;ONLYSIO;left outer join dbo.IOTASK io on (io.IDIOTASK = tk.IDDATA) and (tk.IDDATAIDENT = 'IOTASK')]
        ]]>
      </Command>
      <Command rdbms="oracle">
        <![CDATA[select  
        tk.GROUPTRACKER, tk.GROUPTRACKER as GROUPTRACKERDISPLAYNAME, 
        tk.IDTRK_L, tk.DTINS, tk.IDAINS, tk.HOSTNAMEINS , tk.IDSESSIONINS, 
        tk.READYSTATE, tk.STATUSTRACKER, 
        tk.ISMARKED, tk.NOTEMARKED, tk.IDAMARKED, tk.DTMARKED, 
        tk.POSTEDMSG, tk.POSTEDSUBMSG, tk.SUCCESSMSG, tk.WARNINGMSG, tk.ERRORMSG, tk.NONEMSG,
        pl.IDPROCESS_L, pl.PROCESS, pl.DTSTPROCESS, pl.IDA, pl.HOSTNAME, pl.APPNAME, pl.APPVERSION,
        tk.DTUPD, tk.IDAUPD, tk.HOSTNAMEUPD , tk.IDSESSIONUPD,
        tk.IDDATAIDENT, 
        smd.SHORTMESSAGE, 
        %%DH:SQLIF%%[%%PARAM1%%;COMMON;case when mat.MATURITYMONTHYEAR is not null then mat.MATURITYMONTHYEAR 
 			  	   when asset.IDENTIFIER is not null then asset.IDENTIFIER 
				     when io.IDENTIFIER is not null then io.IDENTIFIER 
				     when ac.IDENTIFIER is not null then ac.IDENTIFIER 
				     when tr.IDENTIFIER is not null then tr.IDENTIFIER 
             when tk.IDDATA > 0 then to_char(tk.IDDATA)
				     else '-'
				     end as IDDATA,]
        %%DH:SQLIF%%[%%PARAM1%%;ONLYSIO;case when io.IDENTIFIER is not null then io.IDENTIFIER 
				     when ac.IDENTIFIER is not null then ac.IDENTIFIER 
				     when tk.IDDATA > 0 then to_char(tk.IDDATA)
				     else '-'
				     end as IDDATA,]
            case when smd.SYSCODE is null and smd_gb.SYSCODE is null
            then pl.MESSAGE
            else
            case sm.SYSCODE when 'LOG' then '' when 'TRK' then ''
                 else sm.SYSCODE || '-' || substr('00000' || to_char(sm.SYSNUMBER),-5) || ': '
                 end
            ||
            replace(replace(replace(replace(replace(
            nvl(smd.MESSAGE,smd_gb.MESSAGE),
            '{1}',nvl(tk.DATA1,'{1}')),
            '{2}',nvl(tk.DATA2,'{2}')),
            '{3}',nvl(tk.DATA3,'{3}')),
            '{4}',nvl(tk.DATA4,'{4}')),
            '{5}',nvl(tk.DATA5,'{5}'))
            end 
            || 
            case when pl.QUEUEMSG is null then '' else '<br/> - Message: <b>' || pl.QUEUEMSG || '</b>' end
            as MESSAGE,
             
        tk.DATA1IDENT, tk.DATA1, tk.DATA2IDENT, tk.DATA2, tk.DATA3IDENT, tk.DATA3, tk.DATA4IDENT, tk.DATA4, tk.DATA5IDENT, tk.DATA5,
        tk.EXTLID, tk.ACKXML, tk.ACKSTATUS, tk.SYSNUMBER,
        tk.EXTLLINK, tk.ROWATTRIBUT, tk.ROWVERSION
        from dbo.TRACKER_L tk 
        left outer join (
          select IDTRK_L, IDPROCESS_L, PROCESS, DTSTPROCESS, IDA, HOSTNAME, APPNAME, APPVERSION, 
          MESSAGE, QUEUEMSG 
          from dbo.PROCESS_L  
          where %%WHEREDATAKEY%% order by IDPROCESS_L offset 0 row fetch next 1 rows only
        ) pl on (pl.IDTRK_L = tk.IDTRK_L)
        left outer join dbo.SYSTEMMSG sm on (sm.SYSCODE=tk.SYSCODE) and (sm.SYSNUMBER=tk.SYSNUMBER)
        left outer join dbo.SYSTEMMSGDET smd on (smd.SYSCODE=sm.SYSCODE) and (smd.SYSNUMBER=sm.SYSNUMBER) and (smd.CULTURE='%%CULTURE_USER_ISO3166_ALPHA2%%')
        left outer join dbo.SYSTEMMSGDET smd_gb on (smd_gb.SYSCODE=sm.SYSCODE) and (smd_gb.SYSNUMBER=sm.SYSNUMBER) and (smd_gb.CULTURE='en')
        left outer join dbo.ACTOR ac on (ac.IDA = tk.IDDATA) and (tk.IDDATAIDENT = 'ACTOR')
        %%DH:SQLIF%%[%%PARAM1%%;COMMON;left outer join dbo.TRADE tr on (tr.IDT = tk.IDDATA) and (tk.IDDATAIDENT like 'TRADE%') 
        left outer join dbo.IOTASK io on (io.IDIOTASK = tk.IDDATA) and (tk.IDDATAIDENT = 'IOTASK') 
        left outer join dbo.QUOTE_FXRATE_H quote on (quote.IDQUOTE_H = tk.IDDATA) and (tk.IDDATAIDENT = 'QUOTE_FXRATE_H')
        left outer join dbo.ASSET_FXRATE asset on (asset.IDASSET = quote.IDASSET)
        left outer join dbo.MATURITY mat on (mat.IDMATURITY = tk.IDDATA) and (tk.IDDATAIDENT = 'MATURITY')]
        %%DH:SQLIF%%[%%PARAM1%%;ONLYSIO;left outer join dbo.IOTASK io on (io.IDIOTASK = tk.IDDATA) and (tk.IDDATAIDENT = 'IOTASK')]
        ]]>
      </Command>
