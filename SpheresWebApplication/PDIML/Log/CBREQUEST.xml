<Referentials>
  <Referential>
    <TableName>POSREQUEST</TableName>
    <AliasTableName>pr</AliasTableName>
    <ColumnByRow>4</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>false</Remove>
    <AttachedDoc>false</AttachedDoc>
    <SQLSelect>
      <Command rdbms = "all">
        <![CDATA[
          select cb.IDCBREQUEST, cb.STATUS as STATUSRETURN, 
          cb.IDA_ENTITY, 
          cb.IDA_CBO, cb.IDB_CBO, cb.IDA_CBO_CBREQUEST, 
          cb.DTBUSINESS, cb.EXCHANGETYPE, 
          cb.IDT, cb.DTSTART, cb.DTEND, cb.IDAINS, cb.DTINS as DTINS, isnull(cb.DTUPD,cb.DTINS) as DTUPD, 
          cb.EXTLLINK, cb.ROWATTRIBUT, cb.ROWVERSION,
          pr.IDPR, pr.REQUESTTYPE, en.DOCUMENTATION, 
          a_ETY.IDENTIFIER as ETY_IDENTIFIER,
          a_CBO.IDENTIFIER as CBO_IDENTIFIER, a_CBO.DISPLAYNAME as CBO_DISPLAYNAME,
          b_CBO.IDENTIFIER as CBO_BOOKIDENTIFIER, b_CBO.DISPLAYNAME as CBO_BOOKDISPLAYNAME,
          a_CBO_PARENT.IDENTIFIER as CBOPARENT_IDENTIFIER, a_CBO_PARENT.DISPLAYNAME as CBOPARENT_DISPLAYNAME,
          
          /*
          ev_mgr.VALORISATION as MARGINREQAMOUNT, ev_mgr.UNIT as MARGINREQCURRENCY, 
          ev_csu.VALORISATION as CASHUSEDAMOUNT, ev_csu.UNIT as CASHUSEDCURRENCY, 
          ev_clu.VALORISATION as COLLATERALUSEDAMOUNT, ev_clu.UNIT as COLLATERALUSEDCURRENCY, 
          ev_csb.VALORISATION as CASHBALANCEAMOUNT, ev_csb.UNIT as CASHBALANCECURRENCY, 
          */
          
          'RISK' as GPRODUCT
          
          from dbo.POSREQUEST pr
          inner join dbo.CBREQUEST cb on (cb.IDPR = isnull(pr.IDPR_POSREQUEST,pr.IDPR)) and (pr.REQUESTTYPE in ('EOD','EOD_CASHBALANCE'))
          inner join dbo.ACTOR a_ETY on (a_ETY.IDA=cb.IDA_ENTITY)
          left outer join dbo.ACTOR a_CBO on (a_CBO.IDA=cb.IDA_CBO)
          left outer join dbo.BOOK b_CBO on (b_CBO.IDB=cb.IDB_CBO)
          inner join dbo.ENUM en on (en.CODE = 'PosRequestTypeEnum') and (en.VALUE = pr.REQUESTTYPE)
          left outer join dbo.TRADE tr on (tr.IDT = cb.IDT)
          left outer join dbo.ACTOR a_CBO_PARENT on (a_CBO_PARENT.IDA=cb.IDA_CBO_CBREQUEST)
          /*
          left outer join dbo.EVENT ev_mgr on (ev_mgr.IDT = cb.IDT) and (ev_mgr.EVENTTYPE = 'MGR')
          left outer join dbo.EVENT ev_csu on (ev_csu.IDT = cb.IDT) and (ev_csu.EVENTTYPE = 'CSU')
          left outer join dbo.EVENT ev_clu (ev_clu.IDT = cb.IDT) and (ev_clu.EVENTTYPE = 'CLU')
          left outer join dbo.EVENT ev_csb (ev_csb.IDT = cb.IDT) and (ev_csb.EVENTTYPE = 'CSB')
          */
      ]]>
      </Command>
    </SQLSelect>

    <SQLRowStyle type="class">
      case STATUSRETURN
      when 'SUCCESS' then 'DataGrid-SUCCESS_2'
      when 'ERROR' then 'DataGrid-ERROR'
      else 'DataGrid_ItemStyle'
      end
    </SQLRowStyle>

    <SQLRowState>
      case when IDA_CBO is null then 'img=src:~/Images/PNG/EOD_CASHBALANCE' || '-' || STATUSRETURN || '.png;' end
    </SQLRowState>

