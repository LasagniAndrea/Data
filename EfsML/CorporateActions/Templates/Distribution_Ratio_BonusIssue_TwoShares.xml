<?xml version="1.0" encoding="utf-8"?>
<procedures xmlns="http://www.efs.org/2007/EFSmL-3-0"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xmlns:ecoreB="http://www.eclipse.org/emf/2002/Ecore"
                xsi:schemaLocation="http://www.efs.org/2007/EFSmL-3-0 ../Schemas/EfsML-CorporateActions-3-0.xsd">
  <underlyers>
    <underlyer>
      <componentMethod id="cumPrice1">
        <description>Cum price Trading day</description>
        <method>
          <name>CumPriceTradingDay</name>
        </method>
      </componentMethod>
    </underlyer>
    <underlyer>
      <componentMethod id="cumPrice2">
        <description>Cum price Trading day</description>
        <method>
          <name>CumPriceTradingDay</name>
        </method>
      </componentMethod>
    </underlyer>
  </underlyers>
  <ratio>
    <description>Distribution of shares</description>
    <methodType>Ratio</methodType>
    <rFactor>
      <componentFormula id="ratioFactor" name="R">
        <description>Adjustment ratio</description>
        <formula>
          <mathExpression>(P1-1*((h/r)*P2))/P1</mathExpression>
          <component id="nbEligibleShares" name="h">
            <description>Shares as eligible for the distribution</description>
            <result>
              <unit>0</unit>
            </result>
          </component>
          <component id="nbDistributedShares" name="r">
            <description>New shares distributed</description>
            <result>
              <unit>0</unit>
            </result>
          </component>
          <componentReference href="cumPrice1" name="P1" id="cumPrice1_r1"/>
          <componentReference href="cumPrice2" name="P2" id="cumPrice2_r1"/>
        </formula>
      </componentFormula>
    </rFactor>
    <contract>
      <!-- Add Ratio XML -->
    </contract>
  </ratio>
</procedures>
