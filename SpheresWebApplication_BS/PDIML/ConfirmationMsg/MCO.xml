<!-- FI 20150416 [XXXXX] Add SESSIONRESTRICT -->
<!-- FI 20120926 [XXXXX] Use value DOCNAME on name attribute of hyperlink -->
<!-- RD 20190718 [XXXXX] Add culture on RowState -->

<Referentials>
  <Referential>
    <TableName>VW_MCO</TableName>
    <Ressource>MCO</Ressource>
    <ColumnByRow>3</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>false</Remove>
    <Image></Image>

    <SQLJoin>inner join dbo.CNFMESSAGE cnfmsg on cnfmsg.IDCNFMESSAGE = tblmain.IDCNFMESSAGE</SQLJoin>
    <SQLJoin>left outer join dbo.TRADE t on t.IDT = tblmain.IDT</SQLJoin>
    <SQLJoin>inner join dbo.NCS ncs on ncs.IDA = tblmain.IDA_NCS</SQLJoin>
    <SQLJoin>inner join dbo.ACTOR a on a.IDA = tblmain.IDA_SENDTOPARTY</SQLJoin>

    <SQLRowState>
      <![CDATA[
        case 
          when tblmain.DTOBSOLETE is not null then 'img=src:unlike:title=Obsolete' 
          else ''
        end
        +
        case 
          when isnull(a.CULTURE_CNF,a.CULTURE)='fr-FR' then 'img=src:fr;title:French'
          when isnull(a.CULTURE_CNF,a.CULTURE)='it-IT' then 'img=src:it;title:Italian'
          else 'img=src:gb;title:English' 
        end
      ]]>
    </SQLRowState>

    <SQLRowStyle type="class">
      case when tblmain.DTOBSOLETE is null then  'DataGrid_ItemStyle' else 'DataGrid_ObsoleteStyle' end
    </SQLRowStyle>

    <SQLWhere>
      <!-- FI 20150416 un enregistrement MCO est consultable si le destinataire est visible si l'émetteur est l'entité rattachée au user 
          Rq l'émetteur réel est le contact office 
          
          Lorque le destinatiare est un client ou un compte maison 
           => MCO.IDA_SENDBYPARTY est alimenté avec l'entité
          
          Lorque le destinatiare est une contrepartie externe (la contrepartie adverse est nécessairement gérée)
          => la contrepartie adverse est un client. MCO.IDA_SENDBYPARTY est alimenté avec l'entité     
          => la contrepartie adverse est un compte maison. MCO.IDA_SENDBYPARTY, MCO.IDB_SENDBYPARTY est alimenté avec lecompte maison. L'entité est l'entité de gestion du book maison.
      -->
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>%%SR:ACTOR_JOIN%%(tblmain.IDA_SENDTOPARTY)</SQLJoin>

      <!-- PL/FI 20170504 mise en commentaire 
        =>Nouvelle règle:  Un message est si l'utilisateur à accès au destinataire
      -->
      <!--<SQLJoin>left outer join BOOK sr_b on sr_b.IDB = tblmain.IDB_SENDBYPARTY</SQLJoin>
      <SQLWhere>isnull(sr_b.IDA_ENTITY,tblmain.IDA_SENDBYPARTY)=%%IDA_ENTITY%%</SQLWhere>-->
    </SQLWhere>
    

