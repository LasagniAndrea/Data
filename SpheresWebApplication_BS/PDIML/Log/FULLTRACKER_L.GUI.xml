<?xml version="1.0" encoding="utf-8" ?>
<customObjects bs-grid="col-sm-4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" style="istableH:true;tblH-titleR:Characteristics;tblH-css:size4;">

  <customObject bs-grid="col-sm-3" bs-row="1" control="label" clientid="DTHISTORY" xsi:type="CustomObjectLabel"   caption="DateFrom" />

  <customObject bs-grid="col-sm-3" control="textbox" clientid="DTHISTORY" xsi:type="CustomObjectTextBox" datatype="date"
                                   width="80"
                                   required="false"
                                   postback="true"
                                   postbackargument="SELFCLEAR_"/>

  <customObject bs-grid="col-sm-3" control="label" clientid="ISMARKED" xsi:type="CustomObjectLabel" caption="ISMARKED"/>

  <customObject bs-grid="col-sm-3" control="htmlcheckbox" clientid="ISMARKED" datatype="bool" defaultvalue="false"
                                   required="true"
                                   postback="true"
                                   misc="label:false"
                                   postbackargument ="SELFCLEAR_"   />

  <customObject bs-grid="col-sm-3" bs-row="1" control="label"    clientid="GROUPTRACKER" xsi:type="CustomObjectLabel"    caption="GroupTracker" />

  <customObject bs-grid="col-sm-2" control="dropdown" clientid="GROUPTRACKER" xsi:type="CustomObjectDropDown" datatype="string"
                                   listretrieval="predef:enum.[code:GroupTrackerEnum;isResource:true;]"
                                   width="250"
                                   required="false"
                                   postback="true"
                                   postbackargument ="SELFCLEAR_"
                                   misc="isClear:true" />

  <customObject bs-grid="col-sm-3" bs-row="1" control="label"    clientid="READYSTATE" xsi:type="CustomObjectLabel"    caption="ReadyState" />

  <customObject bs-grid="col-sm-2" control="dropdown" clientid="READYSTATE" xsi:type="CustomObjectDropDown" datatype="string"
                                   listretrieval="predef:enum.[code:ReadyStateTrackerEnum;isResource:true;]"
                                   width="250"
                                   required="false"
                                   postback="true"
                                   postbackargument ="SELFCLEAR_"
                                   misc="isClear:true" />

  <customObject bs-grid="col-sm-3" bs-row="1" control="label"    clientid="STATUSTRACKER" xsi:type="CustomObjectLabel"    caption="StatusTracker" />

  <customObject bs-grid="col-sm-2" control="dropdown" clientid="STATUSTRACKER" xsi:type="CustomObjectDropDown" datatype="string"
                                   listretrieval="predef:enum.[code:StatusTrackerEnum;isResource:true;]"
                                   width="250"
                                   required="false"
                                   postback="true"
                                   postbackargument ="SELFCLEAR_"
                                   misc="isClear:true" />
</customObjects>

