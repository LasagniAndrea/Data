<?xml version="1.0" encoding="UTF-8" ?>
<procedures xmlns="http://www.efs.org/2007/EFSmL-3-0"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xmlns:ecoreB="http://www.eclipse.org/emf/2002/Ecore"
                xsi:schemaLocation="http://www.efs.org/2007/EFSmL-3-0 ../Schemas/EfsML-CorporateActions-3-0.xsd">
  <underlyers>
    <underlyer>
      <componentMethod id="cumPrice">
        <description>Cum price Trading day</description>
        <method>
          <name>CumPriceTradingDay</name>
        </method>
      </componentMethod>
    </underlyer>
  </underlyers>
  <ratio>
    <description>Take over</description>
    <methodType>Ratio</methodType>
    <takePlace>
      <componentReference href="valueSubscriptionRights" name="TP" id="takePlace"/>
      <condition>PositiveValue</condition>
    </takePlace>
    <rFactor>
      <componentFormula  id="ratioFactor" name="R">
        <description>Adjustment ratio</description>
        <formula>
          <mathExpression>(E-CS)*(h/r)/E</mathExpression>
          <componentFormula id="theoreticalValueofShares" name="E">
            <description>Theoretical value of shares</description>
            <formula>
              <mathExpression>(CS+(r*P))/h</mathExpression>
              <componentReference href="cumPrice" name="P" id="cumPrice_r1"/>
              <component id="cashSettlement" name="CS" >
                <description>Cash amount</description>
                <result>
                  <amount/>
                </result>
              </component>
              <component id="nbEntitlementShares" name="r">
                <description>New shares as the entitlement</description>
                <result>
                  <unit>0</unit>
                </result>
              </component>
              <component id="nbEligibleShares" name="h">
                <description>Shares as eligible for the entitlement</description>
                <result>
                  <unit>0</unit>
                </result>
              </component>
            </formula>
          </componentFormula>
        </formula>
      </componentFormula>
    </rFactor>
    <contract>
      <!-- Add Ratio XML -->
    </contract>
  </ratio>
</procedures>
