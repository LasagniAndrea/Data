<?xml version="1.0" encoding="UTF-8" ?>
<procedures xmlns="http://www.efs.org/2007/EFSmL-3-0"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xmlns:ecoreB="http://www.eclipse.org/emf/2002/Ecore"
                xsi:schemaLocation="http://www.efs.org/2007/EFSmL-3-0 ../Schemas/EfsML-CorporateActions-3-0.xsd">
  <underlyers>
    <underlyer>
      <componentMethod id="cumPrice">
        <description>Cum price Trading day</description>
        <method>
          <name>CumPriceTradingDay</name>
        </method>
      </componentMethod>
    </underlyer>
  </underlyers>
  <ratio>
    <description>Tender offer</description>
    <methodType>Ratio</methodType>
    <takePlace>
      <componentFormula id="takePlace" name="takePlace">
        <description>Ratio take place</description>
        <formula>
          <mathExpression>E-P</mathExpression>
          <componentReference href="cumPrice" name="P" id="cumPrice_r2"/>
          <componentReference href="tenderOfferPrice" name="E" id="tenderOfferPrice_r1"/>
        </formula>
      </componentFormula>
      <condition>PositiveValue</condition>
    </takePlace>
    <rFactor>
      <componentFormula id="ratioFactor" name="R">
        <description>Adjustment ratio</description>
        <formula>
          <mathExpression>((h*P)-(r*E))*(h/(h-r))/(h*P)</mathExpression>
          <component id="nbMaxSharesByOpe" name="r">
            <description>Maximum buy back shares by operation</description>
            <result>
              <unit>0</unit>
            </result>
          </component>
          <componentReference href="cumPrice" name="P" id="cumPrice_r1"/>
          <component id="nbOutstandingShares" name="h">
            <description>Number of outstanding shares</description>
            <result>
              <unit>0</unit>
            </result>
          </component>
          <component id="tenderOfferPrice" name="E">
            <description>Tender offer price</description>
            <result>
              <amount/>
            </result>
          </component>
        </formula>
      </componentFormula>
    </rFactor>
    <contract>
      <!-- Add Ratio XML -->
    </contract>
  </ratio>
</procedures>
