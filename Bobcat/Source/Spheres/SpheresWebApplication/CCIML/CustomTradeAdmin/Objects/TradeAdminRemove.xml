<?xml version="1.0" encoding="utf-8" ?>
<Objects>
	<Object name="TradeAdminRemoveBlock">
		<TradeAdminRemove clientid="tradeAdminRemove" />
	</Object>

	<!-- TradeAdminRemove -->
	<Object name="TradeAdminRemove" context="remove">
		<table style="istableH:true;tblH-bkgH-color:#C00303;tblH-titleR:TradeAdminRemove;">
			<table>
				<TradeAdminRemoveCharacteristics />
			</table>
			<space/>
			<!--<table>
				<TradeAdminRemoveWarnings clientid="warnings" />
			</table>-->
		</table>
	</Object>

	<!-- TradeAdminRemoveCharacteristics -->
	<Object name="TradeAdminRemoveCharacteristics">
		<table>
			<Text_RO_L clientid="identifier" caption="Identifier" required="false"/>
			<br/>
			<Text_RO_L clientid="displayName" required="false"/>
			<br/>
			<Text_RO_L clientid="description" required="false" />
			<br/>
			<Date_L clientid="date" required="true" postback="false" />
			<br/>
			<TradeAdminRemoveComments clientid="comments" required="false" postback="false"/>
		</table>
		<space/>
		<table>
			<TradeAdminRemoveWarnings postback="false" />
		</table>
	</Object>

	<!-- TradeAdminRemoveComments -->
	<Object name="TradeAdminRemoveComments">
		<Label/>
		<TextBox textMode="MultiLine" datatype="string" required="false" style="width:100%;height:50px;" />
	</Object>
	<Object name="TradeAdminRemoveWarnings">
		<table>
			<table style="istableH:true;tblH-titleR:Warning;tblH-css:size5;height:100%">
				<TextBox textMode="MultiLine" clientid="warning" datatype="string" style="background-color:Transparent;width:100%;height:50px;" misc="IsReadOnly:true"/>
				<br/>
				<hr style="margin-top:5px;margin-bottom:5px;" />
				<table>
					<Text_RO_L clientid="lstInvoice" required="false"/>
					<br/>
					<!--<Text_RO_L clientid="lstAdditionalInvoice" required="false"/>
					<br/>-->
					<Text_RO_L clientid="lstCreditNote" required="false"/>
					<br/>
					<Text_RO_L clientid="lstInvoiceSettlement" required="false" />
				</table>
			</table>
		</table>
	</Object>

	<!--Text_RO-->
	<Object name="Text_RO_L">
		<Label/>
		<TextBox datatype="string" style="width:100%;" misc="IsReadOnly:true"/>
	</Object>
	<!--Text200_RO-->
	<Object name="Text200_RO">
		<TextBox datatype="string" style="width:200px;" misc="IsReadOnly:true"/>
	</Object>
	<!--Text200_RO_L-->
	<Object name="Text200_RO_L">
		<Label/>
		<Text200_RO/>
	</Object>
	<!--Text250_RO-->
	<Object name="Text250_RO">
		<TextBox datatype="string" style="width:250px;" misc="IsReadOnly:true"/>
	</Object>
	<!--Comment-->
	<Object name="Comment_L">
		<Label/>
		<TextBox textMode="MultiLine" datatype="string" style="width:100%;height:50px;" />
	</Object>
	<!--Check-->
	<Object name="Check">
		<Label />
		<HtmlCheckBox style="width:60px;" datatype="bool" />
	</Object>


</Objects>


