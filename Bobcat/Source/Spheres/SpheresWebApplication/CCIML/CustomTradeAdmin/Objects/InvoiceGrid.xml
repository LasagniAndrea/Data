<?xml version="1.0" encoding="utf-8" ?>
<Objects>
  <!-- GridInvoiceDetailsBlock -->
  <Object name="GridInvoiceDetailsBlock" >
    <Table colspan="-1">
      <Table style="istableH:true;tblH-titleF:DisplayKey_InvoiceFee;" >
        <Table id="tblInvoiceFee">
          <Table overflow="100;1630" style="class:tableH;">
            <GridInvoiceFeeHeader clientid="invoiceTrade_feeHeader" helpschema="efsml" helpelement="InvoiceTrade"/>
            <GridInvoiceTrade clientid="invoiceTrade" occurs="1" maxoccurs="sessiontools" />
            <br/>
          </Table>
        </Table>
      </Table>
    </Table>
  </Object>

  <!-- GridInvoiceFeeHeader -->
  <Object name="GridInvoiceFeeHeader">
    <rowheader cssclass="tableH">
      <cellheader clientid="otcmlId" colspan="2" />
      <cellheader clientid="feeDate" helpschema="efsml" helpelement="InvoiceFee"/>
      <cellheader clientid="feeType" helpschema="efsml" helpelement="InvoiceFee"/>
      <cellheader clientid="feeAmount" colspan="4" helpschema="efsml" helpelement="InvoiceFee"/>
      <cellheader clientid="feeBaseAmount" colspan="4" helpschema="efsml" helpelement="InvoiceFee"/>
      <cellheader clientid="feeInitialAmount" colspan="4" helpschema="efsml" helpelement="InvoiceFee"/>
      <cellheader clientid="feeScheduleIdentifier" helpschema="efsml" helpelement="InvoiceFeeSchedule"/>
      <cellheader clientid="formulaDCF" helpschema="efsml" helpelement="InvoiceFeeSchedule"/>
      <cellheader clientid="assessmentBasisValue1" helpschema="efsml" helpelement="InvoiceFeeSchedule"/>
      <cellheader clientid="trade_otcmlId" colspan="2"/>
      <cellheader clientid="trade_identifier"/>
      <cellheader clientid="instrument_identifier" />
      <cellheader clientid="trade_side" />
      <cellheader clientid="trade_counterparty" />
      <cellheader clientid="tradeDate"/>
      <cellheader clientid="trade_inDate"/>
      <cellheader clientid="trade_outDate"/>
      <cellheader clientid="trade_currency"/>
    </rowheader>
    <br/>
  </Object>

  <!-- GridInvoiceTrade -->
  <Object name="GridInvoiceTrade">
    <GridInvoiceFeeBody clientid="invoiceFee" occurs="1" helpschema="efsml" helpelement="InvoiceFee" required="false"/>
  </Object>

  <!-- GridInvoiceFeeBody -->
  <Object name="GridInvoiceFeeBody">
    <GridInvoiceFeeInfoId clientid="otcmlId"/>
    <TextBox clientid="feeDate" datatype="date" style="width:80px" misc="IsReadOnly:true;" />
    <TextBox clientid="feeType" datatype="string" style="width:25px" misc="IsReadOnly:true;"/>
    <GridGlobalInvoiceFeeAmount clientid="feeAmount" />
    <GridGlobalInvoiceFeeAmount clientid="feeBaseAmount" />
    <GridGlobalInvoiceFeeAmount clientid="feeInitialAmount" />
    <TextBox clientid="feeScheduleIdentifier" required="false" datatype="string" misc="IsReadOnly:true;"/>
    <TextBox clientid="formulaDCF" datatype="string" required="false"  misc="IsReadOnly:true;"/>
    <TextBox clientid="assessmentBasisValue1" datatype="decimal" regex="RegexAmountExtend" required="false" style="width:120px" misc="IsReadOnly:true;"/>
    <GridInvoiceTradeInfoId clientid="trade_otcmlId" />
    <TextBox  clientid="trade_identifier"      style="white-space:nowrap;width:100px" datatype="string" misc="IsReadOnly:true;" />
    <TextBox  clientid="instrument_identifier" style="white-space:nowrap;width:50px"  datatype="string" misc="IsReadOnly:true;" />
    <TextBox  clientid="trade_side"            style="white-space:nowrap;width:50px"  datatype="string" misc="IsReadOnly:true;" />
    <TextBox  clientid="trade_counterparty"    style="white-space:nowrap;width:100px" datatype="string" misc="IsReadOnly:true;" />
    <TextBox  clientid="tradeDate"             style="width:85px"                     datatype="date"   misc="IsReadOnly:true;" />
    <TextBox  clientid="trade_inDate"          style="width:85px"                     datatype="date"   misc="IsReadOnly:true;" />
    <TextBox  clientid="trade_outDate"         style="width:85px"                     datatype="date"   misc="IsReadOnly:true;" />
    <Currency clientid="trade_currency"        style="width:30px"                                       misc="IsReadOnly:true"/>
    <br/>
  </Object>

  <!-- GridGlobalInvoiceFeeAmount -->
  <Object name="GridGlobalInvoiceFeeAmount">
    <GridInvoiceFeeAmount required="false"/>
    <GridInvoiceFeeAmount required="false" clientid="Accounting"/>
  </Object>

  <!-- GridInvoiceFeeAmount -->
  <Object name="GridInvoiceFeeAmount">
    <TextBox clientid="amount" datatype="decimal" regex="RegexAmountExtend" style="width:90px" misc="IsReadOnly:true" />
    <DropDown clientid="currency" datatype="string" width="30px" listretrieval="predef:CURRENCY" misc="IsReadOnly:true"/>
  </Object>

  <Object name="GridInvoiceFeeInfoId">
    <TextBox datatype="integer"  regex="RegexInteger" style="width:70px" misc="IsReadOnly:true" />
    <Button type="Submit" menu="OTC_INP_OTHERS_EVT" cssclass="fa-icon" imageurl="fas fa-external-link-square-alt" tooltip="ViewEvent" />
  </Object>

  <Object name="GridInvoiceTradeInfoId">
    <TextBox datatype="integer"  regex="RegexInteger" style="width:80px" misc="IsReadOnly:true" />
    <Button type="Submit" menu="OTC_INP_TRD" cssclass="fa-icon" imageurl="fas fa-external-link-square-alt" tooltip="ViewTrade"/>
  </Object>
</Objects>