<?xml version="1.0" encoding="utf-8" ?>
<Objects>
	<!-- START : BLOCKS INVOICE TRADES -->

	<!-- InvoiceTrade -->
  <Object name="InvoiceTrade">
    <table colspan="-1">
      <table id="tblInvoiceTrade_H" style="istableH:true;tblH-titleF:DisplayKey_InvoiceTrade;tblH-css:size5;tblH-reverse:1;tblH-startdisplay:collapse;">
        <table id="tblInvoiceTrade" verticalAlign="Top">
          <table verticalAlign="top" colspan="-1" controlalignleft="true">
            <InvoiceTradeInfoGen clientid="trade"/>
            <br/>
            <InvoiceTradeInstrument clientid="instrument"/>
          </table>
          <table verticalAlign="top" colspan="-1" controlalignleft="true">
            <hr style="margin-top:5px;margin-bottom:5px;" />
          </table>
          <table verticalAlign="top" colspan="-1" controlalignleft="true">
            <table verticalAlign="top" xcontrolalignleft="true">
              <InvoiceTradeInfoTradeDate clientid="tradeDate"/>
              <!--<InvoiceFees/>-->
              <br/>
              <Date_RO_L clientid="trade_inDate"  misc="IsReadOnly:true"/>
              <br/>
              <Date_RO_L clientid="trade_outDate" misc="IsReadOnly:true"/>
              <br/>
              <Currency_L clientid="trade_currency" misc="IsReadOnly:true"/>
              <Fill/>
            </table>
            <table verticalAlign="top">
              <InvoiceFees/>              
            </table>
          </table>
          <!--<table verticalAlign="top">-->
            <!--<table id="tblInvoiceFeeH" style="istableH:true;tblH-title:InvoiceFees;tblH-css:size5;tblH-reverse:1;tblH-startdisplay:collapse;">-->
              <!--<table id="tblInvoiceFee" verticalAlign="Top">-->
                <!--<table overflow="80" style="class:tableH;">-->
                  <!--<InvoiceFeeHeader clientid="feeHeader" />
                  <InvoiceFeeBody   clientid="invoiceFee" occurs="1" helpschema="efsml" helpelement="InvoiceFee"/>-->
                  <!--<br/>-->
                <!--</table>-->
              <!--</table>-->
            <!--</table>-->
          <!--</table>-->
        </table>
      </table>
    </table>
  </Object>
  <!-- InvoiceTradeInfoTradeDate -->
	<Object name="InvoiceTradeInfoTradeDate">
		<Label style="width:80px"/>
		<Date_RO misc="IsReadOnly:true"/>
	</Object>
	<!-- InvoiceTradeInfoGen -->
	<Object name="InvoiceTradeInfoGen">
		<InvoiceTradeInfo />
		<InvoiceTradeInfoId clientid="otcmlId"/>
	</Object>
	<!-- InvoiceTradeInstrument -->
	<Object name="InvoiceTradeInstrument">
		<InvoiceTradeInfo />
		<InvoiceTradeInstrumentId clientid="otcmlId"/>
	</Object>
	<!-- InvoiceTradeInfo -->
	<Object name="InvoiceTradeInfo">
		<Label style="width:80px"/>
		<TextBox clientid="identifier"  datatype="string" misc="IsReadOnly:true" />
		<TextBox clientid="displayName" style="width:250px" datatype="string" misc="IsReadOnly:true" />
	</Object>
	<!-- InvoiceTradeInfoId -->
	<Object name="InvoiceTradeInfoId">
		<Label/>
		<TextBox style="width:80px" datatype="string"  misc="IsReadOnly:true" />
		<Button type="Submit" menu="OTC_INP_TRD" cssclass="fa-icon" imageurl="fas fa-external-link-square-alt" tooltip="ViewTrade" />
		
	</Object>
	<!-- InvoiceTradeInstrumentId -->
	<Object name="InvoiceTradeInstrumentId">
		<Label/>
		<TextBox style="width:80px" datatype="string"  misc="IsReadOnly:true" />
	</Object>
	<!-- InvoiceTradeInfoDetail -->
	<Object name="InvoiceTradeInfoNotional">
		<Label clientid="amount" helpschema="fpml" helpelement="Money" />
		<Table>
			<TextBox clientid="amount" datatype="decimal" regex="RegexAmountExtend" misc="IsReadOnly:true"/>
			<Space />
			<DropDown datatype="string" width="60px" listretrieval="predef:CURRENCY" misc="IsReadOnly:true"/>
			<Fill/>
		</Table>
		<br/>
	</Object>
	<!-- END : BLOCKS INVOICE TRADES -->
</Objects>