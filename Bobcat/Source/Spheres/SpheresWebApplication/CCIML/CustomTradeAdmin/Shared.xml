<?xml version="1.0" encoding="utf-8" ?>
<Objects>
	<!--Amount-->
	<Object name="Amount">
		<TextBox datatype="decimal" regex="RegexAmountExtend" width="150px" />
	</Object>
	<!--Amount_L-->
	<Object name="Amount_L">
		<Label />
		<Amount />
	</Object>
	<!--AssetFxRate_L-->
	<Object name="AssetFxRate_L" helpelement="none">
		<Label />
		<Table style="width:10px;">
			<TextBox datatype="string" width="180px" />
			<Button type="Submit" referential="ASSET_FXRATE" cssclass="fa-icon"  tooltip="search"/>
			<xFill/>
		</Table>
	</Object>
	<!--BracketValue-->
	<Object name="BracketValue">
		<TextBox datatype="decimal" regex="RegexAmountExtend" width="150px" />
	</Object>
	<!--BracketValue_L-->
	<Object name="BracketValue">
		<Label />
	</Object>
	<!--BusinessCenterTime-->
	<Object name="BusinessCenterTime" helpschema="fpml" helpelement="BusinessCenterTime">
		<Label />
		<Table>
			<Time clientid="hourMinuteTime" postback="false" />
			<BusinessCenter_L clientid="businessCenter" postback="false" />
		</Table>
	</Object>
	<!--Check-->
	<Object name="Check">
		<Table>
			<HtmlCheckBox datatype="bool" />
			<Filler />
		</Table>
	</Object>
	<!--Currency-->
	<Object name="Currency">
		<DropDown datatype="string" width="60px" listretrieval="predef:CURRENCY" />
	</Object>
	<Object name="Currency_RO">
		<DropDown datatype="string" width="60px" listretrieval="predef:CURRENCY" misc="IsReadOnly:true"/>
	</Object>
	<!--Currency_L-->
	<Object name="Currency_L">
		<Label/>
		<Currency_RO />
	</Object>
	<Object name="CurrencyRO_L">
		<Label/>
		<Currency />
	</Object>
	<!--Date-->
	<Object name="Date">
		<Table>
			<TextBox datatype="date" width="90px" />
		</Table>
	</Object>
	<!--Date_L-->
	<Object name="Date_L">
		<Label />
		<Date />
	</Object>
	<!--DateRelativeTo-->
	<Object name="DateRelativeTo">
		<DropDown width="100px" datatype="string" relativeto="DateRelativeToReference" />
	</Object>
	<!--DateRelativeTo_L-->
	<Object name="DateRelativeTo_L">
		<Label />
		<DateRelativeTo />
	</Object>

	<!--Date_RO-->
	<Object name="Date_RO">
		<TextBox datatype="date" width="90px" misc="IsReadOnly:true"/>
	</Object>
	<!--Date_RO_L-->
	<Object name="Date_RO_L">
		<Label width="50px"/>
		<Date_RO />
	</Object>

	<!-- Money -->		 
	<Object name="Money" helpschema="fpml" helpelement="Money">
		<Table>
			<Amount clientid="amount" required="true" />
			<Space />
			<Currency clientid="currency" required="true" />
		</Table>
	</Object>
	<!-- Money_L -->
	<Object name="Money_L" helpschema="fpml" helpelement="Money">
		<Label clientid="amount" helpschema="fpml" helpelement="Money" />
		<Money />
	</Object>

	<!-- MoneyRO_L -->
	<Object name="MoneyRO_L" helpschema="fpml" helpelement="Money" misc="IsReadOnly:true">
		<Label clientid="amount" helpschema="fpml" helpelement="Money" />
		<Table>
			<TextBox clientid="amount" datatype="decimal" regex="RegexAmountExtend" width="150px" misc="IsReadOnly:true"/>
			<Space />
			<DropDown clientid="currency" datatype="string" width="60px" listretrieval="predef:CURRENCY" misc="IsReadOnly:true"/>
		</Table>
	</Object>


	<!--AdjustableDate -->
	<Object name="AdjustableDate" helpschema="fpml" helpelement="AdjustableDate">
		<Date_L clientid="unadjustedDate" ressource="Date" required="true" helpschema="fpml" helpelement="AdjustableDate" />
		<BDA_BC clientid="dateAdjustments" required="true" />
	</Object>

	<!--BusinessDayAdjustments -->
	<Object name="BDA_BC" helpschema="fpml" helpelement="BusinessDayAdjustments">
		<BDC_BC_L clientid="bDC" helpschema="fpml" helpelement="BusinessDayAdjustments" />
	</Object>

	<!--RelativeDate -->
	<Object name="RelativeDate" helpschema="fpml" helpelement="RelativeDate">
		<table style="width:10px;">
			<DateRelativeTo_L clientid="dateRelativeTo" required="true" postback="false" helpschema="fpml" helpelement="DateRelativeTo" />
			<Offset_L />
			<BDC_BC_L clientid="bDC" required="true" postback="true" helpschema="fpml"  helpelement="BusinessDayConventionEnum" />
		</table>
	</Object>

	<!--RelativeDateOffset -->
	<Object name="RelativeDateOffset_L" helpschema="fpml" helpelement="RelativeDateOffset">
		<DateRelativeTo_L clientid="dateRelativeTo" required="true" postback="false" helpschema="fpml" helpelement="DateRelativeTo" />
		<br/>
		<Offset_L />
		<br/>
		<BDC_BC_L clientid="bDC" required="true" postback="true" helpschema="fpml"  helpelement="BusinessDayConventionEnum" />
	</Object>

	<!--PaymentDate -->
	<Object name="PaymentDate" helpschema="fpml" helpelement="AdjustableOrRelativeAndAdjustedDate">
		<Date_L clientid="adjustedDate" required="true" helpschema="fpml" helpelement="IdentifiedDate" />
		<br/>
		<RelativeDate clientid="relativeDate" required="true" postback="false" helpschema="fpml" helpelement="RelativeDateOffset" />
	</Object>

	<!--PaymentDate -->
	<Object name="PaymentDate2" helpschema="fpml" helpelement="AdjustableOrRelativeAndAdjustedDate">
		<Date_L clientid="adjustedDate" required="true" helpschema="fpml" helpelement="IdentifiedDate" />
		<br/>
		<RelativeDateOffset_L clientid="relativeDate" required="true" postback="false" helpschema="fpml" helpelement="RelativeDateOffset" />
	</Object>


	<!-- Offset -->
	<Object name="Offset" helpschema="fpml" helpelement="Offset">
		<Table style="width:10px;">
			<Interval/>
			<Space/>
			<DayType clientid="dayType" required="false" postback="false" helpschema="fpml" helpelement="DayTypeEnum" />
			<Fill/>
		</Table>
	</Object>
	<!-- Offset_L -->
	<Object name="Offset_L" helpschema="fpml" helpelement="Offset">
		<Label clientid="period" />
		<Offset />
	</Object>

	<!--Interval-->
	<Object name="Interval" helpschema="fpml" helpelement="Interval">
		<Table style="width:10px;">
			<PeriodMultiplier clientid="periodMultiplier" required="true" postback="false" />
			<Space />
			<Period clientid="period" required="true" postback="false" helpschema="fpml" helpelement="PeriodEnum" />
		</Table>
	</Object>

	<!--Interval_RO-->
	<Object name="Interval_RO" helpschema="fpml" helpelement="Interval">
		<Table style="width:10px;">
			<PeriodMultiplier_RO clientid="periodMultiplier" required="false" postback="false" />
			<Space />
			<Period_RO clientid="period" postback="false" required="false" helpschema="fpml" helpelement="PeriodEnum" />
		</Table>
	</Object>

	<!--PeriodMultiplier-->
	<Object name="PeriodMultiplier">
		<TextBox datatype="integer" regex="RegexInteger" width="20px" />
	</Object>
	<Object name="PeriodMultiplier_RO">
		<TextBox datatype="integer" regex="RegexInteger" width="20px"  misc="IsReadOnly:true"/>
	</Object>

	<!--PeriodMultiplier_L-->
	<Object name="PeriodMultiplier_L">
		<Label/>
		<PeriodMultiplier />
	</Object>

	


	<!--Time-->
	<Object name="Time">
		<TextBox datatype="time" regex="RegexLongTime" width="60px" />
	</Object>
	<!--Time_L-->
	<Object name="Time_L">
		<Label />
		<Time />
	</Object>

	<!--Time_RO-->
	<Object name="Time_RO">
		<TextBox datatype="time" regex="RegexLongTime" width="60px" misc="IsReadOnly:true" />
	</Object>
	<!--Time_RO_L-->
	<Object name="Time_RO_L">
		<Label />
		<Time_RO />
	</Object>

</Objects>
