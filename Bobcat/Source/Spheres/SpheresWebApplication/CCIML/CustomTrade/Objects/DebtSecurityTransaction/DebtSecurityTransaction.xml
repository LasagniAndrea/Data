	<!--DebtSecurityTransactionBlock-->
	<Object name="DebtSecurityTransactionBlock" >
		<Table style="istableH:true;tblH-titleF:DisplayKey_Instrument;tblH-css:size4;helpschema:efsml;helpelement:DebtSecurityTransaction;" colspan="-1">
			<SecurityAsset clientid="securityAsset"/>
			<table colspan="-1">
				<SecurityOrderQuantity clientid="orderQuantity"/>
				<SecurityPrice clientid="orderPrice"/>
				<SecurityCoupon clientid="orderPrice"/>
      </table>
      <SecurityGrossAmount clientid="grossAmount"/>
      <SecurityTradeType />
    </Table>
	</Object>

	
	<!--SecurityAssetBlock-->
	<Object name="SecurityAsset" helpschema="efsml" helpelement="SecurityAsset">
		<table style="margin-top:2px;" colspan="-1" >
			<table controlalignleft="true" colspan="-1">
				<SecurityAssetId_L clientid="securityId"  required="false" postback="true" helpschema="efsml" helpelement="SecurityAsset" match="true" />
			</table>
			<table controlalignleft="true" colspan="-1">
				<HtmlCheckBox clientid="isNewAsset" required="false" postback="true" datatype="bool" misc="visibleonconsult:false" />
				<Label clientid="isNewAsset" misc="visibleonconsult:false" />
				<DebtSecurityTemplate clientid="template" required="false" postback="true" enabled="false" helpschema="efsml" helpelement="SecurityAsset" />
			</table>
			<xbr/>
			<table style="istableH:true;tblH-titleR:lbl_debtSecurityDescription;tblH-reverse:1;tblH-css:size5;tblH-startdisplay:collapse;helpschema:efsml;helpelement:DebtSecurity;" colspan="-1">
				<table id="tblDebtSecurityAsset">
          <DebtSecurityAsset2/>
					<Fill/>
				</table>
			</table>
		</table>
	</Object>

  <Object name ="SecurityTradeType" helpschema="efsml" helpelement="TradeTypeReport">
    <table cellstyle="vertical-align:top;width:33%" style="istableH:true;tblH-title:Electronic Trade;tblH-css:size5;tblH-reverse:1;tblH-startdisplay:collapse">
      <table id="tblTrade" style="margin-top:2px;" controlalignleft="true" colspan="-1">
        <!-- Type, Secondary Type and SubType -->
        <FixTrdType_L clientid="TrdTyp" required="false" postback="true" helpschema="fixml" helpelement="TrdType_enum_t"/>
        <FixSecondaryTrdType_L caption="debtSecurityTransaction_TrdTyp2" clientid="TrdTyp2" required="false" postback="true" helpschema="fixml" helpelement="TrdType2_enum_t"/>
        <FixTrdSubType_L clientid="TrdSubTyp" required="false" helpschema="fixml" helpelement="TrdSubType_enum_t"/>
      </table>
    </table>
  </Object>

