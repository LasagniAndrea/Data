<?xml version="1.0" encoding="utf-8" ?>
<Objects>
	<Object name="StrategyFxOptionLegBlock" >
		<Table colspan="-1" style="istableH:true;tblH-titleF:DisplayKey_Instrument;tblH-css:size4;helpschema:efsml;helpelement:Strategy;">
			<StrategyFxOptionLegCommon clientid="fxSimpleOption" helpschema="efsml" helpelement="FxOptionLeg" />
			<StrategyFxOptionHeaderAndRow clientid="fxSimpleOption" occurs="2" addsuboperator="true" helpschema="efsml" helpelement="FxOptionLeg"/>
		</Table>
	</Object>
	<!--******************************-->
  <Object name="StrategyFxOptionLegCommon" >
    <Table colspan="-1" controlalignleft="true" >
      <Table >
        <CallPutEnum_L clientid="optionType" required="false" postback="false" helpschema="fpml" helpelement="OptionTypeEnum" />
      </Table>
      <Table controlalignleft="true"  >
        <ExerciseStyleAmericanEuropean_L clientid="exerciseStyle" required="true" postback="true" helpschema="fpml" helpelement="ExerciseStyleEnum" />
      </Table >
      <br/>
      <hr style="margin-top:5px;margin-bottom:5px;"/>
    </Table>
  </Object>
  <!--******************************-->
  <Object name="StrategyFxOptionHeaderAndRow" >
    <Table style="class:rowHeaderBlue;" gridlines="both" bordercolor="cornflowerblue" colspan="-1" >
      <StrategyFxOptionHeader />
      <br/>
      <StrategyFxOptionRow />
    </Table>
    <hr style="margin-top:5px;margin-bottom:5px;"/>
  </Object>
  <!--******************************-->
  <Object name="StrategyFxOptionRow">
    <Table style="class:rowBorderNone;" >
      <PartyReference_L caption ="strategy_Buy" clientid="buyer" postback="true" required="true" />
      <br />
      <PartyReference_L  caption ="strategy_Sell" clientid="seller" postback="1" required="true" />
    </Table>
    <Table style="class:rowBorderNone;" >
      <Table style="class:rowBorderNone;" controlalignleft="true">
        <Money_Amount_w100_L caption="strategy_Call"  clientid="callCurrencyAmount" required="true" postback="true" helpschema="fpml" helpelement="Money"/>
        <br />
        <Money_Amount_w100_L caption="strategy_Put" clientid="putCurrencyAmount" required="true" postback="true" helpschema="fpml" helpelement="Money"/>
      </Table>
      <Table style="class:rowBorderNone;" controlalignleft="true" >
        <StrategyFxOptionFxStrikePrice clientid="fxStrikePrice" required="true" postback="true" helpschema="fpml-fx" helpelement="FxStrikePrice"/>
      </Table>
    </Table>
    <Table style="class:rowBorderNone;" >
      <Table style="class:rowBorderNone;" controlalignleft="true">
        <Label clientid="expiryDateTime_expiryDate" caption="strategyFxSimpleOption_Date"/>
        <Date clientid="expiryDateTime_expiryDate" required="true" postback="false" />
      </Table>
      <Table style="class:rowBorderNone;" controlalignleft="true">
        <Label clientid="expiryDateTime_expiryTime_hourMinuteTime" caption="strategyFxSimpleOption_Time"/>
        <Time clientid="expiryDateTime_expiryTime_hourMinuteTime" postback="false" />
        <space/>
        <BusinessCenter_w70 clientid="expiryDateTime_expiryTime_businessCenter" postback="false" />
      </Table>
      <br/>
      <Table style="class:rowBorderNone;" controlalignleft="true" colspan="-1">
        <Label clientid="valueDate" caption="strategyFxSimpleOption_Setttlement"/>
        <Date clientid="valueDate" required="true" postback="false" />
      </Table>
    </Table>
    <Table style="class:rowBorderNone;" controlalignleft="true" colspan="-1">
      <StrategyFxOptionPremium clientid="fxOptionPremium" required="false" occurs="1" addsuboperator="false" helpschema="efsml" helpelement="FxOptionPremium" />
    </Table>
    <Table style="class:rowBorderNone;" controlalignleft="true" colspan="-1">
      <banner caption="Cash Settlement" level="5" helpschema="efsml" helpelement="FxCashSettlement" />
      <StrategyFxOptionFxCashSettlement clientid="cashSettlementTerms" required="false" helpschema="efsml" helpelement="FxCashSettlement" />
    </Table>
    <br/>
  </Object>
  <!--******************************-->
  <Object name="StrategyFxOptionHeader">
    <rowheader cssclass="rowHeaderBlue">
      <cellheader clientid="strategy_Header_Buyer/Seller" />
      <cellheader clientid="strategyFxSimpleOption_Header_Amounts/Strike" />
      <cellheader clientid="strategyFxSimpleOption_Header_ExpiryDate/ValueDate" />
      <cellheader clientid="strategy_Header_Premium" />
    </rowheader>
  </Object>
  <!--******************************-->
  <Object name="StrategyFxOptionPremium" helpschema="efsml" helpelement="FxOptionPremium">
    <Table colspan="-1"  controlalignleft="true">
      <!--<Table style="class:rowBorderNone;" >
        <PartyReference_L  clientid="payer" postback="true" required="true" />
        <br />
        <PartyReference_L   clientid="receiver" postback="1" required="true" />
      </Table>-->

      <Table  controlalignleft="true">
        <StrategyFxOptionPremiumQuote clientid="premiumQuote" required="false" helpschema="fpml" helpelement="PremiumQuote" postback="true"/>
      </Table>
      <br/>
      <Table controlalignleft="true">
        <Table controlalignleft="true">
          <Money_Amount_w100_L caption="strategyFxSimpleOption_Premium_Amount"  clientid="premiumAmount" required="1" postback="true" helpschema="fpml" helpelement="Money"/>
        </Table>
        <Date_L clientid="premiumSettlementDate" required="1" helpschema="fpml" helpelement="AdjustableDate" />
      </Table>
    </Table>
  </Object>
  <!--******************************-->
  <Object name="StrategyFxOptionPremiumQuote" helpschema="fpml" helpelement="PremiumQuote">
    <FixedRate_L caption="strategyFxSimpleOption_Premium_Rate" clientid="premiumValue" />
    <space/>
    <PremiumQuotedBasis clientid="premiumQuoteBasis" />
  </Object>
  <!--******************************-->
  <Object name="StrategyFxOptionFxCashSettlement" helpschema="efsml" helpelement="FxCashSettlement">
    <Table style="class:rowBorderNone;" colspan="-1" controlalignleft="true">
      <Currency_L clientid="settlementCurrency" required="true" postback="true" helpschema="efsml" helpelement="FxCashSettlement" />
      <FxFixingAssetFxRate clientid="fixing" required="true" postback="false" occurs="1" helpschema="fpml" helpelement="FxFixing" />
    </Table>
  </Object>
  <!--******************************-->
  <Object name="StrategyFxOptionFxStrikePrice" helpschema="fpml" helpelement="FxStrikePrice">
    <Table>
      <StrikeQuoteBasis  clientid="strikeQuoteBasis" postback="true" required="true" />
      <br />
      <FxRate  clientid="rate" postback="true" required="true" />
    </Table>
  </Object>





