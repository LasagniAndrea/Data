<?xml version="1.0" encoding="utf-8" ?>

<Objects>

  <Object name="RemoveAllocationBlock" context="RemoveAllocation">
    <RemoveAllocation clientid="removeAllocation" />
  </Object>

  <!-- RemoveAllocation -->
  <Object name="RemoveAllocation">
    <table style="istableH:true;tblH-titleR:TradeRemove;tblH-bkgH-color:#C00303;tblH-css:size4;">
      <table>
        <table verticalAlign ="Top" style="istableH:true;tblH-titleR:Characteristics;tblH-css:size5;tblH-reverse:1;">
          <table style="margin-top:2px;" colspan="-1" controlalignleft="true" verticalAlign ="Top">
            <RemoveAllocationCharacteristics/>
          </table>
        </table>
        <table verticalAlign ="Top" style="istableH:true;tblH-titleR:NoteEvents;tblH-css:size5;tblH-reverse:1;">
          <table controlalignleft="true" colspan="-1" verticalAlign="top">
            <TextBox clientid="note" textMode="MultiLine" required="false" datatype="string" style="width:600px;height:44px;" />
          </table>
        </table>
      </table>
    </table>
  </Object>

  <!-- RemoveAllocationCharacteristics -->
  <Object name="RemoveAllocationCharacteristics">
    <table >
      <table controlalignleft="true" colspan="-1">
        <Date_RO_L caption ="Date" clientid="date" required="false"  postback="false" />
        <br/>
        <NonNegativeIntegerQte_w100_L clientid="initialQuantity" required="false"  postback="false" />
        <br/>
        <NonNegativeIntegerQte_w100_L clientid="availableQuantity" required="false"  postback="false" />
        <space/>
        <Button clientid="unclearingmessage" imageurl="fas fa-info-circle blue" tooltiptitle="Information" tooltip="removeAllocation_unclearing_ToolTip"/>
      </table>
    </table>
  </Object>
</Objects>
