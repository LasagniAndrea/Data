<?xml version="1.0" encoding="utf-8" ?>

<Objects>

  <Object name="CorrectionOfQuantityBlock" context="PositionCancelation">
    <CorrectionOfQuantity clientid="correctionOfQuantity" />
  </Object>

  <!-- CorrectionOfQuantity -->
  <Object name="CorrectionOfQuantity">
    <table style="istableH:true;tblH-titleR:CorrectionOfQuantity;tblH-bkgH-color:#C00303;tblH-css:size4;">
      <table>
        <table verticalAlign ="Top" style="istableH:true;tblH-titleR:Characteristics;tblH-css:size5;tblH-reverse:1;">
          <table style="margin-top:2px;" colspan="-1" controlalignleft="true" verticalAlign ="Top">
            <CorrectionOfQuantityCharacteristics/>
          </table>
        </table>
        <table verticalAlign ="Top" style="istableH:true;tblH-titleR:NoteEvents;tblH-css:size5;tblH-reverse:1;">
          <table controlalignleft="true" colspan="-1" verticalAlign="top">
            <TextBox clientid="note" textMode="MultiLine" required="false" datatype="string" style="width:500px;height:44px;" />
          </table>
        </table>
      </table>
    </table>
  </Object>

  <!-- CorrectionOfQuantityCharacteristics -->
  <Object name="CorrectionOfQuantityCharacteristics">
    <table controlalignleft="true" colspan="-1">
      <table>
      <Date_RO_L caption ="Date" clientid="date" required="true"  postback="false" />
      </table>
      <br/>
      <table>
      <IntegerQte_w100_L_RO clientid="availableQuantity" required="true"  postback="false" />
      </table>
      <br/>
      <table>
        <NonNegativeIntegerQte_w100_L clientid="quantity" required="true"  postback="true"/>
      <space/>
      <Button imageurl="fas fa-info-circle blue" tooltiptitle="Information" tooltip="correctionOfQuantity_ToolTipQuantity"/>
      </table>
    </table>
    <CorrectionOfQuantityFees/>
  </Object>

  <!-- CorrectionOfQuantityFees -->
  <Object name="CorrectionOfQuantityFees" >
    <table controlalignleft="true" colspan="-1">
      <CheckMid clientid="isFeeRestitution" required="true"  postback="false"/>
      <Button clientid="isFeeRestitution" imageurl="fas fa-info-circle blue" tooltiptitle="Information" tooltip="correctionOfQuantity_isFeeRestitution_ToolTip"/>
    </table>
  </Object>
  <!-- EG 20150716 [21103] New : Safekeeping context-->
  <Object name="CorrectionOfQuantityFees" context="_Safekeeping">
    <table controlalignleft="true" colspan="-1">
      <CheckMid clientid="isFeeRestitution" required="true"  postback="false"/>
      <Button clientid="isFeeRestitution" imageurl="fas fa-info-circle blue" tooltiptitle="Information" tooltip="correctionOfQuantity_isFeeRestitution_ToolTip"/>
      <CheckMid clientid="isSafekeepingRestitution" required="true"  postback="false"/>
      <Button clientid="isSafekeepingRestitution" imageurl="fas fa-info-circle blue" tooltiptitle="Information" tooltip="correctionOfQuantity_isSafekeepingRestitution_ToolTip"/>
    </table>
  </Object>



</Objects>