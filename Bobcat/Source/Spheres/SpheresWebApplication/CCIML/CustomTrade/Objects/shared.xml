  <!--DebtSecurityTemplate-->
  <!-- 
        // ATTENTION ********************************************************************************************************************************************
        // Toute Modification de cette restiction sur ASSETENV doit être reportée sur SQL_TradeDebtSecurity.SetSQLWhere()
        // Ce n'est pas un simple Copier/Coller:
        // - remplacer le parametre dynamique %%ID_INSTRUMENT%% par le parametre @IDI
        // ******************************************************************************************************************************************************                
        // EG 20191115 [25077] RDBMS : New version of Trades tables architecture (TRADESTSYS merge to TRADE, NEW TABLE TRADEXML)-->
      -->
  <Object name="DebtSecurityTemplate">
    <Label misc="visibleonconsult:false" />
    <DropDown datatype="string" listretrieval="sql:
select t.idt as VALUE, '[' +  i.IDENTIFIER + '] ' + t.IDENTIFIER as TEXT
from dbo.TRADE t
inner join dbo.INSTRUMENT i on i.IDI = t.IDI 
inner join dbo.PRODUCT p on p.IDP = i.IDP and p.IDENTIFIER = 'debtSecurity'
inner join dbo.TRADEASSET tasset on tasset.IDT = t.IDT 
where (t.IDSTENVIRONMENT = 'template') and 
exists (
        select 1 from 
        dbo.ASSETENV ae 
        where(
                (ae.IDI = %%ID_INSTRUMENT%%) and 
                (ae.IDC is null or ae.IDC = tasset.IDC_ISSUE) and 
                (ae.IDCOUNTRY is null or ae.IDCOUNTRY= tasset.IDCOUNTRY_ISSUE) and
                (ae.IDM is null or ae.IDM = tasset.IDM) and
                (ae.IDI_ASSET is null or ae.IDI_ASSET=t.IDI)and 
                (ae.SECURITYCLASS is null or ae.SECURITYCLASS = tasset.SECURITYCLASS) and 
                (ae.CFICODE is null or tasset.CFICODE like ae.CFICODE) and 
                (ae.PRODUCTTYPECODE is null or ae.PRODUCTTYPECODE = tasset.PRODUCTTYPECODE) and  
                (ae.FIPRODUCTTYPECODE is null or ae.FIPRODUCTTYPECODE = tasset.FIPRODUCTTYPECODE)
              )
        )" width="400px" misc="visibleonconsult:false"/>
  </Object>

