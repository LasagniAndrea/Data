<?xml version="1.0" encoding="utf-8" ?>
<Objects>

  <!-- ************************* -->
  <!-- ***   BLOCS PRICING   *** -->
  <!-- ************************* -->

  <!-- PricingFxBlock -->
  <Object name="PricingFxBlock">
    <table style="class:PnlRoundedColor1;">
      <EventParentAttached clientid="event"/>
      <PricingFx clientid="event_eventDet_pricingFx" required="false"/>
    </table>
  </Object>

  <!-- PricingFxOptionBlock -->
  <Object name="PricingFxOptionBlock">
    <table style="class:PnlRoundedColor1;">
      <EventParentAttached clientid="event"/>
      <PricingFxOption clientid="event_eventDet_pricingFxOption" required="false"/>
    </table>
  </Object>

  <!-- Pricing2Block -->
  <Object name="Pricing2Block">
    <table>
      <EventParentAttached clientid="event"/>
      <br/>
      <Pricing2 clientid="event" required="false"/>
    </table>
  </Object>

  <!-- *************************** -->
  <!-- ***   COMPLEX OBJECTS   *** -->
  <!-- *************************** -->

  <!--PricingFxOption-->
  <Object name="PricingFxOption">
    <table verticalAlign="top">
      <Banner caption="Pricing" level="2" />
      <EventPricingFxOptionParameters />
      <br/>
      <EventPricingFxOptionResult />
      <br/>
      <table verticalAlign="top">
        <Banner caption="SensibilityCoef" level="3" />
        <EventPricingGreeksCall/>
        <space/>
        <EventPricingGreeksPut/>
        <space/>
        <EventPricingOtherCoefficients/>
      </table>
    </table>
  </Object>

  <!--PricingFx-->
  <Object name="PricingFx">
    <table verticalAlign="top">
      <Banner caption="Pricing" level="2" />
      <EventPricingFxForwardParameters />
      <EventPricingFxForwardResult />
    </table>
  </Object>

  <!--EventPricingFxForwardParameters-->
  <Object name="EventPricingFxForwardParameters">
    <table verticalAlign="top">
      <Banner caption="ParametersPricingEvent" level="3" />
      <table>
        <Currency_RO_L         clientid="idCOne" />
        <Currency_RO_L         clientid="idCTwo" />
        <br/>
        <DayCountFraction_RO_L clientid="dcf"    />
        <DayCountFraction_RO_L clientid="dcfTwo"    />
        <br/>
        <Integer_RO_L clientid="num" />
        <Integer_RO_L clientid="numTwo" />
        <br/>
        <Integer_RO_L clientid="den" />
        <Integer_RO_L clientid="denTwo" />
        <br/>
        <Integer_RO_L clientid="totalOfYear" />
        <Integer_RO_L clientid="totalOfYearTwo" />
        <br/>
        <Integer_RO_L clientid="totalOfDay" />
        <Integer_RO_L clientid="totalOfDayTwo" />
        <br/>
      </table>
    </table>
  </Object>

  <!--EventPricingFxOptionParameters-->
  <Object name="EventPricingFxOptionParameters">
    <table verticalAlign="top">
      <Banner caption="ParametersPricingEvent" level="3" />
      <table>
        <Currency_RO_L         clientid="idCOne" />
        <Integer_RO_L clientid="num" />
        <br/>
        <Currency_RO_L         clientid="idCTwo" />
        <Integer_RO_L clientid="den" />
        <br/>
        <FxRate100_RO_L        clientid="strike" />
        <Integer_RO_L clientid="totalOfYear" />
        <br/>
        <DayCountFraction_RO_L clientid="dcf"    />
        <Integer_RO_L clientid="totalOfDay" />
      </table>
    </table>
  </Object>

  <!--EventPricingFxForwardResult-->
  <Object name="EventPricingFxForwardResult">
    <table verticalAlign="top">
      <Banner caption="ResultsPricingEvent" level="3" />
      <table>
        <FxRate100_RO_L clientid="spotRate" />
        <br/>
        <FxRate100_RO_L clientid="interestRateOne" />
        <br/>
        <FxRate100_RO_L clientid="interestRateTwo" />
        <br/>
        <FxRate100_RO_L clientid="forwardRate" />
      </table>
    </table>
  </Object>

  <!--EventPricingFxOptionResult-->
  <Object name="EventPricingFxOptionResult">
    <table verticalAlign="top">
      <Banner caption="ResultsPricingEvent" level="3" />
      <table>
        <FxRate100_RO_L clientid="interestRateOne" />
        <FxRate100_RO_L clientid="volatility" />
        <br/>
        <FxRate100_RO_L clientid="interestRateTwo" />
        <FxRate100_RO_L clientid="exchangeRate" />
      </table>
    </table>
  </Object>

  <!--EventPricingGreeksCall-->
  <Object name="EventPricingGreeksCall">
    <table verticalAlign="top">
      <Banner caption="GreekCallEvent" level="4" />
      <table verticalAlign="top" overflow="90">
        <CoefPricing_RO_L clientid="callPrice" />
        <br/>
        <CoefPricing_RO_L clientid="callDelta" />
        <br/>
        <CoefPricing_RO_L clientid="callRhoOne" />
        <br/>
        <CoefPricing_RO_L clientid="callRhoTwo" />
        <br/>
        <CoefPricing_RO_L clientid="callTheta" />
        <br/>
        <Banner caption="OtherCallEvent" level="4" />
        <CoefPricing_RO_L clientid="callCharm" />
        <br/>
      </table>
    </table>
  </Object>

  <!--EventPricingGreeksPut-->
  <Object name="EventPricingGreeksPut">
    <table verticalAlign="top">
      <Banner caption="GreekPutEvent" level="4" />
      <table verticalAlign="top" overflow="90">
        <CoefPricing_RO_L clientid="putPrice" />
        <br/>
        <CoefPricing_RO_L clientid="putDelta" />
        <br/>
        <CoefPricing_RO_L clientid="putRhoOne" />
        <br/>
        <CoefPricing_RO_L clientid="putRhoTwo" />
        <br/>
        <CoefPricing_RO_L clientid="putTheta" />
        <br/>
        <Banner caption="OtherPutEvent" level="4" />
        <CoefPricing_RO_L clientid="putCharm" />
        <br/>
      </table>
    </table>
  </Object>

  <!--EventPricingOtherCoefficients-->
  <Object name="EventPricingOtherCoefficients">
    <table verticalAlign="top">
      <Banner caption="OthersCoefEvent" level="4" />
      <table verticalAlign="top" overflow="90">
        <CoefPricing_RO_L clientid="gamma" />
        <br/>
        <CoefPricing_RO_L clientid="vega" />
        <br/>
        <CoefPricing_RO_L clientid="color" />
        <br/>
        <CoefPricing_RO_L clientid="speed" />
        <br/>
        <CoefPricing_RO_L clientid="vanna" />
        <br/>
        <CoefPricing_RO_L clientid="volga" />
        <br/>
      </table>
    </table>
  </Object>

  <!--Pricing2-->
  <Object name="Pricing2">
    <table colspan="-1" verticalAlign="Top" style="istableH:true;tblH-title:Pricing;tblH-css:size4;" controlalignleft="true" >
      <table colspan="-1" verticalAlign="Top" overflow="220" controlalignleft="true">
        <EventPricing2Item clientid="pricingIRD" required="false" occurs="1" />
      </table>
    </table>
  </Object>

  <Object name="EventPricing2Item">
    <table>
      <TitlePricing2_RO_L clientid="flowType" />
      <Fill/>
    </table>
    <br/>
    <EventPricing2Parameters />
    <EventPricing2Result />
    <br/>
    <table colspan="2">
      <Banner caption="" level="3" />
    </table>
    <br/>
    <space/>
    <br/>
  </Object>

  <!--EventPricing2Parameters-->
  <Object name="EventPricing2Parameters">
    <table verticalAlign="top">
      <table>
        <Banner caption="ParametersPricingEvent" level="3" />
        <TextPricing2_RO_L clientid="idYieldCurveDef" />
        <br/>
        <DatePricing2_RO_L clientid="dtClosing"/>
        <br/>
        <TextPricing2_RO_L clientid="methodDisplayName" />
        <br/>
        <IntegerPricing2_RO_L clientid="totalOfDay" />
        <br/>
      </table>
    </table>
  </Object>

  <!--EventPricing2Result-->
  <Object name="EventPricing2Result">
    <table verticalAlign="top">
      <table>
        <Banner caption="ResultsPricingEvent" level="3" />
        <RatePricing2_RO_L clientid="rate" />
        <br/>
        <RatePricing2_RO_L clientid="discountFactor" />
        <br/>
        <RatePricing2_RO_L clientid="zeroCouponOne" />
        <br/>
        <RatePricing2_RO_L clientid="zeroCouponTwo" />
        <br/>
        <RatePricing2_RO_L clientid="forwardRate" />
        <br/>
        <AmountPricing2_RO_L clientid="cashFlow" />
        <br/>
        <AmountPricing2_RO_L clientid="npv" />
        <br/>
      </table>
    </table>
  </Object>


  <!-- ************************** -->
  <!-- ***   SIMPLE OBJECTS   *** -->
  <!-- ************************** -->

  <!--CoefPricing_RO-->
  <Object name="CoefPricing_RO">
    <TextBox datatype="decimal" regex="RegexDecimal" style="width:90px;" misc="IsReadOnly:true" />
  </Object>
  <!--CoefPricing_RO_L-->
  <Object name="CoefPricing_RO_L">
    <Label/>
    <CoefPricing_RO/>
  </Object>
  <!--TitlePricing2_RO-->
  <Object name="TitlePricing2_RO">
    <TextBox datatype="string" style="font-weight:bold;font-size:11pt;" misc="IsReadOnly:true" />
  </Object>
  <!--TitlePricing2_RO_L-->
  <Object name="TitlePricing2_RO_L">
    <Image source="Images/PNG/true.png" />
    <Label style="font-size:9pt;"/>
    <TitlePricing2_RO/>
  </Object>

  <!--TextPricing2_RO-->
  <Object name="TextPricing2_RO">
    <TextBox datatype="string" style="width:200px;" misc="IsReadOnly:true" />
  </Object>
  <!--TextPricing2_RO_L-->
  <Object name="TextPricing2_RO_L">
    <Label/>
    <TextPricing2_RO/>
  </Object>
  <!--AmountPricing2_RO-->
  <Object name="AmountPricing2_RO">
    <TextBox datatype="decimal" regex="RegexAmountExtend" style="width:200px;" misc="IsReadOnly:true"/>
  </Object>
  <!--AmountPricing2_RO_L-->
  <Object name="AmountPricing2_RO_L">
    <Label/>
    <AmountPricing2_RO />
  </Object>
  <!--RatePricing2_RO-->
  <Object name="RatePricing2_RO">
    <TextBox datatype="decimal" regex="RegexFxRateExtend" style="width:200px;" misc="IsReadOnly:true"/>
  </Object>
  <!--RatePricing2_RO_L-->
  <Object name="RatePricing2_RO_L">
    <Label/>
    <RatePricing2_RO/>
  </Object>
  <!--IntegerPricing2_RO-->
  <Object name="IntegerPricing2_RO">
    <TextBox datatype="integer" regex="RegexPositiveInteger" style="width:200px;"  />
  </Object>
  <!--IntegerPricing2_RO_L-->
  <Object name="IntegerPricing2_RO_L">
    <label />
    <IntegerPricing2_RO />
  </Object>
  <!--DatePricing2_RO-->
  <Object name="DatePricing2_RO">
    <TextBox datatype="date" style="width:200px;" misc="IsReadOnly:true"/>
  </Object>
  <!--DatePricing2_RO_L-->
  <Object name="DatePricing2_RO_L">
    <Label/>
    <DatePricing2_RO />
  </Object>

</Objects>
