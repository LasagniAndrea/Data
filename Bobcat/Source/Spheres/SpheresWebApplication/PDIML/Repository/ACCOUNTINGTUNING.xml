    <Image>gif\Administration.gif</Image>

    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <!-- CC 20100802 Restrict in comment (see column IDA)  -->
      <!--SQLJoin>left outer join dbo.SESSIONRESTRICT sra on sra.ID=tblmain.IDA and sra.CLASS='ACTOR' And sra.SESSIONID='%%SESSIONID%%'</SQLJoin-->
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sri on sri.ID=tblmain.IDI and sri.CLASS='INSTRUMENT' And sri.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>
        left outer join (select distinct IDGINSTR from dbo.INSTRG nsg
        inner join dbo.SESSIONRESTRICT sr on sr.ID=nsg.IDI and sr.CLASS='INSTRUMENT' And sr.SESSIONID='%%SESSIONID%%') gInstrSr on gInstrSr.IDGINSTR=tblmain.IDGINSTR
      </SQLJoin>
      <SQLWhere>
        ( ((tblmain.IDI is null) or (sri.ID is not null))
        and
        --((tblmain.IDA is null) or (sra.ID is not null)) and 
        ((tblmain.IDGINSTR is null) or (gInstrSr.IDGINSTR is not null))     )
      </SQLWhere>
    </SQLWhere>

