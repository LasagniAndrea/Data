		<SQLWhere>
			<!-- join for limited resultset to current elementtype -->
			<SQLJoin>
        left outer join dbo.VW_INVRULESINSTR vw_ti1 on (vw_ti1.TYPEINSTR=tblmain.TYPEINSTR and vw_ti1.ID=vw_ti2.ID)
        left outer join dbo.VW_INVRULESMARKET vw_tm1 on (vw_tm1.TYPEMARKETCONTRACT=tblmain.TYPEMARKETCONTRACT and vw_tm1.ID=vw_tm2.ID)
        left outer join dbo.VW_INVRULESINSTR vw_tiu1 on (vw_tiu1.TYPEINSTR_UNL=tblmain.TYPEINSTR_UNL and vw_tiu1.ID=vw_tiu2.ID)
      </SQLJoin>
			<SQLWhere>
        (vw_ti1.TYPEINSTR is not null or vw_ti2.ID is null)
        and
        (vw_tm1.TYPEMARKETCONTRACT is not null or vw_tm2.ID is null)
        and
        (vw_tiu1.TYPEINSTR_UNL is not null or vw_tiu2.ID is null)
      </SQLWhere>
		</SQLWhere>
