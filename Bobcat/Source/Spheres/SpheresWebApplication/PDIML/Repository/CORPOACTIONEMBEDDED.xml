<Referentials>
  <Referential>
    <CmptLevel>2.5</CmptLevel>
    <TableName>CORPOACTION</TableName>
    <AliasTableName>ca</AliasTableName>
    <IsUseSQLParameters>true</IsUseSQLParameters>
    <Ressource></Ressource>
    <ColumnByRow>3</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>false</Remove>
    <Notepad>false</Notepad>
    <LoadOnStart>false</LoadOnStart>
    <Timeout>200</Timeout>

    <SQLSelect>
      <Command rdbms = "all">
        <![CDATA[
        select ca.IDCA, ca.IDM, ca.IDENTIFIER, /* mk.MARKETTYPE, */ ca.DISPLAYNAME, ca.READYSTATE, ca.REFNOTICE, 
        case when ca.USEURLNOTICE = 1 then ca.URLNOTICE || ca.NOTICEFILENAME else ca.NOTICEFILENAME end as URLNOTICEFILENAME,
        ca.PUBDATE, ca.CFICODE, ce.IDCE, ce.CEGROUP, ce.CETYPE, ce.CECOMBINEDOPER, ce.CECOMBINEDTYPE, ce.ADJMETHOD, ce.EXECORDER, 
        ce.EXDATE, ce.EFFECTIVEDATE, ce.ADJPROCEDURES,
        ca.EXTLLINK, ca.ROWVERSION, ca.ROWATTRIBUT
        from dbo.CORPOEVENT ce
        inner join dbo.CORPOACTION ca on (ca.IDCA = ce.IDCA)
        inner join dbo.MARKET mk on (mk.IDM = ca.IDM) and (mk.IDM = case when -1=@IDM then mk.IDM else @IDM end)
        ]]>
      </Command>
    </SQLSelect>

    <SQLRowState >
      <![CDATA[
      case READYSTATE
      when 'DEPRECATED' then 'lstaw=name:leddeprecated'
      when 'EXPIRED'    then 'lstaw=name:ledexpired' 
      when 'UNMANAGED'  then 'lstaw=name:ledunmanaged' 
      else case when EFFECTIVEDATE is null then 'lstaw=name:ledwarning' end end      
      ]]>
    </SQLRowState>


