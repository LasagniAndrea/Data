    <!--CC 20100726 Add SessionRestrict for MARKET-->
    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT srm on srm.ID=tblmain.IDM and srm.CLASS='MARKET' and srm.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT srm2 on srm2.ID=tblmain.IDM_RELATED and srm2.CLASS='MARKET' and srm2.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT srm3 on srm3.ID=tblmain.IDM_OPTIONS and srm3.CLASS='MARKET' and srm3.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLWhere>
        (
        ((tblmain.IDM is null) or (srm.ID is not null))
        and ((tblmain.IDM_RELATED is null) or (srm2.ID is not null))
        and ((tblmain.IDM_OPTIONS is null) or (srm3.ID is not null))
        )
      </SQLWhere>
    </SQLWhere>

