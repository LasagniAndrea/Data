        <Condition>
          <TableName>ACTORROLE</TableName>
          <ColumnRelation>
            <ColumnName>IDA</ColumnName>
            <DataType>int</DataType>
          </ColumnRelation>
          <!--Pour pouvoir selectionner:
          - la chambre de compensation du marchÃ©, si elle existe
          - sinon n'importe quel Custodian-->
          <SQLWhere>
            (ACTORROLE.IDROLEACTOR = 'CSS' and ACTORROLE.IDA in (select mk.IDA from dbo.MARKET mk where mk.IDM=%%COLUMN_VALUE%%IDM%%))
            or
            (ACTORROLE.IDROLEACTOR = 'CUSTODIAN' and exists (select 1 from dbo.MARKET mk where mk.IDM=%%COLUMN_VALUE%%IDM%% and mk.IDA is null))
          </SQLWhere>
        </Condition>
        <AutoComplete enabled="true"/>
      </Relation>
