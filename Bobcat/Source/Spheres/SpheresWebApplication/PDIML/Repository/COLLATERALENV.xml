<Referentials>
  <Referential>
    <!-- FI 20210219 [XXXXX] Add -->
    <JQuery function="Init"/>
    <TableName>COLLATERALENV</TableName>
    <Ressource>COLLATERALENV</Ressource>
    <ColumnByRow>3</ColumnByRow>
    <Create>true</Create>
    <Modify>true</Modify>
    <Remove>true</Remove>
    <Image></Image>
    
    <!--CC 20111124 Add SessionRestrict for ACTOR -->
    <!--Un USER non admin peut accéder aux enregistrements :                                                                                  -->
    <!--sans déposant spécifié (IDPAY is null) ou avec déposant auquel il a accès                                                             -->
    <!--(Dans le cas où le déposant est un groupe d'acteurs, il suffit que le USER ait accès au moins à un des acteurs composant le groupe    -->
    <!--ET                                                                                                                                    -->
    <!--sans dépositaire spécifié (IDREC is null) ou avec dépositaire auquel il a accès                                                       -->
    <!--(Dans le cas où le dépositaire est un groupe d'acteurs, il suffit que le USER ait accès au moins à un des acteurs composant le groupe -->
    <!--ET                                                                                                                                    -->
    <!--sans chambre spécifiée (IDA_CSS is null) ou avec chambre à laquelle il a accès                                                        -->
    <!--RD 20130424 [18614] Rendre Mandatory les colonnes IDPAY et IDREC                                                                           -->
    
    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sr_pay_a on sr_pay_a.ID=tblmain.IDPAY and sr_pay_a.CLASS='ACTOR' 
      and sr_pay_a.SESSIONID='%%SESSIONID%%' and tblmain.PAYTYPEPARTY='Actor'</SQLJoin>
      <SQLJoin>
        left outer join (select distinct IDGACTOR from dbo.ACTORG pay_ag
        inner join dbo.SESSIONRESTRICT sr_pay_ga on sr_pay_ga.ID=pay_ag.IDA and sr_pay_ga.CLASS='ACTOR' 
        and sr_pay_ga.SESSIONID='%%SESSIONID%%') pay_ga
        on pay_ga.IDGACTOR=tblmain.IDPAY and tblmain.PAYTYPEPARTY='GrpActor'
      </SQLJoin>
      
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sr_rec_a on sr_rec_a.ID=tblmain.IDREC and sr_rec_a.CLASS='ACTOR' 
      and sr_rec_a.SESSIONID='%%SESSIONID%%' and tblmain.RECTYPEPARTY='Actor'</SQLJoin>
      <SQLJoin>
        left outer join (select distinct IDGACTOR from dbo.ACTORG rec_ag
        inner join dbo.SESSIONRESTRICT sr_rec_ga on sr_rec_ga.ID=rec_ag.IDA and sr_rec_ga.CLASS='ACTOR'
        and sr_rec_ga.SESSIONID='%%SESSIONID%%') rec_ga
        on rec_ga.IDGACTOR=tblmain.IDREC and tblmain.RECTYPEPARTY='GrpActor'
      </SQLJoin>

      <SQLJoin>left outer join dbo.SESSIONRESTRICT sr_css on sr_css.ID=tblmain.IDA_CSS and sr_css.CLASS='ACTOR' and sr_css.SESSIONID='%%SESSIONID%%'</SQLJoin>
      
      <SQLWhere>
        (
        (tblmain.IDPAY is null or sr_pay_a.ID is not null or pay_ga.IDGACTOR is not null)
        and
        (tblmain.IDREC is null or sr_rec_a.ID is not null or rec_ga.IDGACTOR is not null)
        and
        (tblmain.IDA_CSS is null or sr_css.ID is not null)
        )
      </SQLWhere>
    </SQLWhere>

