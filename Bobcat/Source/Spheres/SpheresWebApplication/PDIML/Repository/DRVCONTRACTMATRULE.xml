    <Column>
      <ColumnName>IDMATURITYRULE</ColumnName>
      <Ressource>IDMATURITYRULE</Ressource>
      <DataType>int</DataType>
      <Colspan>0</Colspan>
      <Length>10</Length>
      <Scale>0</Scale>
      <IsHide>false</IsHide>
      <IsHideInDataGrid>false</IsHideInDataGrid>
      <IsUpdatable isupdatableincreation="true">false</IsUpdatable>
      <Relation>
        <!--CC/PL 20131114 Ticket 19164 To eliminate MR with the same IDENTIFIER-->
        <!--RD 20191031 [25021] Don't display deactivated MR-->
        <TableName>VW_MATURITYRULE</TableName>
        <ColumnRelation>
          <ColumnName>IDMATURITYRULE</ColumnName>
          <DataType>int</DataType>
        </ColumnRelation>
        <ColumnSelect>
          <ColumnName>MATURITYRULE</ColumnName>
          <Ressource>IDMATURITYRULE_IDENTIFIER</Ressource>
          <DataType>string</DataType>
        </ColumnSelect>
        <Condition>
          <SQLWhere>
            <![CDATA[IDMATURITYRULE in (
                    select min(IDMATURITYRULE)
                    from dbo.VW_MATURITYRULE
                    where (DTENABLED <= getdate() and ((DTDISABLED is null) or (DTDISABLED > getdate())))
                    group by IDENTIFIER) 
                    and (IDENTIFIER != 'Default Rule')]]>
          </SQLWhere>
        </Condition>
      </Relation>
      <Information>
        <Type>information</Type>
        <LabelMessage>Msg_MATURITYRULEForDERIVATIVECONTRACT</LabelMessage>
