    <Notepad>false</Notepad>
    <LoadOnStart>false</LoadOnStart>
    <Timeout>200</Timeout>

    <SQLSelect>
      <Command rdbms = "all">
        <![CDATA[
        select arq.IDARQ, arq.IDENTIFIER, arq.DISPLAYNAME, arq.READYSTATE, arq.IDA_ENTITY, arq.EFFECTIVEDATE, 
        arq.REQUESTTYPE as REQUESTTYPE_VALUE, rqt.EXTVALUE as REQUESTTYPE_EXTVALUE, arq.TIMING,
        arq.MODE_C as MODE_C_VALUE, clo.EXTVALUE as MODE_C_EXTVALUE, 
        arq.MODE_O as MODE_O_VALUE, reo.EXTVALUE as MODE_O_EXTVALUE, 
        arq.EXTLLINK, arq.ROWVERSION, arq.ROWATTRIBUT
        from dbo.ACTIONREQUEST arq
        inner join dbo.ENUM rqt on (rqt.CODE = 'PosRequestTypeEnum') and (rqt.VALUE = arq.REQUESTTYPE)
        inner join dbo.ENUM clo on (clo.CODE = 'TransferModeEnum') and (clo.VALUE = arq.MODE_C)
        left outer join dbo.ENUM reo on (reo.CODE = 'TransferModeEnum') and (reo.VALUE = arq.MODE_O)
        ]]>
      </Command>
    </SQLSelect>

    <SQLRowState >
      <![CDATA[
      case READYSTATE
      when 'DEPRECATED' then 'lstaw=name:leddeprecated;'
      when 'RESERVED'    then 'lstaw=name:ledreserved;' 
      when 'TOCOMPLETE'  then 'lstaw=name:ledtocomplete;' 
      when 'REGULAR'  then 'lstaw=name:ledregular;' 
      else case when EFFECTIVEDATE is null then 'lstaw=name:ledwarning;' end end      
      ]]>
    </SQLRowState>


