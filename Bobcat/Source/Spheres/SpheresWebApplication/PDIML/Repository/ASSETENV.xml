    <Create>true</Create>
    <Modify>true</Modify>
    <Remove>true</Remove>
    <Image></Image>

    <SQLSelect>
      <Command rdbms = "all">
        <![CDATA[
        select tblmain.IDASSETENV, tblmain.IDI, tblmain.IDC, tblmain.IDCOUNTRY,
        tblmain.IDM, tblmain.QUOTEPLACE, tblmain.IDI_ASSET, tblmain.SECURITYGRP, tblmain.SECURITYCLASS, 
        tblmain.CFICODE, tblmain.PRODUCTTYPECODE, tblmain.FIPRODUCTTYPECODE,
        tblmain.DEFINITION, tblmain.DOCUMENTATION, 
        tblmain.IDAINS, tblmain.IDAUPD, tblmain.DTINS, tblmain.DTUPD, tblmain.DTENABLED, tblmain.DTDISABLED, 
        tblmain.EXTLLINK, tblmain.ROWATTRIBUT, tblmain.ROWVERSION
        from dbo.ASSETENV tblmain
        ]]>
      </Command>
    </SQLSelect>

    <!--CC 20100727 Add SessionRestrict for MARKET and INSTRUMENT -->
    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>inner join dbo.SESSIONRESTRICT sri on sri.ID=tblmain.IDI and sri.CLASS='INSTRUMENT' and sri.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT srm on srm.ID=tblmain.IDM and srm.CLASS='MARKET' and srm.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT sri2 on sri2.ID=tblmain.IDI_ASSET and sri2.CLASS='INSTRUMENT' and sri2.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLWhere>
        (
        ((tblmain.IDM is null) or (srm.ID is not null))
        and ((tblmain.IDI_ASSET is null) or (sri2.ID is not null))
        )
      </SQLWhere>
    </SQLWhere>

