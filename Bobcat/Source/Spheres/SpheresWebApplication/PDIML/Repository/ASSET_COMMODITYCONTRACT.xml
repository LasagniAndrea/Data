    <!--CC 20100726 Add SessionRestrict for MARKET -->
    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>left outer join dbo.SESSIONRESTRICT srm on srm.ID=tblmain.IDM and srm.CLASS='MARKET' and srm.SESSIONID='%%SESSIONID%%'</SQLJoin>
      <SQLWhere>
        (
        ((tblmain.IDM is null) or (srm.ID is not null))
        )
      </SQLWhere>
    </SQLWhere>

