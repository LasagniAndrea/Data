<?xml version="1.0" encoding="utf-8" ?>
<Referentials>
  <Referential>
    <CmptLevel>2.5</CmptLevel>
    <TableName>CORPOEVENTCONTRACT</TableName>
    <AliasTableName>cec</AliasTableName>
    <IsUseSQLParameters>true</IsUseSQLParameters>
    <Ressource></Ressource>
    <ColumnByRow>3</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>false</Remove>
    <Notepad>false</Notepad>
    <LoadOnStart>true</LoadOnStart>
    <Timeout>200</Timeout>

    <SQLSelect>
      <Command rdbms = "all">
        <![CDATA[
        select cec.IDCEC, cec.IDCE, cec.IDA_ENTITY, cec.IDDC, cec.CATEGORY, 
        cec.READYSTATE, cec.ADJSTATUS, cec.ADJMETHOD,
        cec.RENAMINGMETHOD,cec.RENAMINGVALUE,cec.EXRENAMINGVALUE,
        cec.RFACTOR, cec.RFACTOR_RND, cec.RFACTOR_RETAINED,
        cec.CMULTIPLIER, cec.EXCMULTIPLIER, cec.EXCMULTIPLIER_RND,
        cec.CSIZE, cec.EXCSIZE, cec.EXCSIZE_RND,
        
        cec.RFACTOR as RFACTOR_GRID, cec.RFACTOR_RND as RFACTOR_RND_GRID, cec.RFACTOR_RETAINED as RFACTOR_RETAINED_GRID,  
        cec.CMULTIPLIER as CMULTIPLIER_GRID, 
        cec.EXCMULTIPLIER as EXCMULTIPLIER_GRID, cec.EXCMULTIPLIER_RND as EXCMULTIPLIER_RND_GRID,
        cec.CSIZE as CSIZE_GRID, 
        cec.EXCSIZE as EXCSIZE_GRID, cec.EXCSIZE_RND as EXCSIZE_RND_GRID,
        
        ca.IDM, /* mk.MARKETTYPE, */ ac.IDENTIFIER as ENTITY_IDENTIFIER, ac.DISPLAYNAME as ENTITY_DISPLAYNAME, 
        ca.IDENTIFIER as CA_IDENTIFIER, ca.REFNOTICE, ca.DOCUMENTATION,
        ca.PUBDATE, ce.EFFECTIVEDATE 
        from dbo.CORPOEVENTCONTRACT cec
        inner join dbo.CORPOEVENT ce on (ce.IDCE = cec.IDCE)
        inner join dbo.CORPOACTION ca on (ca.IDCA = ce.IDCA)
        inner join dbo.MARKET mk on (mk.IDM = ca.IDM)
        %%SR:ACTOR_JOIN%%(cec.IDA_ENTITY)
        inner join dbo.ACTOR ac on (ac.IDA = cec.IDA_ENTITY)
        where (cec.IDA_ENTITY=case when -1=@ENTITY then cec.IDA_ENTITY else @ENTITY end)
        ]]>
      </Command>
    </SQLSelect>

