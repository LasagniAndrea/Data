    <!-- PM 20231129 [XXXXX][WI759] -->
    <!-- SQLJoin for VirtualColumn INITIALMARGINMETH 
         Used to display sub-menu 'Parameters for the initial margin "IMSM"'
    -->
    <SQLJoin>
      left outer join
      (
      select tblmain.IDCC, (case when im_mk.INITIALMARGINMETH is null then im_css.INITIALMARGINMETH else im_mk.INITIALMARGINMETH end) as INITIALMARGINMETH
      from COMMODITYCONTRACT tblmain
      inner join dbo.MARKET mk on mk.IDM = tblmain.IDM
      left outer join dbo.CSS css on css.IDA = mk.IDA
      left outer join dbo.IMMETHOD im_mk on (im_mk.IDIMMETHOD = mk.IDIMMETHODCOMSPOT)
      left outer join dbo.IMMETHOD im_css on (im_css.IDIMMETHOD = css.IDIMMETHODCOMSPOT)
      ) immeth on immeth.IDCC = tblmain.IDCC
    </SQLJoin>
    
