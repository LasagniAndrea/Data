						<DataType>string</DataType>
					</ColumnRelation>
					<SQLWhere>
            (
            (ra2.IDROLEACTOR != 'SYSADMIN' or '%%ROLE_USER%%'='SYSADMIN')
            and
            (ra2.IDROLEACTOR != 'SYSOPER' or '%%ROLE_USER%%' in ('SYSADMIN','SYSOPER'))
