<Referentials>
  <Referential>
    <TableName>POSREQUEST</TableName>
    <AliasTableName>pr</AliasTableName>
    <ColumnByRow>4</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>false</Remove>
    <AttachedDoc>false</AttachedDoc>
    <SQLSelect>
      <Command rdbms = "all">
        <![CDATA[
          select im.IDIMREQUEST, im.STATUS as STATUSRETURN, 
          im.IDA_ENTITY, im.IDA_CSS, im.IDA_MRO, im.IDB_MRO, im.IDA_MRO_IMREQUEST, 
          im.DTBUSINESS, 
          im.IDT, im.DTSTART, im.DTEND, im.IDAINS, im.DTINS as DTINS, isnull(im.DTUPD,im.DTINS) as DTUPD, 
          im.EXTLLINK, im.ROWATTRIBUT, im.ROWVERSION,
          pr.IDPR, pr.REQUESTTYPE, en.DOCUMENTATION, 
          a_ETY.IDENTIFIER as ETY_IDENTIFIER,
          a_CSS.IDENTIFIER as CSS_IDENTIFIER,
          a_MRO.IDENTIFIER as MRO_IDENTIFIER, a_MRO.DISPLAYNAME as MRO_DISPLAYNAME,
          b_MRO.IDENTIFIER as MRO_BOOKIDENTIFIER, b_MRO.DISPLAYNAME as MRO_BOOKDISPLAYNAME,
          a_MRO_PARENT.IDENTIFIER as MROPARENT_IDENTIFIER, a_MRO_PARENT.DISPLAYNAME as MROPARENT_DISPLAYNAME,
          ev.VALORISATION as MARGINREQAMOUNT, ev.UNIT as MARGINREQCURRENCY, 'RISK' as GPRODUCT,
          pr.REQUESTTYPE || '-' || isnull(im.STATUS,'NONE') || '-0' as REQUESTTYPE_DISPLAY
          from dbo.POSREQUEST pr
          inner join dbo.IMREQUEST im on (im.IDPR = isnull(pr.IDPR_POSREQUEST,pr.IDPR)) and (pr.REQUESTTYPE in ('EOD','EOD_INITIALMARGIN'))
          inner join dbo.ACTOR a_ETY on (a_ETY.IDA=im.IDA_ENTITY)
          inner join dbo.ACTOR a_CSS on (a_CSS.IDA=im.IDA_CSS)
          left outer join dbo.ACTOR a_MRO on (a_MRO.IDA=im.IDA_MRO)
          left outer join dbo.BOOK b_MRO on (b_MRO.IDB=im.IDB_MRO)
          inner join dbo.ENUM en on (en.CODE = 'PosRequestTypeEnum') and (en.VALUE = pr.REQUESTTYPE)
          left outer join dbo.TRADE tr on (tr.IDT = im.IDT)
          left outer join dbo.ACTOR a_MRO_PARENT on (a_MRO_PARENT.IDA=im.IDA_MRO_IMREQUEST)
          left outer join dbo.EVENT ev on (ev.IDT = im.IDT) and (ev.EVENTTYPE = 'MGR')
      ]]>
      </Command>
    </SQLSelect>

    <SQLRowStyle type="class">
      case STATUSRETURN
      when 'SUCCESS' then 'DataGrid-SUCCESS_2'
      when 'ERROR' then 'DataGrid-ERROR'
      else 'DataGrid_ItemStyle'
      end
    </SQLRowStyle>

