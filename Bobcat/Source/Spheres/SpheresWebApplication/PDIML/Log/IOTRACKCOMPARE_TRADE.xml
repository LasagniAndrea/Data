    <SQLSelect>

      <Command rdbms = "oracle">
        <![CDATA[
        select
        pl.DTSTPROCESS as DTSTPROCESS_L, pl.PROCESS as PROCESS_L, pl.IDSTPROCESS as IDSTPROCESS_L, pl.IDDATA as IDDATAPROCESS_L, pl.IDA, pl.HOSTNAME, pl.APPNAME, pl.APPVERSION,
        iot.IDIOTASKDET,iot.DATASOURCEIDENT,
        iotc.IDIOTRACK, iotc.MATCHSTATUS,
        iotc.MESSAGE,
        case when iot.DATA3 like '%TXNTM;%' then null 
        else t.DTEXECUTION end 
        as DTTRANSACTION,
        posetd.ISO10383_ALPHA4 as MARKET,
        asset.CONTRACTSYMBOL as CONTRACT,
        case 
          when asset.PUTCALL = 0
          then 'Put'
          when asset.PUTCALL = 1
          then 'Call'
          else null
        end as PUTCALL,
        asset.STRIKEPRICE as STRIKE,
        case 
          when iot.DATA3 like '%LASTPX;%' then null 
          else posetd.PRICE
        end as PRICE,
        asset.MATURITYMONTHYEAR as MATURITY,
        case 
          when iot.DATA3 like '%ACCT;%' then null 
          else b.IDENTIFIER
        end as ACCOUNT,
        case 
          when iot.DATA3 like '%ACCTTYP;%' then null
          when ar_clearer.IDA is not null then 'Clearer'
          when ar_client.IDA is not null and b.ISPOSKEEPING = 1 then 'Client'
          when b.ISPOSKEEPING = 1 then 'House'					
          else 'Give-up'
        end as ACCOUNTTYPE,
        case 
          when iot.DATA3 like '%POSEFCT;%' then null
          else case 
                when posetd.POSITIONEFFECT = 'O' then 'Open'
                when posetd.POSITIONEFFECT = 'C' then 'Close'
                else posetd.POSITIONEFFECT
               end
        end as POSITIONEFFECT,
        iotc.INTDATAID, iotc.INTDATAIDS,
        iotc.EXTDATAID,null as DATAROWNUMBER,
        iotc.INTVALUE1 as INTLONGQTY,
        iotc.INTVALUE2 as INTSHORTQTY,
        iotc.EXTVALUE1 as EXTLONGQTY,
        iotc.EXTVALUE2 as EXTSHORTQTY,
        iotc.MATCHVALUE1 as MATCHLONGQTY,
        iotc.MATCHVALUE2 as MATCHSHORTQTY,
        iotc.DTINS, iotc.IDAINS, iotc.ROWVERSION,
        t.IDSTACTIVATION
        from dbo.IOTRACKCOMPARE iotc
        inner join dbo.TRADE t on (iotc.IDIOTRACK = %%FOREIGN_KEY%%) and (t.IDT = iotc.INTDATAID)
        inner join dbo.VW_TRADE_POSETD posetd on (posetd.IDT = t.IDT)
        inner join dbo.VW_ASSET_ETD_EXPANDED asset on (asset.IDASSET = posetd.IDASSET)	
        inner join dbo.BOOK b on (b.IDB = posetd.IDB_DEALER)
        inner join dbo.ACTOR a on (a.IDA = posetd.IDA_DEALER)
        left outer join dbo.ACTORROLE ar_client on (ar_client.IDA = a.IDA) and (ar_client.IDROLEACTOR = 'CLIENT')
        left outer join 
        (
          select distinct IDA 
          from dbo.ACTORROLE
          where IDROLEACTOR in ('CLEARER','CCLEARINGCOMPART','HCLEARINGCOMPART','MCLEARINGCOMPART')
        ) ar_clearer on (ar_clearer.IDA = a.IDA)        
        inner join dbo.IOTRACK iot on (iot.IDIOTRACK = iotc.IDIOTRACK) and (iot.IDIOTRACK = %%FOREIGN_KEY%%)
        inner join dbo.PROCESS_L pl on (pl.IDPROCESS_L = iot.IDPROCESS_L)
        where (iotc.INTVALUE1 is not null or iotc.INTVALUE2 is not null)

        union all

        --To obtain unmatched lines on Trades with internal data (table EXTLDATADET)
        select
        pl.DTSTPROCESS as DTSTPROCESS_L, pl.PROCESS as PROCESS_L, pl.IDSTPROCESS as IDSTPROCESS_L, pl.IDDATA as IDDATAPROCESS_L, pl.IDA, pl.HOSTNAME, pl.APPNAME, pl.APPVERSION,
        iot.IDIOTASKDET,iot.DATASOURCEIDENT,
        iotc.IDIOTRACK, iotc.MATCHSTATUS,
        iotc.MESSAGE,
        case 
          when iot.DATA3 like '%TXNTM;%' then null 
          else TO_DATE(extractValue(edd.DATAXML, '/fixml:TrdCaptRpt/@TxnTm', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'),'YYYY-MM-DD"T"HH24:MI:SS') 
        end as DTTRANSACTION,
        extractValue(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@Exch', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') as MARKET,
        extractValue(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@Sym', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') as CONTRACT,
        case 
          when existsNode(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt[@PutCall="0"]', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') =1
            then 'Put'
          when existsNode(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt[@PutCall="1"]', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') =1
            then 'Call'
          else null
        end as PUTCALL,
        to_number(extractValue(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@StrkPx', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'),'99999.9999') as STRIKE,
        case 
          when iot.DATA3 like '%LASTPX;%' then null 
          else to_number(extractValue(edd.DATAXML, '/fixml:TrdCaptRpt/@LastPx', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'),'999999.999999') 
        end as PRICE,
        case 
          when length(extractValue(DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"')) = 6
            then substr(extractValue(DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'), 1,4)
        || '-' || substr(extractValue(DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'), 5,2)
          when length(extractValue(DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"')) = 8
            then substr(extractValue(DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'), 1,4)
        || '-' || substr(extractValue(DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'), 5,2)
        || '-' || substr(extractValue(DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'), 7,2)
          else extractValue(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"')
        end as MATURITY,
        case 
          when iot.DATA3 like '%ACCT;%' then null 
          else extractValue(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:RptSide/@Acct', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') 
        end as ACCOUNT,         
        case 
          when iot.DATA3 like '%ACCTTYP;%' then null
          when ar_clearer.IDA is not null then 'Clearer'
          when ar_client.IDA is not null and b.ISPOSKEEPING = 1 then 'Client'
          when b.ISPOSKEEPING = 1 then 'House'					
          when b.IDB is not null then 'Give-up'
          else 'n/a'  
        end as ACCOUNTTYPE,
        case 
          when iot.DATA3 like '%POSEFCT;%' then null
          else case 
                when existsNode(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:RptSide[@PosEfct="O"]', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') =1
                  then 'Open'
                when existsNode(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:RptSide[@PosEfct="C"]', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') =1
                  then 'Close'
                else extractValue(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:RptSide/@PosEfct', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') || ' [Unknown]'
              end
        end as POSITIONEFFECT,
        iotc.INTDATAID, iotc.INTDATAIDS,
        iotc.EXTDATAID, edd.DATAROWNUMBER,
        iotc.INTVALUE1 as INTLONGQTY,
        iotc.INTVALUE2 as INTSHORTQTY,
        iotc.EXTVALUE1 as EXTLONGQTY,
        iotc.EXTVALUE2 as EXTSHORTQTY,
        iotc.MATCHVALUE1 as MATCHLONGQTY,
        iotc.MATCHVALUE2 as MATCHSHORTQTY,
        iotc.DTINS, iotc.IDAINS, iotc.ROWVERSION,
        null as IDSTACTIVATION
        from dbo.IOTRACKCOMPARE iotc
        left outer join dbo.EXTLDATADET edd on (edd.IDEXTLDATADET = iotc.EXTDATAID)
        inner join dbo.IOTRACK iot on (iot.IDIOTRACK = iotc.IDIOTRACK) and (iot.IDIOTRACK = %%FOREIGN_KEY%%)
        inner join dbo.PROCESS_L pl on (pl.IDPROCESS_L = iot.IDPROCESS_L)
        left outer join dbo.BOOK b on (b.IDENTIFIER = extractValue(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:RptSide/@Acct', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'))
        left outer join dbo.ACTOR a on (a.IDA = b.IDA)
        left outer join dbo.ACTORROLE ar_client on (ar_client.IDA = a.IDA) and (ar_client.IDROLEACTOR = 'CLIENT')
        left outer join 
        (
          select distinct IDA 
          from dbo.ACTORROLE
          where IDROLEACTOR in ('CLEARER','CCLEARINGCOMPART','HCLEARINGCOMPART','MCLEARINGCOMPART')
        ) ar_clearer on (ar_clearer.IDA = a.IDA)
        where (iotc.EXTVALUE1 is not null or iotc.EXTVALUE2 is not null) and (iotc.INTVALUE1 is null and iotc.INTVALUE2 is null)
        and existsNode(edd.DATAXML, '/fixml:TrdCaptRpt', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"')=1

        union all

        --To obtain unmatched lines on Positions with internal data (table EXTLDATADET)
        select
        pl.DTSTPROCESS as DTSTPROCESS_L, pl.PROCESS as PROCESS_L, pl.IDSTPROCESS as IDSTPROCESS_L, pl.IDDATA as IDDATAPROCESS_L, pl.IDA, pl.HOSTNAME, pl.APPNAME, pl.APPVERSION,
        iot.IDIOTASKDET,iot.DATASOURCEIDENT,
        iotc.IDIOTRACK, iotc.MATCHSTATUS,
        iotc.MESSAGE,
        null as DTTRANSACTION,
        extractValue(edd.DATAXML, '/fixml:PosRpt/fixml:Instrmt/@Exch', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') as MARKET,
        extractValue(edd.DATAXML, '/fixml:PosRpt/fixml:Instrmt/@Sym', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') as CONTRACT,
        case 
          when existsNode (edd.DATAXML, '/fixml:PosRpt/fixml:Instrmt[@PutCall="0"]', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') =1
            then 'Put'
          when existsNode (edd.DATAXML, '/fixml:PosRpt/fixml:Instrmt[@PutCall="1"]', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') =1
            then 'Call'
          else null
        end as PUTCALL,
        to_number(extractValue(edd.DATAXML, '/fixml:PosRpt/fixml:Instrmt/@StrkPx', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'),'99999.9999') as STRIKE,
        null as PRICE,
        case 
          when length(extractValue(DATAXML, '/fixml:PosRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"')) = 6
           then substr(extractValue(DATAXML, '/fixml:PosRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'), 1,4)
        || '-' || substr(extractValue(DATAXML, '/fixml:PosRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'), 5,2)
          when length(extractValue(DATAXML, '/fixml:PosRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"')) = 8
           then substr(extractValue(DATAXML, '/fixml:PosRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'), 1,4)
        || '-' || substr(extractValue(DATAXML, '/fixml:PosRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'), 5,2)
        || '-' || substr(extractValue(DATAXML, '/fixml:PosRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'), 7,2)
          else extractValue(edd.DATAXML, '/fixml:PosRpt/fixml:Instrmt/@MMY', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"')
        end as MATURITY,
        case 
          when iot.DATA3 like '%ACCT;%' then null 
          else extractValue(edd.DATAXML, '/fixml:PosRpt/@Acct', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"') 
        end as ACCOUNT,      
        case 
          when iot.DATA3 like '%ACCTTYP;%' then null
          when ar_clearer.IDA is not null then 'Clearer'
          when ar_client.IDA is not null and b.ISPOSKEEPING = 1 then 'Client'
          when b.ISPOSKEEPING = 1 then 'House'					
          when b.IDB is not null then 'Give-up'
          else 'n/a'  
        end as ACCOUNTTYPE,
        case 
          when iot.DATA3 like '%POSEFCT;%' then null
          else 'Open' 
        end as POSITIONEFFECT,
        iotc.INTDATAID, iotc.INTDATAIDS,
        iotc.EXTDATAID, edd.DATAROWNUMBER,
        iotc.INTVALUE1 as INTLONGQTY,
        iotc.INTVALUE2 as INTSHORTQTY,
        iotc.EXTVALUE1 as EXTLONGQTY,
        iotc.EXTVALUE2 as EXTSHORTQTY,
        iotc.MATCHVALUE1 as MATCHLONGQTY,
        iotc.MATCHVALUE2 as MATCHSHORTQTY,
        iotc.DTINS, iotc.IDAINS, iotc.ROWVERSION,
        null as IDSTACTIVATION
        from dbo.IOTRACKCOMPARE iotc
        left outer join dbo.EXTLDATADET edd on (edd.IDEXTLDATADET = iotc.EXTDATAID)
        inner join dbo.IOTRACK iot on (iot.IDIOTRACK = iotc.IDIOTRACK) and (iot.IDIOTRACK = %%FOREIGN_KEY%%)
        inner join dbo.PROCESS_L pl on (pl.IDPROCESS_L = iot.IDPROCESS_L)
        left outer join dbo.BOOK b on (b.IDENTIFIER = extractValue(edd.DATAXML, '/fixml:TrdCaptRpt/fixml:RptSide/@Acct', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"'))
        left outer join dbo.ACTOR a on (a.IDA = b.IDA)
        left outer join dbo.ACTORROLE ar_client on (ar_client.IDA = a.IDA) and (ar_client.IDROLEACTOR = 'CLIENT')
        left outer join 
        (
          select distinct IDA 
          from dbo.ACTORROLE
          where IDROLEACTOR in ('CLEARER','CCLEARINGCOMPART','HCLEARINGCOMPART','MCLEARINGCOMPART')
        ) ar_clearer on (ar_clearer.IDA = a.IDA)
        where (iotc.EXTVALUE1 is not null or iotc.EXTVALUE2 is not null) and (iotc.INTVALUE1 is null and iotc.INTVALUE2 is null)
        and existsNode(edd.DATAXML, '/fixml:PosRpt', 'xmlns:fixml="http://www.fixprotocol.org/FIXML-5-0-SP1"')=1
      ]]>
      </Command>

      <Command rdbms = "sqlserver">
        <![CDATA[
        select
        vw_ioc_t.DTSTPROCESS_L, 
        vw_ioc_t.PROCESS_L, 
        vw_ioc_t.IDSTPROCESS_L, 
        vw_ioc_t.IDDATAPROCESS_L, 
        vw_ioc_t.IDA, 
        vw_ioc_t.HOSTNAME, 
        vw_ioc_t.APPNAME, 
        vw_ioc_t.APPVERSION,
        vw_ioc_t.IDIOTASKDET,
        vw_ioc_t.DATASOURCEIDENT,
        vw_ioc_t.IDIOTRACK, 
        vw_ioc_t.MATCHSTATUS,
        vw_ioc_t.MESSAGE,
        vw_ioc_t.DTTRANSACTION,
        vw_ioc_t.MARKET,
        vw_ioc_t.CONTRACT,
        vw_ioc_t.PUTCALL,
        vw_ioc_t.STRIKE,
        vw_ioc_t.PRICE,
        vw_ioc_t.MATURITY,
        vw_ioc_t.ACCOUNT, 
        case 
          when ar_clearer.IDA is not null then 'Clearer'
          when ar_client.IDA is not null and b.ISPOSKEEPING = 1 then 'Client'
          when b.ISPOSKEEPING = 1 then 'House'					
          when b.IDB is not null then 'Give-up'
          else 'n/a'  
        end as ACCOUNTTYPE,
        vw_ioc_t.POSITIONEFFECT,
        vw_ioc_t.INTDATAID, 
        vw_ioc_t.INTDATAIDS,
        vw_ioc_t.EXTDATAID,
        vw_ioc_t.DATAROWNUMBER,
        vw_ioc_t.INTLONGQTY,
        vw_ioc_t.INTSHORTQTY,
        vw_ioc_t.EXTLONGQTY,
        vw_ioc_t.EXTSHORTQTY,
        vw_ioc_t.MATCHLONGQTY,
        vw_ioc_t.MATCHSHORTQTY,
        vw_ioc_t.DTINS, 
        vw_ioc_t.IDAINS, 
        vw_ioc_t.ROWVERSION,
        vw_ioc_t.IDSTACTIVATION
        from VW_IOCOMPARE_TRADE vw_ioc_t
        left outer join dbo.BOOK b on (b.IDENTIFIER = vw_ioc_t.ACCOUNT)
        left outer join dbo.ACTOR a on (a.IDA = b.IDA)
        left outer join dbo.ACTORROLE ar_client on (ar_client.IDA = a.IDA) and (ar_client.IDROLEACTOR = 'CLIENT')
        left outer join 
        (
          select distinct IDA 
          from dbo.ACTORROLE
          where IDROLEACTOR in ('CLEARER','CCLEARINGCOMPART','HCLEARINGCOMPART','MCLEARINGCOMPART')
        ) ar_clearer on (ar_clearer.IDA = a.IDA)
        where vw_ioc_t.IDIOTRACK = %%FOREIGN_KEY%%
      ]]>
      </Command>

    </SQLSelect>

    <SQLJoin>left outer join dbo.MARKET mk on (mk.IDENTIFIER=tblmain.MARKET)</SQLJoin>
    <!--RD 20140527 [20010] A lack of access to business date, the process date DTSTPROCESS_L is used.-->
    <SQLJoin>
      left outer join dbo.DERIVATIVECONTRACT dc on (dc.IDM=mk.IDM) and (dc.IDENTIFIER=tblmain.CONTRACT)
      and (dc.DTENABLED &lt;=tblmain.DTSTPROCESS_L) and (dc.DTDISABLED is null or dc.DTDISABLED > tblmain.DTSTPROCESS_L)
    </SQLJoin>

    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>%%SR:IOTASK_JOIN%%(IOTASKDET.IDIOTASK)</SQLJoin>
    </SQLWhere>

    <SQLWhere>
      <SQLWhere>
        <![CDATA[
        (tblmain.MATCHSTATUS like 
                              case when @MATCHSTATUS = 'ALL' then 
                                    tblmain.MATCHSTATUS 
                                   when @MATCHSTATUS = 'ALL_UNMATCH' then 
                                    'UNMATCH%' 
                                   else  @MATCHSTATUS end)
        ]]>
      </SQLWhere>
    </SQLWhere>

    <SQLRowStyle type="style">
      <![CDATA[
      	case tblmain.MATCHSTATUS
              when 'MATCH' then 'background-color:palegreen'
              when 'UNMATCH_SHORTQTY' then 'background-color:yellow'
		          when 'UNMATCH_LONGQTY' then 'background-color:yellow'
		          when 'UNMATCH_QTY' then 'background-color:yellow'
		          else 'background-color:lightpink'
		    end
      ]]>
    </SQLRowStyle>


