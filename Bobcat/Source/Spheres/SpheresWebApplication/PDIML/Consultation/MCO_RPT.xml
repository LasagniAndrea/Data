<Referentials>
  <Referential>

    <TableName>VW_MCO_MULTITRADES</TableName>
    <Ressource>MCO</Ressource>
    <AliasTableName>tblmain</AliasTableName>
    <ColumnByRow>3</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>false</Remove>

    <SQLSelect>
      <Command rdbms="all">
        <![CDATA[
        select mco.IDMCO, 
        case when cnfMsg.DTUPD is not null then cnfMsg.DTUPD else cnfMsg.DTINS end as DTMSG,
        mco.IDB_SENDTOPARTY as IDB_SENDTOPARTY, b.IDENTIFIER as IDENTIFIER_BOOK
        from dbo.VW_MCO_MULTITRADES mco
        inner join dbo.CNFMESSAGE cnfMsg on cnfMsg.IDCNFMESSAGE = mco.IDCNFMESSAGE
        left outer join dbo.BOOK b on b.IDB = mco.IDB_SENDTOPARTY
        
        ]]>
      </Command>
    </SQLSelect>

    <SQLJoin>inner join dbo.VW_MCO_MULTITRADES mco on mco.IDMCO = tblmain.IDMCO</SQLJoin>
    <SQLJoin>inner join dbo.NCS ncs on ncs.IDA = mco.IDA_NCS</SQLJoin>

