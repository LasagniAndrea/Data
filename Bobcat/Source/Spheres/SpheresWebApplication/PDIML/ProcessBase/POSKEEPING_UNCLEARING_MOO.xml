    <RequestTrack>
      <RequestTrackData>
        <ColumnIdA alias="RTK_IDA_DEALER">
          uncl.IDA_DEALER
        </ColumnIdA>
        <ColumnIdB alias="RTK_IDB_DEALER">
          uncl.IDB_DEALER
        </ColumnIdB>
        <ColumnGrp alias="RTK_GRP_DEALER">
          uncl.ROLEDEALER
        </ColumnGrp>
      </RequestTrackData>
    </RequestTrack>

    <SQLSelect>
      <Command rdbms = "all">
          <![CDATA[
          select 
          result.IDA_ENTITY, a_entity.IDENTIFIER as ENTITY_IDENTIFIER, a_entity.DISPLAYNAME as ENTITY_DISPLAYNAME, 
          result.IDA_CSS, a_css.IDENTIFIER as CSSCUSTODIAN_IDENTIFIER, 
          m.SHORT_ACRONYM as MARKET, result.DTENTITY, result.IDEM, 
          result.IDI, result.IDM, result.IDASSET, 
          result.IDA_DEALER, result.IDB_DEALER, result.IDA_CLEARER, result.IDB_CLEARER, result.IDA_ENTITYCLEARER, 
          /*--DEALER--*/
          dealer.IDENTIFIER as DEALER_IDENTIFIER,dealer.DISPLAYNAME as DEALER_DISPLAYNAME,
          bdealer.IDENTIFIER as DEALER_BOOKIDENTIFIER,
          case when arCLIENT.IDA is not null then 'Client' else 'House' end as ROLEDEALER,
          /*--CLEARER--*/
          clearer.IDENTIFIER as CLEARER_IDENTIFIER, clearer.DISPLAYNAME as CLEARER_DISPLAYNAME,
          bclearer.IDENTIFIER as CLEARER_BOOKIDENTIFIER,
          /*--ASSET DC--*/
          asset.IDDC as ASSET_IDDC,
          /*--ASSET IDENTIFIER--*/
          asset.IDENTIFIER as ASSET_IDENTIFIER,
          /*--ASSETSYMBOL--*/
          asset.ASSETSYMBOL as ASSETSYMBOL,
          /*--DERIVATIVECONTRACT--*/
          asset.CONTRACTIDENTIFIER, asset.CONTRACTDISPLAYNAME,
          /*--PUTCALL--*/
          case asset.PUTCALL when '0' then 'Put' when '1' then 'Call' else '' end as PUTCALL,
          /*--STRIKE--*/
          asset.STRIKEPRICE as STRIKE,
          /*--MATURITY--*/
          asset.MATFMT_MMMYY,
          result.IDPR, result.IDPADET, result.REQUESTTYPE_VALUE, result.REQUESTTYPE_EXTVALUE,
          result.DTBUSINESS, result.IDT_CLOSING, result.IDT, result.QTY, result.UNCLEARINGQTY, result.IDT_DELIVERY
          from
          (
          select tr.IDM, tr.DTENTITY, tr.IDI, tr.IDASSET, tr.IDEM, 
          tr.IDA_DEALER, tr.IDB_DEALER, tr.IDA_ENTITYDEALER,
          tr.IDA_CLEARER, tr.IDB_CLEARER, tr.IDA_ENTITYCLEARER,
          tr.IDT as IDT_CLOSING, tl.IDT_A as IDT_DELIVERY,   
          
          pr.IDA_ENTITY, pr.IDA_CSS, pr.IDPR, pad.IDPADET, pr.REQUESTTYPE as REQUESTTYPE_VALUE, en.EXTVALUE as REQUESTTYPE_EXTVALUE, pa.DTBUSINESS,
          case when (pad.IDT_BUY is null) then pad.IDT_SELL else 
          case when (pad.IDT_SELL is null) then pad.IDT_BUY else        
          case when (pad.IDT_BUY=pad.IDT_CLOSING) then pad.IDT_SELL else
          case when (pad.IDT_SELL=pad.IDT_CLOSING) then pad.IDT_BUY end end end end as IDT,          
          
          pad.QTY, pad.QTY as UNCLEARINGQTY 
          
          from dbo.POSACTION pa
          inner join dbo.POSACTIONDET pad on (pad.IDPA = pa.IDPA) and (pad.DTCAN is null)
          inner join dbo.POSREQUEST pr on (pr.IDPR = pa.IDPR) and (pr.REQUESTTYPE in ('AUTOABN', 'AUTOASS', 'AUTOEXE'))
          inner join dbo.ENTITYMARKET em on (em.IDEM = pr.IDEM) and (em.DTENTITY = pa.DTBUSINESS) 
          and (em.IDM = case when -1=@MARKET then em.IDM else @MARKET end)
          and (em.IDA = case when -1=@ENTITY then em.IDA else @ENTITY end)
          inner join dbo.VW_TRADE_POSETD tr on (tr.IDT = pad.IDT_CLOSING)

	        inner join dbo.ENUM en on (en.CODE = 'PosRequestTypeEnum') and (en.VALUE = pr.REQUESTTYPE)
          
		      left outer join 
		      (
			      select tl.IDT_B, tl.IDT_A, tl.LINK
			      from dbo.TRADELINK tl
			      inner join dbo.TRADE tr_A on (tr_A.IDT = tl.IDT_A)
			      where (tl.LINK like '%AutomaticOption%') and (tr_A.IDSTACTIVATION = 'REGULAR')
          ) tl on (tl.IDT_B = pad.IDT_CLOSING) and (tl.LINK like '%AutomaticOption%')
          
          
          %%SR:TRADEALLOC_JOIN%%(tr.IDT, tr)
          where (%%SR:TRADEALLOC_WHERE_PREDICATE%%)
                
          ) result
          inner join dbo.VW_MARKET_IDENTIFIER m on m.IDM=result.IDM
          inner join dbo.ACTOR a_entity on a_entity.IDA=result.IDA_ENTITY
          inner join dbo.ACTOR a_css on a_css.IDA=result.IDA_CSS
          inner join dbo.ACTOR dealer on dealer.IDA = result.IDA_DEALER
          inner join dbo.BOOK  bdealer on bdealer.IDB = result.IDB_DEALER
          inner join dbo.ACTOR clearer on clearer.IDA = result.IDA_CLEARER
          inner join dbo.BOOK  bclearer on bclearer.IDB = result.IDB_CLEARER
          inner join dbo.VW_ASSET_ETD_EXPANDED asset on asset.IDASSET = result.IDASSET
          left outer join (select IDA from dbo.ACTOR 
          where exists (select 1 from dbo.ACTORROLE where IDA=ACTOR.IDA and IDROLEACTOR = 'CLIENT'))  arCLIENT on arCLIENT.IDA=dealer.IDA
      ]]>
      </Command>
    </SQLSelect>

