<Referentials>
  <Referential>
    <IsUseSQLParameters>true</IsUseSQLParameters>
    <TableName>VW_TRADE</TableName>
    <Ressource>TRADE</Ressource>
    <ColumnByRow>3</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>false</Remove>
    <Image>gif\Process.gif</Image>
    <Notepad>false</Notepad>

    
    <SQLSelect>
      <Command rdbms="all">
        <![CDATA[
        select distinct t.IDT,t.IDENTIFIER,t.IDI,DTTRADE,t.DTSYS,t.IDSTENVIRONMENT,t.IDSTBUSINESS,t.IDSTACTIVATION,t.IDSTPRIORITY,t.SOURCE,
        t.DESCRIPTION, t.EXTLLINK, p.GPRODUCT
        from dbo.TRADE t
        %%SR:TRADE_JOIN%%(t.IDT, t)
        inner join dbo.INSTRUMENT i on i.IDI=t.IDI
        inner join dbo.PRODUCT p on p.IDP=i.IDP and (p.GPRODUCT not in ('ADM','RISK','ASSET') or (p.IDENTIFIER in ('cashBalance','cashPayment') ))  
        <choose>
            <when test="{ENTITY} != -1">
              left outer join dbo.BOOK bbuyer on bbuyer.IDB = t.IDB_BUYER 
              left outer join dbo.BOOK bseller on bseller.IDB = t.IDB_SELLER
            </when>
         </choose>
        inner join dbo.EVENT e on e.IDT=t.IDT
        inner join dbo.EVENTCLASS ec on ec.IDE=e.IDE And  ec.EVENTCLASS='STL'
        where ec.DTEVENTFORCED = @DATE1
        <choose>
            <when test="{ENTITY} != -1">
            and  ((bbuyer.IDA_ENTITY = @ENTITY) or (bseller.IDA_ENTITY  = @ENTITY))
            </when>
        </choose>
        and (%%SR:TRADE_WHERE_PREDICATE%%)
        
        <choose>
            <when test="{ISTOEND} = 1">
            union
            select distinct t.IDT,t.IDENTIFIER,t.IDI,t.DTTRADE,t.DTSYS,t.IDSTENVIRONMENT,t.IDSTBUSINESS,t.IDSTACTIVATION,t.IDSTPRIORITY,t.SOURCE,
            t.DESCRIPTION, t.EXTLLINK, p.GPRODUCT
            from dbo.TRADE t
            %%SR:TRADE_JOIN%%(t.IDT, t)
            inner join dbo.INSTRUMENT i on i.IDI=t.IDI
            inner join dbo.PRODUCT p on p.IDP=i.IDP and (p.GPRODUCT not in ('ADM','RISK','ASSET') or (p.IDENTIFIER in ('cashBalance','cashPayment') ))  
            <choose>
                <when test="{ENTITY} != -1">
                  left outer join dbo.BOOK bbuyer on bbuyer.IDB = t.IDB_BUYER 
                  left outer join dbo.BOOK bseller on bseller.IDB = t.IDB_SELLER
                </when>
             </choose>
            inner join dbo.EVENT e on e.IDT=t.IDT
            inner join dbo.EVENTCLASS ec on ec.IDE=e.IDE And  ec.EVENTCLASS='STL'
            where ec.DTEVENTFORCED >= @DATE1
            <choose>
                <when test="{ENTITY} != -1">
                and  ((bbuyer.IDA_ENTITY = @ENTITY) or (bseller.IDA_ENTITY  = @ENTITY))
                </when>
            </choose>
            and (%%SR:TRADE_WHERE_PREDICATE%%)
            </when>
        </choose>
        ]]>
      </Command>
    </SQLSelect>

