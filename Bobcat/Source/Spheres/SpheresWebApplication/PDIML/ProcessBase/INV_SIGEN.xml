    <Remove>false</Remove>
    <Notepad>false</Notepad>

    <SQLSelect>
      <Command rdbms="all">
        <![CDATA[
                select tblresult.IDT,tblresult.IDENTIFIER,tblresult.IDI,instr.IDENTIFIER as INSTRUMENT_IDENTIFIER,tblresult.DTTRADE,tblresult.DTSYS,
                tblresult.IDSTENVIRONMENT,tblresult.IDSTBUSINESS,tblresult.IDSTACTIVATION,tblresult.IDSTPRIORITY,tblresult.SOURCE,
                tblresult.DESCRIPTION,tblresult.EXTLLINK,tblresult.IDA_SELLER,tblresult.IDA_ENTITYBUYER, tblresult.IDA_ENTITYSELLER,tblresult.GPRODUCT
                from
                (
                select distinct t.IDT,t.IDENTIFIER,t.IDI,t.DTTRADE,t.DTSYS,t.IDSTENVIRONMENT,t.IDSTBUSINESS,t.IDSTACTIVATION,t.IDSTPRIORITY,t.SOURCE,
                t.DESCRIPTION,t.EXTLLINK, t.IDA_BUYER, t.IDA_SELLER,t.IDA_ENTITYBUYER, t.IDA_ENTITYSELLER, t.GPRODUCT
                from  dbo.VW_TRADEADMIN t
                %%SR:TRADEADMIN_JOIN%%(t.IDT, t)
                inner join dbo.EVENT e on e.IDT=t.IDT and e.EVENTCODE='INV' and e.EVENTTYPE='PER'
                inner join dbo.EVENTCLASS ec on ec.IDE=e.IDE and ec.EVENTCLASS='GRP'
                where ec.DTEVENT = @DATE1
                and (%%SR:TRADEADMIN_WHERE_PREDICATE%%)
                ) tblresult
                inner join dbo.INSTRUMENT instr on instr.IDI = tblresult.IDI
                where
                (
                  (
                    (tblresult.IDA_ENTITYBUYER = case when -1=@ENTITY Then
                    tblresult.IDA_ENTITYBUYER
                    else
                    @ENTITY
                    end
                    )
                    or
                    (tblresult.IDA_ENTITYSELLER = case when -1=@ENTITY Then
                    tblresult.IDA_ENTITYSELLER
                    else
                    @ENTITY
                    end
                    )
                  )
                  and (tblresult.IDSTENVIRONMENT in ('REGULAR','SIMUL'))
                )
                ]]>
      </Command>
    </SQLSelect>

