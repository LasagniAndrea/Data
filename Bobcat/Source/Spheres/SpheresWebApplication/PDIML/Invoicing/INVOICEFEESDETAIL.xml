<Referentials>
  <Referential>
    <IsUseSQLParameters>true</IsUseSQLParameters>
    <TableName>VW_INVOICEFEESDETAIL</TableName>
    <AliasTableName>fee</AliasTableName>
    <Ressource>INVOICEFEESDETAIL</Ressource>
    <ColumnByRow>3</ColumnByRow>
    <Create>false</Create>
    <Modify>false</Modify>
    <Remove>false</Remove>
    <Image>gif\Views.gif</Image>
    <Notepad>false</Notepad>
    <LoadOnStart>false</LoadOnStart>
    <Timeout>200</Timeout>

    <SQLWhere>
      <ConditionSystem>SESSIONRESTRICT</ConditionSystem>
      <SQLJoin>%%SR:TRADEADMIN_JOIN%%(fee.IDT_INVOICE)</SQLJoin>
      <SQLWhere>%%SR:TRADEADMIN_WHERE_PREDICATE%%</SQLWhere>
    </SQLWhere>

    <SQLWhere>
      <SQLWhere>
        <![CDATA[
        (fee.INVOICEDATE = @DATE1)
        and (fee.IDA_BENEFICIARY = case when 0 > @BENEFICIARY       then fee.IDA_BENEFICIARY else @BENEFICIARY end) and (fee.IDA_ENTITY = @ENTITY)
        --and (fee.IDA_INVOICED    = case when fee.IDA_BENEFICIARY = @BENEFICIARY then fee.IDA_INVOICED else @ENTITY end)
        --and (isnull(fee.FIXPARTYROLE,0) !=  case when fee.IDA_BENEFICIARY = @ENTITY then '-1' else '27' end)
        ]]>
      </SQLWhere>
    </SQLWhere>


