<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="EFS.SpheresIO.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
      <section name="SpheresIO.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
      
  <connectionStrings>
    <add name="EFS.SpheresIO.Properties.Settings.SPHERES_MARKETDATAConnectionString"
      connectionString="Data Source=SVR-DB01;Initial Catalog=SPHERES_MARKETDATA;Persist Security Info=True;User ID=sa"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
  
  <userSettings>
    <EFS.SpheresIO.Properties.Settings>
      <setting name="ConnectionString" serializeAs="String">
        <value>Data Source=test;Initial Catalog= test;Persist Security Info=True;User ID= test ;pwd= test</value>
      </setting>
      <setting name="EurosysSchemaPrefix" serializeAs="String">
        <value>dbo.</value>
      </setting>
      <setting name="AsyncLevel" serializeAs="String">
        <value>20</value>
      </setting>
    </EFS.SpheresIO.Properties.Settings>
  </userSettings>

  <appSettings>
    
    <add key="ImportTrade_isLogCheckAndRecord" value="false"/>
    <!-- IMPORT TRADE I/O 
    0 (all in transaction), 
    1 (TRADE in short-transaction and others with connectionString)
    2 (TRADE in short-transaction and others with connectionString and multi-threading)
    -->
    <add key="ImportTrade_recordMode" value="1"/>
    <!-- PRISMA - InputTheoreticalPriceFile -->
    <add key="InputTheoreticalPriceFile_isSearchProduct" value="true"/>
    <add key="InputTheoreticalPriceFile_isSearchSerie" value="true"/>
    <!-- PRISMA - InputMarketCapacitiesFile -->
    <add key="InputMarketCapacitiesFile_isSearchProduct" value="true"/>

    <!-- Enable/Disable Use of Logger Service -->
    <add key="RemoteLogEnabled" value="true"/>
  </appSettings>

  <system.diagnostics>
    <sources>
      <source name="SpheresTrace" switchName="SpheresTraceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="SpheresTraceListener"/>
          <remove name="Default"/>
        </listeners>
      </source>
      <source name="SpheresTraceTime" switchName="SpheresTraceTimeSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="SpheresTraceTimeListener"/>
          <remove name="Default"/>
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="SpheresTraceSwitch" value="Information" />
      <add name="SpheresTraceTimeSwitch" value="Off" />
    </switches>
    <sharedListeners>
      <add name="SpheresTraceListener" type="System.Diagnostics.DelimitedListTraceListener" initializeData="~\Trc_.txt" traceOutputOptions="DateTime" />
      <add name="SpheresTraceTimeListener" type="System.Diagnostics.DelimitedListTraceListener" initializeData="~\TrcTime_.txt" traceOutputOptions="DateTime" />
    </sharedListeners>
    <trace autoflush="true" />
  </system.diagnostics>
  
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_ISpheresLoggerService" openTimeout="00:10:00" closeTimeout="00:10:00" sendTimeout="00:10:00" receiveTimeout="00:10:00" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8736/SpheresLogger/SpheresLoggerService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISpheresLoggerService"
                contract="LoggerService.ISpheresLoggerService" name="BasicHttpBinding_ISpheresLoggerService" />
    </client>
  </system.serviceModel>

  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>