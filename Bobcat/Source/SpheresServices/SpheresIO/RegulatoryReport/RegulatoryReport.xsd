                                <xs:element name="accountingLocalClass" type="xs:string" />
                                <xs:element name="residenceCode" type="xs:unsignedShort" />
                                <!-- GS 20110707 verify if this field is in use-->
                                <xs:element name="economicAreaCode" type="xs:unsignedShort" />
