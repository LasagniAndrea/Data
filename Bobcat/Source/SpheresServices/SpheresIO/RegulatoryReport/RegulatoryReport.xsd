<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="regulatoryReport">
    <xs:annotation>
      <xs:documentation xml:lang="en">A element containing the datas designed to regulatory reports.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="trade">
          <xs:annotation>
            <xs:documentation xml:lang="en">
              A element containing the business datas of the trade.
            </xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="header">
                <xs:annotation>
                  <xs:documentation xml:lang="en">
                    A element containing the global business datas common to all products.
                  </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="beginDate" type="xs:date" />
                    <xs:element name="endDate" type="xs:date" />
                    <xs:element name="sysDate" type="xs:date" />
                    <xs:element name="packageVersion" type="xs:string" />
                    <xs:element name="eFSMLVersion" type="xs:string" />
                    <xs:element name="idtTemplate" type="xs:unsignedShort" />
                    <xs:element maxOccurs="unbounded" name="tradeId">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:unsignedInt">
                            <xs:attribute name="tradeIdScheme" type="xs:string" use="required" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="idt" type="xs:unsignedShort" />
                    <xs:element name="identifier" type="xs:string" />
                    <xs:element name="frontId" type="xs:string" />
                    <xs:element name="productFamily" type="xs:string" />
                    <xs:element name="productGroupIdentifier" type="xs:string"/>
                    <xs:element name="productIdentifier" type="xs:string" />
                    <xs:element name="productType" type="xs:string" />
                    <xs:element name="instrumentGroupIdentifier" type="xs:string"/>
                    <xs:element name="instrumentIdentifier" type="xs:string"/>
                    <!-- GS 20160201 (ticket 21183 CT 41: add "Gerarchia Fair value"-->
                    <xs:element name="fairValueHierarchy" type="xs:string"/>
                    <xs:element name="tradeDate" type="xs:date" />
                    <xs:element name="terminationDate" type="xs:date">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">
                          The last day of the term of the trade. It is an adjusted date.
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="isOpen" type="xs:boolean" >
                      <xs:annotation>
                        <xs:documentation xml:lang="en">
                          Returns true value if the trade is open.
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="isOpenAndClosed" type="xs:boolean">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">
                          Returns true value if the trade is opened and closed between during the observation period.
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="isDeliverable" type="xs:boolean" />
                    <xs:element name="isAccountingCurrency" type="xs:boolean" />
                    <xs:element name="isInTheScope" type="xs:boolean" />
                    <xs:element name="isStructuredProduct" type="xs:boolean" />
                    <xs:element name="isStructuredMaster" type="xs:boolean" />
                    <xs:element name="structuredIdentifier" type="xs:string" />
                  </xs:sequence>
                  <xs:attribute name="OTCmlId" type="xs:unsignedByte" use="required" />
                </xs:complexType>
              </xs:element>
              <xs:element name="dataDocument">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="repository">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="mainActor">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="ida" type="xs:unsignedShort" />
