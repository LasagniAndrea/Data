                                <xs:element name="accountingCurrency" type="xs:string" />
                                <xs:element name="tradeLocalClass" type="xs:string" />
                                <xs:element name="iasDerivativeClassification" type="xs:string" />
                                <xs:element name="iasHedgedDerivativeClassification" type="xs:string" />
