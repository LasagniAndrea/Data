                                                    <xs:attribute name="idc2" type="xs:string" use="required" />
                                                    <xs:attribute name="quoteBasis" type="xs:string" use="required" />
                                                    <xs:attribute name="fixingDate" type="xs:date" use="required" />
                                                    <xs:attribute name="fxAsset" type="xs:string" use="required" />
                                                    <xs:attribute name="fxQuotation" type="xs:decimal" use="required" />
                                                  </xs:extension>
                                                </xs:simpleContent>
                                              </xs:complexType>
                                            </xs:element>
                                          </xs:sequence>
