                                <xs:element name="identifier" type="xs:string" />
                                <xs:element name="extllink" type="xs:string" />
                                <xs:element name="numeroDirezioneGenerale" type="xs:string" />
                                <xs:element name="numeroRapporto" type="xs:string" />
                                <xs:element name="codiceBanca" type="xs:string" />
                                <xs:element name="codiceAziendale" type="xs:string" />
                                <xs:element name="sportello" type="xs:string" />
                                <xs:element name="numeroContoRapporto" type="xs:string" />
                                <xs:element name="servizioProvenienza" type="xs:string" />
                                <xs:element name="servizioRapporto" type="xs:string" />
                                <xs:element name="bookIdb" type="xs:unsignedShort" />
                                <xs:element name="bookIdentifier" type="xs:string" />
                                <xs:element name="buyerSeller" type="xs:string" />
                                <xs:element name="residenceCode" type="xs:string" />
                                <xs:element name="idCountryResidence" type="xs:string" />
                                <xs:element name="uicCountryCode" type="xs:string" />
                                <xs:element name="mifidClientCategorisation" type="xs:string" />
                                <!-- GS 20110707 verify if this field is in use-->
                                <xs:element name="economicAreaCode" type="xs:unsignedShort" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="masterAgreement">
                            <xs:complexType>
